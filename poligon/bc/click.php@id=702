<script type="text/javascript">
    $(document).ready(function(){
        //Ставим куку на 30 мин чтоб не доставала
        var date = new Date();
        var minutes = 30;
        date.setTime(date.getTime() + (minutes * 60 * 1000));
        $.cookie('popup_fishing_alert', '1', {expires: date, path: '/'});
        
        $("div[modal_content='popup_fishing_alert_modal']").click();
        $("input[name='popup_fishing_ok_submit']").click(function(){$(".popup_fishing_alert_modal_window .close_modal_win_btn").click();});
    })        
</script>

