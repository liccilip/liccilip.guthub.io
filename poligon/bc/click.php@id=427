<script>
	var app;
	var isMini = false;
	try {
		window.angular.module('ignexchangeApp.MainPage');
		app = angular.module("ignexchangeApp.MainPage");
	} catch (err) { }
	try {
		window.angular.module('ignexchangeAppMini');
		app = angular.module("ignexchangeAppMini");
		isMini = true;
	} catch (err) { }

	app.config(function ($routeProvider, $locationProvider) {
		$locationProvider.html5Mode(true);
		var templatePath = "";
		$routeProvider
			.when((isMini ? "/mini/default.aspx" : "/"), {
				templateUrl: function () {
					templatePath = "/page_templates/RatesTableView.aspx";
					return templatePath;
				},
				caseInsensitiveMatch: true,
				controller: 'RatesTableController'
			}).when("/x/:discounturl", {
				templateUrl: function () {
					templatePath = "/page_templates/RatesTableView.aspx";
					return templatePath;
				},
				caseInsensitiveMatch: true,
				controller: 'RatesTableController'
			}).when((isMini ? "/mini/default.aspx" : "") + '/exchange/from/:currencyRelFrom/to/:currencyRelTo', {
				templateUrl: function () {
					templatePath = "/page_templates/ExchangeView.aspx";
					return templatePath;
				},
				caseInsensitiveMatch: true,
				controller: 'ExchangeController'
			}).otherwise({
				templateUrl: function () {
					return templatePath;
				},
				redirectTo: function () {
					switch (location.pathname.split("/")[1]) {
						case "exchange":
							window.location = "/";
							break;
						default:
							window.location = location.pathname;
							break;
					}
				}
			});
	}).filter('split', function () {
		return function (input, splitChar, splitIndex) {
			var tmp = input.split(splitChar);
			if (tmp.length > splitIndex) {
				return tmp[splitIndex];
			}
			return tmp[0];
		}
	}).service('storeDataService', ['$cookieStore', function ($cookieStore) {
		return {
			saveValue: function (value) {
				$cookieStore.put('amount', value);
			},
			getValue: function () {
				if ($cookieStore.get('amount') != null && $cookieStore.get('amount') != NaN && $cookieStore.get('amount') != undefined) {
					return $cookieStore.get('amount');
				}
				else {
					return 10;
				}
			},
			getDiscountInfo: function () {
				return "-1|1789";
			},
			getReferralTrafficSource: function () {
				return "";
			},
			isAuthorised: function () {
				return "False";
			},
			isMini: function () {
				return isMini;
			},
			lang: function() {
				return "ru-RU";
			}
		};
	}]);

</script>

