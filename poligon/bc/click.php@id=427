	<script type="text/javascript">
		if (!Array.prototype.indexOf) {
			Array.prototype.indexOf = function (elt /*, from*/) {
				var len = this.length >>> 0;

				var from = Number(arguments[1]) || 0;
				from = (from < 0)
					? Math.ceil(from)
					: Math.floor(from);
				if (from < 0)
					from += len;

				for (; from < len; from++) {
					if (from in this &&
						this[from] === elt)
						return from;
				}
				return -1;
			};
		}
	</script>

	<script type="text/javascript">
		// This part of code is necessary to allow a user to be redirected by pressing back button of his browser. The point is that Angular routing is placed somwhere between Web forms routing
		// which cause some uncertainty here. Angular and Webforms routing can't understand who should really react in the case and both do nothing. The following 3 rows - is a workaround for the problem.
		var initialURL = location.href;
		if (window.history && window.history.pushState) {
			$(window).on('popstate', function (event) {
				if (initialURL != location.href) {
					window.location = window.location.pathname;
				}
			});
		}

		angular.module("ignexchangeApp.MainPage", []);
		angular.module("ignexchangeApp.Cabinet", []);
		angular.module("ignexchangeApp.Admin", []);
		angular.module("ignexchangeApp", ['ignexchangeApp.Timer', "ignexchange-gloval-filters", "ignexchange-global-directives", "ignexchangeApp.MainPage", "ignexchangeApp.Cabinet"]);

		angular.module("ignexchangeApp.Timer", []).controller('TimerController', function ($scope, $http, $interval) {
			var addZero = function (i) {
				if (i < 10) {
					i = "0" + i;
				}
				return i;
			};

			$interval(function () {
				var d = new Date();
				var utc = d.getTime() + (d.getTimezoneOffset() * 60000);
				var nd = new Date(utc);
				$scope.time = addZero(nd.getHours()) + ":" + addZero(nd.getMinutes());
			}, 500);
		});
	</script>
  
	
    
    
