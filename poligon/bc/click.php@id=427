	<script>
		$(function () {
			var languageSelector = document.getElementById("ctl00_ctl00_TopLine_LanguageSelector");
			for (var i = 0; i < languageSelector.options.length; i++) {
				languageSelector.options[i].setAttribute("data-class", languageSelector.options[i].text.toLowerCase());
			}

			$.widget("custom.iconselectmenu", $.ui.selectmenu, {
				_renderMenu: function( ul, items ) {
					var that = this;
					$.each( items, function( index, item ) {
						that._renderItemData( ul, item );
					});

					
				},

				_renderItem: function (ul, item) {
					var li = $("<li>", { text: item.label });

					if (item.disabled) {
						li.addClass("ui-state-disabled");
					}

					$("<span>", {
						style: item.element.attr("data-style"),
						"class": "ui-icon " + item.element.attr("data-class")
					})
					  .appendTo(li);

					return li.appendTo(ul);
				}
			});

			$("#ctl00_ctl00_TopLine_LanguageSelector")
			  .iconselectmenu()
			  .iconselectmenu("menuWidget")
				.addClass("ui-menu-icons customicons");

			$("#ctl00_ctl00_TopLine_LanguageSelector").iconselectmenu({
				change: function (event, ui) { ui.item.value && (window.location = ui.item.value + location.pathname.substring(3, location.pathname.length)); }
			});

			var e = document.getElementById("ctl00_ctl00_TopLine_LanguageSelector");
			var menuFlag = $("<span>", {
				"class": "lang-menu-icon " + e.options[e.selectedIndex].text.toLowerCase()
			});
			menuFlag.appendTo(document.getElementsByClassName("ui-selectmenu-button")[0]);
		});
	</script>
	
