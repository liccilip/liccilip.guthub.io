$('#subscribe-form').on('beforeSubmit', function (a, b, c, d) {
        var form = $(this);
        $.ajax({
            url    : form.attr('action'),
            type   : 'post',
            data   : form.serialize(),
            success: function (response) {
                $('#subscribe-form').hide();
                $('#confirm-message-subscribe').show();
                dataLayerEvent('success-subscription');
            },
            error  : function () 
            {
                console.log('internal server error');
            }
        });
        return false;
});
