	function change_sel(ob){
	   id=ob.value;
	   var opts = sel[id];
	   var IE='\v'=='v';
	   /*if(IE || $.browser.msie){
		opts = '<option>truncatethis</option>' + opts;
	   }*/
	   //document.exchange.cur_to.innerHTML = opts;
	   $('#cur_to').html(opts);
	   /*if(IE){
		document.exchange.cur_to.outerHTML = document.exchange.cur_to.outerHTML;
	   }*/
	   $('#cur_to').resetSS();
	   document.exchange.val_from.value = dval[id];
		document.exchange.val_to.value = calc(dval[id]);
	}
	function get_minimal_summ(){
	   var curid = document.exchange.cur_from.value;
		var curidto = document.exchange.cur_to.value;
		sum = document.exchange.val_from.value;		
		if(par[curid+'_'+curidto][10] > 0 && sum < par[curid+'_'+curidto][10]){
		 alert('Минимальная сумма к обмену '+par[curid+'_'+curidto][10]+' !');
		 return false;
		}
		return true;
	}
	function calc(sum_tmp,revers,revers_out){
	   var sum = parseFloat(sum_tmp);
	   if (typeof revers == 'undefined') {
			revers = false;
		}
		if (typeof revers_out == 'undefined') {
			revers_out = false;
		}
		
		var curid = document.exchange.cur_from.value;
		var curidto = document.exchange.cur_to.value;
		
		if(!revers && par[curid+'_'+curidto][12]==2){
			if(par[curid+'_'+curidto][11] > 0 && sum > par[curid+'_'+curidto][11]){sum = par[curid+'_'+curidto][11]; document.exchange.val_from.value = sum;}
		}	

	   $.ajax({
          type: "POST",
          url: "calc.php",
          data: "p=1&curid="+curid+"&curidto="+curidto+"&sum="+sum+"&cu=0",
          success: function(data){
              if (typeof data != 'undefined') {
         			document.exchange.val_to.value = data;
                  dval2 = data;
                  if(!revers && !revers_out){
            		   if(par[curid+'_'+curidto][13] > 0 && dval2 > par[curid+'_'+curidto][13]){
            			   dval2 = par[curid+'_'+curidto][13];
            			   document.exchange.val_to.value = dval2;
            			   calc_out(dval2,true);
            			}
            		}
            		if(!revers && !revers_out && (par[curid+'_'+curidto][12]==1 || par[curid+'_'+curidto][12]==3)){
            			if(par[curid+'_'+curidto][11] > 0 && dval2 > par[curid+'_'+curidto][11]){
            			   dval2 = par[curid+'_'+curidto][11];
            			   document.exchange.val_to.value = dval2;
            			   calc_out(dval2,true);
            			}
            		}
         	  }
          }
      });
	}

	function calc_out(sum_tmp,r_out){
	   if (typeof r_out == 'undefined'){
			r_out = false;
		}
	   var sum = parseFloat(sum_tmp);
	   var curid = document.exchange.cur_from.value;
	   var curidto = document.exchange.cur_to.value;
	   if(par[curid+'_'+curidto][13] > 0 && sum > par[curid+'_'+curidto][13]){sum = par[curid+'_'+curidto][13]; document.exchange.val_to.value = sum;}
	   if(par[curid+'_'+curidto][12]==1 || par[curid+'_'+curidto][12]==3){if(par[curid+'_'+curidto][11] > 0 && sum > par[curid+'_'+curidto][11]){sum = par[curid+'_'+curidto][11]; document.exchange.val_to.value = sum;}}
	
	   $.ajax({
          type: "POST",
          url: "calc.php",
          data: "p=2&curid="+curid+"&curidto="+curidto+"&sum="+sum+"&cu=0",
          success: function(data){
              if (typeof data != 'undefined') {         			
         			outzn = data;
         			if(!r_out && par[curid+'_'+curidto][12]==2){
            		    if(par[curid+'_'+curidto][11] > 0 && outzn > par[curid+'_'+curidto][11]){outzn = par[curid+'_'+curidto][11];calc(outzn);}
            		}
            		document.exchange.val_from.value = (outzn*1).toFixed(2);
         	  }
          }
      });
	}
</script>


