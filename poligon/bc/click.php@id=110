			function updatelink(types_print, types, id_tarif)
			{

           		var options = {
                url: '/wm_soft/modules/table_tarifs/ajax_fancy_link.php?RND='+Math.random(),
                type: "POST",
                dataType:"TEXT",
                data: "type="+types+"&id_tarif="+id_tarif,
                beforeSend: function()
                {
              		//$('#divCheckLoad').css('display','block');

            	},
                  error: function()
                {
              		$("#error_link").html('Сервис временно не доступен');
            	},

              	success:
                  function(msg) {


                  $(".print_link_rights_"+types_print).html(msg);

                          }
                };


                $.ajax(options);
                if( types_print == 'in' )
                {
                	$("#link_econom_in").html('<a href="http://wmt24.ru/news_company/index.php?ID=589" class="fancy_econom" target=_blank >Доплата за Ввод</a>');
                }

			}


			function commission_input(wm_otdaete, val_otdaete, arCurse)
			{
             	wm_otdaete = parseFloat(wm_otdaete);
                var wm_com_purs ='';
                if ( arCurse.ComissionType.length==0 )
                {
                	ComissionType = arCurse.comisType;
                }
                else
                {
                	var ComissionType = arCurse.ComissionType;
                }

                if ( ComissionType.length==0 )
                {
                	return wm_com_purs;
                }

				wm_com_purs = comission( ComissionType, val_otdaete, wm_otdaete );


			    wm_com_purs = wm_com_purs + wm_otdaete;

  				return(wm_com_purs);
			}

			function commission_input_reverse(wm_comission, val_otdaete, arCurse)
			{
             	wm_comission = parseFloat(wm_comission);
                if ( arCurse.ComissionType.length==0 )
                {
                	ComissionType = arCurse.comisType;
                }
                else
                {
                	var ComissionType = arCurse.ComissionType;
                }

                if ( ComissionType.length==0 )
                {
                	return wm_com_purs;
                }
				var wm_comis = comission_obr( ComissionType, val_otdaete, wm_comission );

  				return(wm_comis);
			}

     function CheckCountry()
     {
            var result = 'rur';
     		$.each(valutes, function(country, val)
     		{
     			if(val.name == 	$("#strana_value_in").val() )
     			{

     				result = country;
     				return false;
     			}
     		});
     		//возвращаем по умолчанию rur
     		return result;

     }

	//Проверки для направления ввод/вывод наличные рубли
	var CitiesCustom = {
	'Москва':{'full_name': 'Москва, г.Москва'},
'Казань':{'full_name': 'Казань, Республика Татарстан'},
'Самара':{'full_name': 'Самара, Самарская область'},

	}

    function CheckCash(type)
    {

    	$("#notice_error_form").css('display','none');
    	if((type == 'in' && $("#valute_otdaete").val() != 'nal') || (type == 'out' && $("#valute_poluchite").val() != 'nal'))
    	{

            	$("#poluchite").removeAttr("disabled","disabled");
            	$("#otdaete").removeAttr("disabled","disabled");
            	$("#otdaete_komission").removeAttr("disabled","disabled");
    		return false;
    	}

    	if( type == 'in'  )
    	{
    		var nameStr = $("#valute_otdaete option:selected").text().substr(14);
    		nameStr = CitiesCustom[nameStr];
    		if( nameStr )
    		{
    			if( $("#otdaete_city_in").val() != nameStr.full_name)
    			{
    				$("#otdaete_city_in").val(nameStr.full_name);
    				ajax_table_punkts();
    			}

            }


        }
        else
        {
     		var nameStr = $("#valute_poluchite option:selected").text().substr(14);
    		nameStr = CitiesCustom[nameStr];
    		if( nameStr )
    		{
    			if( $("#otdaete_city_in").val() != nameStr.full_name)
    			{
    				$("#otdaete_city_in").val(nameStr.full_name);
    				ajax_table_punkts();
    			}

            }
        }

        if ( type == "in" )
        {
        	var text_err="ввода";
        }
        else
        {
        	var text_err="вывода";
        }

            if( $("#otdaete_city_in").val() == "" )
            {
            	$("#notice_error_form").css('display','');

            	notice_country();
            	$("#poluchite").attr("disabled","disabled");
            	$("#otdaete").attr("disabled","disabled");
            	$('#otdaete_komission').attr("disabled","disabled");
            }
            else if(!$("#variants_select").val() || $("#variants_select").val() == "0" || $("#variants_select").val().length == 0)
            {

            	$("#notice_error_form").css('display','');
            	$("#notice_error_form").html( 'Подсказка: Выберите вариант '+text_err );
            	$("#poluchite").attr("disabled","disabled");
            	$("#otdaete").attr("disabled","disabled");
            	$('#otdaete_komission').attr("disabled","disabled");
            }
            else
            {
            	$("#notice_error_form").css('display','none');
            	$("#poluchite").removeAttr("disabled","disabled");
            	$("#otdaete").removeAttr("disabled","disabled");
            	$("#otdaete_komission").removeAttr("disabled","disabled");
            	$("#notice_error_form").html( '');
            	return true;
            }
            return false;
    }

	function BlockUnblockJs(t_otdaete, t_poluchite, detect_select)
	{
        if( detect_select == 3 || detect_select == 4 || detect_select == 5)
        	return true;

        if( detect_select == 1 && t_otdaete == 'el' )
        {
        	$("#valute_poluchite option[value='28']").removeAttr("disabled"); 
$("#valute_poluchite option[value='10']").removeAttr("disabled"); 
$("#valute_poluchite option[value='128']").removeAttr("disabled"); 
$("#valute_poluchite option[value='47']").removeAttr("disabled"); 
$("#valute_poluchite option[value='11']").removeAttr("disabled"); 
$("#valute_poluchite option[value='32']").removeAttr("disabled"); 
$("#valute_poluchite option[value='34']").removeAttr("disabled"); 
$("#valute_poluchite option[value='48']").removeAttr("disabled"); 
$("#valute_poluchite option[value='169']").removeAttr("disabled"); 
$("#valute_poluchite option[value='50']").removeAttr("disabled"); 
$("#valute_poluchite option[value='49']").removeAttr("disabled"); 
$("#valute_poluchite option[value='181']").removeAttr("disabled"); 
$("#valute_poluchite option[value='57']").removeAttr("disabled"); 
$("#valute_poluchite option[value='12']").removeAttr("disabled"); 
$("#valute_poluchite option[value='nal']").removeAttr("disabled"); 
$("#valute_poluchite option[value='44']").removeAttr("disabled"); 

        	return true;
        }

        if(  detect_select == 2 && (t_poluchite == 'el' || t_poluchite == 'service') )
        {
        	$("#valute_otdaete option[value='nal']").removeAttr("disabled"); 
        	if( t_poluchite == 'service' )
        	{
            	$("#valute_otdaete option[value='45']").attr("disabled","disabled");
            	$("#valute_otdaete option[value='2']").attr("disabled","disabled");
        	}
        	return true;
        }


    	if( detect_select == 1 && t_otdaete != 'el'  )
    	{
        	$("#valute_poluchite option[value='28']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='10']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='128']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='47']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='11']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='32']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='34']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='48']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='169']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='50']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='49']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='181']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='57']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='12']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='nal']").attr("disabled","disabled"); 
$("#valute_poluchite option[value='44']").attr("disabled","disabled"); 
        	if( $("#valute_otdaete").val() == 2 ||  $("#valute_otdaete").val() == 45 )
        	{
             	        	}
        	else
        	{
        		        	}
        	return true;
    	}


    	if( detect_select == 2 && t_poluchite != 'el' && t_poluchite != 'service')
    	{
    		$("#valute_otdaete option[value='nal']").attr("disabled","disabled"); 
    		return true;
    	}


	}

	function DetectTypeOperation(napr_calc, detect_select)
	{
		var result = 0;
		var type_otdaete = "";
		var type_poluchite = "";
        var text_no_obmen = "Обмен в выбранном направлении недоступен";
        $('#error_form').html('');
        $("#minimalSum").html('');
        $("#minimalSum").css('display','none');
        $(".change-block").css('display','none');
        $("#buy_all_change").hide();
        $("#buy_all_inout").hide();
        $("#notice_error_form").html( '' );
        $("#div_reserves").html('');
        $("#div_reserves_econom").html('');
        $("#div_reserves_owner").html('');
        $("#poluchite_alert").html('');
        $("#poluchite_alert_inout").html('');
        $("#reserv_value").val(0);
        $('.submit_order').attr('disabled', 'disabled');
        $('#div_reserves').attr('class', '');
        $('#div_reserves_econom').attr('class', '');
        $('#div_reserves_owner').attr('class', '');
        $("#curses_field").html('');
        $("#curses_field").css('display','none');
        $(".service_display_inout").css('display','');
        $('#econom_out').css('display','none');
        $('#nonres_out').css('display','none');
        $('#owner_in').css('display','none');
        $(".economNal").html('Экономичный обмен (от ИП)');
        $(".sberhtml").html("");
        //Блок вопросы
        $(".autoshow-info").css('display','none');
        $(".entermoney-info").css('display','none');
        $(".entermoney-out-info").css('display','none');
        $(".entermoney-service-info").css('display','none');

        document.getElementById('otdaete').value=document.getElementById('otdaete').value.replace(",",".");
        document.getElementById('poluchite').value=document.getElementById('poluchite').value.replace(",",".");

		var valute_otdaete = $("#valute_otdaete").val();
		var valute_poluchite = $("#valute_poluchite").val();
		var poluchite = parseFloat($('#poluchite').val().replace(',','.'));
		var otdaete = parseFloat($('#otdaete').val().replace(',','.'));
		var otdaete_komission = parseFloat($('#otdaete_komission').val().replace(',','.'));
        //$("#testjs").html(poluchite+'|'+otdaete+'|'+otdaete_komission);
		$("#type_i").val('');

            if( $("#valute_poluchite").val() == 12 )
            {
            	$("#link_econom_out").hide();
            	$("#link_econom_out2").hide();
            }
            else if( global_type == 'fiz')
            {
            	$("#link_econom_out").show();
            	$("#link_econom_out2").show();
            }

		if( valute_otdaete=="wmr"
		|| valute_otdaete=="wmz"
		|| valute_otdaete=="wme"
		|| valute_otdaete=="yad")
	    {
	    	var type_otdaete = "el";
	    }

		if(valute_poluchite=="wmr"
		|| valute_poluchite=="wmz"
		|| valute_poluchite=="wme"
		|| valute_poluchite=="yad")
	    {
        	var type_poluchite = "el";
	    }

        if( valute_poluchite=="nal" )
        {
        	var type_poluchite = "nal";


            $("#type_i").val('out');
            var valute_def = CheckCountry();

        }
        else if( valute_otdaete=="nal" )
        {
        	var type_otdaete = "nal";
            $("#type_i").val('in');
        	var valute_def = CheckCountry();

        }
        else
        {

        	$('#out_variants').css('display','none');
            var valute_def = 'rur';
        }


	    

		BlockUnblockJs(type_otdaete, type_poluchite, detect_select);

        if( valute_otdaete =='' || valute_poluchite=='' || valute_poluchite == valute_otdaete)
        {
        	$(".right-block").css('display','none');
        	$(".mainhead-right").css('display','none');
        	$(".input_sum").css('display','none');
        	$('.inout_visible_submit').css('display','none');
        	$(".popular-block").css('display','');
        	$('#otdaete_city_in').val('');
        	return false;
        }
        else
        {
        	$("#sw_type").hide();
        	$(".input_sum").css('display','');
        	$(".right-block").css('display','');
        	$(".mainhead-right").css('display','');
        	if( valute_otdaete=="nal" || valute_poluchite=="nal" )
        	{
        		$('.inout_visible').css('display','');
        		$('.inout_visible_37').hide();
        	}
        	else if( valute_otdaete == "37" )
        	{
        		$('.inout_visible_37').css('display','');
        		$('.inout_visible').css('display','none');
        	}
        	else
        	{
        		$('#otdaete_city_in').val('');
        		$('.inout_visible').css('display','none');
        		$('.inout_visible_37').hide();
        	}
        	$(".popular-block").css('display','none');
        	$('.inout_visible_submit').css('display','');

        }

		if( type_otdaete == "el" && type_poluchite == "el" )
		{
			$('.inout_visible').css('display','none');
			$('.inout_visible_37').css('display','none');
			$('.inout_visible_submit').css('display','none');
			$(".autoshow-info").css('display','');
			$(".btn_oneclick").css('display','none');
			$("#buy_all_change").show();

            $("#type_napr_select").html(': Автообмен');
			var name_tarif =valute_poluchite+'-'+valute_otdaete;
			var select_tarif = tarifs_change[name_tarif];
         		if(!select_tarif){

        			$('#error_form').html(text_no_obmen);

        			return false;
        		}
            $('#listid').val( select_tarif.listid );
            $('#wm_act_form').val( 'submitform' );
            updatelink('change', 'change', $('#listid').val());

            if( napr_calc == 'line' )
            {
            	var wm_out=RoundMoneyFloor(otdaete*select_tarif.curse);
            	if(isNaN(wm_out)) wm_out = "";
            	$('#poluchite').val( wm_out );
            }
            else if( napr_calc == 'comission' )
            {
                var comiss = commission_input_reverse(otdaete_komission,valute_otdaete, select_tarif);
                $('#otdaete').val(RoundMoney(otdaete_komission-comiss));
                var wm_out=RoundMoneyFloor($('#otdaete').val()*select_tarif.curse);
                if(isNaN(wm_out)) wm_out = "";
                $('#poluchite').val( wm_out );
            }
            else
            {
            	var wm_out=RoundMoneyCeil(poluchite/select_tarif.curse);
            	if(isNaN(wm_out)) wm_out = "";
            	$('#otdaete').val( wm_out );
            }


                if ( napr_calc != 'comission' )
                {
                	select_tarif.ComissionType=select_tarif.comisType;
                	var comission = RoundMoney(commission_input($('#otdaete').val(),valute_otdaete, select_tarif));
                	if(isNaN(comission)) comission = "";
                	$('#otdaete_komission').val( comission );
                }


            	$("#type_otdaete_purse").html(select_tarif.out_val);
            	$("#type_poluchite_purse").html(select_tarif.reserv_val);

            	$(".change-block").css('display','');

            	if( valute_otdaete == 'yad' || valute_poluchite == 'yad' )
            	{
            		$(".type_otdaete_purse").css('display','');
            	}
            	else
            	{
            		$(".type_otdaete_purse").css('display','none');
            	}

			    if($('#otdaete').val()!='' && $('#poluchite').val()!='')
			    {
			    	$('.submit_order').removeAttr('disabled');
			    }
            $("#div_reserves").show();
            $("#div_reserves").html(''+select_tarif.reserv+' '+select_tarif.reserv_val);
            $("#div_reserves").addClass(valute_poluchite);
            $("#reserv_value").val(select_tarif.reserv);
            	if(select_tarif.out_val=="WMR")
            	{
            		var leftcurse=Okrugl(1/select_tarif.curse*10000)/10000;
            		var rightcurse=1;
            	}
				else
				{
					var leftcurse=1;
					var rightcurse=Okrugl(select_tarif.curse*10000)/10000;
				}
			$("#curses_field").css('display','');
            $("#curses_field").html(leftcurse+' '+select_tarif.out_val+' = '+rightcurse+' '+select_tarif.reserv_val);

                    if( parseFloat($('#poluchite').val()) >  parseFloat(select_tarif.reserv))
                    {
                    	$("#poluchite_alert").html('Резерва недостаточно ');
                    	$('.submit_order').attr('disabled', 'disabled');
                    }

			result = 'wm_change';
		}
		else if( type_poluchite=="service" )
		{
        	var valute_def='rur';

            $(".service_display_inout").css('display','none');
            $(".entermoney-service-info").css('display','');
            $('#wm_act_form').val( 'form' );
         	if( type_otdaete == "el" )
         	{
                var type_i = 'out';
                var name_tarif = valute_otdaete+'-'+valute_poluchite;
        		var select_tarif = tarifs_service[name_tarif];
                var select_curses = curses_service[valute_otdaete];

          		if(!select_tarif || !select_curses){

        			$('#error_form').html(text_no_obmen);

        			return false;
        		}


                if( napr_calc == 'line' )
        		{
                    var comiss_minus = commission_input_reverse(otdaete,valute_otdaete, select_curses);

                	//var wm_out = parseInt ( OrderCalcLine(select_tarif, select_curses, type_i, otdaete-comiss_minus)*100 )/100;
                	var wm_out = parseInt ( OrderCalcLine(select_tarif, select_curses, type_i, otdaete)*100 )/100;

                	if(isNaN(wm_out)) wm_out = "";
                	$('#poluchite').val( wm_out );
                }
                else if ( napr_calc == 'comission' )
                {
                	//otdaete_komission
                	var comiss = commission_input_reverse(otdaete_komission,valute_otdaete, select_curses);
                	otdaete =RoundMoney(otdaete_komission-comiss);
                	if(isNaN(otdaete)) otdaete = "";
                	$('#otdaete').val(otdaete);
                	var comiss_minus = commission_input_reverse(otdaete,valute_otdaete, select_curses);

                	//wm_out = RoundMoney ( OrderCalcLine(select_tarif, select_curses, type_i, otdaete-comiss_minus) );
                	wm_out = RoundMoney ( OrderCalcLine(select_tarif, select_curses, type_i, otdaete) );

                	if(isNaN(wm_out)) wm_out = "";
                	$('#poluchite').val( wm_out );
                }
                else
                {

                 	wm_out = OrderCalcReverse(select_tarif, select_curses, type_i, poluchite) ;
                 	//wm_out = parseInt(commission_input(wm_out,valute_otdaete, select_curses)*100)/100;
                 	wm_out = parseInt(wm_out*100)/100;
                 	if(isNaN(wm_out)) wm_out = "";
                 	$('#otdaete').val( wm_out );
                }

                if ( napr_calc != 'comission' )
                {
                	var comission = RoundMoney( commission_input($('#otdaete').val(),valute_otdaete, select_curses) );
                	if(isNaN(comission)) comission = "";
                	$('#otdaete_komission').val( comission );
                }
                $("#minimalSum").css('display','');
                $("#minimalSum").html('от '+select_tarif.minimalSum+' '+e_valutes[valute_otdaete].name_o);
          	}
           	else
            {
            	var type_i = 'in';
            	//Проверим на операции за наличные
                if( CheckCash(type_i) == true )
                {

                	valute_otdaete = $('#variants_select').val();
                }
                else if( valute_otdaete == 'nal' || valute_poluchite == 'nal')
                {

                	return false;
                }

            	var name_tarif =valute_otdaete+'-'+valute_poluchite;
        		var select_tarif = tarifs_service[name_tarif];
            	var select_curses = curses['wmr-rur'];

         		if(!select_tarif || !select_curses){

        			$('#error_form').html(text_no_obmen);

        			return false;
        		}


            	if( napr_calc == 'line' )
            	{
                	wm_out = RoundMoney ( OrderCalcLine(select_tarif, select_curses, type_i, otdaete) );
                	if(isNaN(wm_out)) wm_out = "";
                	$('#poluchite').val( wm_out );
                }
                else
                {
                 	wm_out = OrderCalcReverse(select_tarif, select_curses, type_i, poluchite) ;
					wm_out = parseInt(wm_out*100)/100;
					if(isNaN(wm_out)) wm_out = "";
                 	$('#otdaete').val( wm_out );
                }

				$("#minimalSum").css('display','');
            	$("#minimalSum").html('от '+select_tarif.minimalSum+' '+valutes[select_tarif.minimalSum_valute].name_o);

            }

			    if($('#otdaete').val()!='' && $('#poluchite').val()!='')
			    {

			    	$('.submit_order').removeAttr('disabled');
			    }
        	$("#div_reserves").html(''+select_tarif.reserv+' '+valutes[select_tarif.reserv_val].name_o);
        	result = 'wm_service';

		}
		else
		{

            $(".btn_oneclick").css('display','none');
            $("#buy_all_inout").show();
			if( type_otdaete == "el" )
			{
                $(".entermoney-out-info").css('display','');
                var type_i = 'out';
                $("#type_i").val(type_i);
                $("#type_napr_select").html(': Вывод');
                //Проверим на операции за наличные
                if( CheckCash(type_i) == true )
                {   $(".economNal").html('Экономичный вывод');
                	$(".btn_oneclick").css('display','none');
                	valute_poluchite = $('#variants_select').val();
                }
                else if( valute_otdaete == 'nal' || valute_poluchite == 'nal')
                {
        			$(".input_sum").css('display','none');
        			$(".right-block").css('display','none');
        			$(".mainhead-right").css('display','none');
        			$(".btn_oneclick").css('display','');
                	return false;
                }

				var name_tarif =valute_otdaete+'-'+valute_def+'-'+valute_poluchite;
                var name_tarif_econom = 'econom-'+valute_otdaete+'-'+valute_def+'-'+valute_poluchite;
                var name_tarif_nonres = 'nonResident-'+valute_otdaete+'-'+valute_def+'-'+valute_poluchite;
                  // alert(name_tarif);
                if( tarifs[name_tarif_econom] )
                {
                	$('#econom_out').css('display','');
                }
                if( tarifs[name_tarif_nonres] )
                {
                	$('#nonres_out').css('display','');

                }


				if(  $('#CheckEconom1:checked').length && !tarifs[name_tarif_econom]  )
				{
					$("#CheckEconom0").attr('checked',true);
	            	$("#link_econom_out").hide();
	            	$("#link_econom_out2").hide();
					$("#CheckEconom1").removeAttr('checked');
				}
				if(  $('#CheckEconom0:checked').length && tarifs[name_tarif_econom]  )
				{
					$("#CheckEconom1").attr('checked',true);

	            	$("#link_econom_out").hide();
	            	$("#link_econom_out2").hide();
					$("#CheckEconom0").removeAttr('checked');
				}


				//$("#testjs").html(name_tarif);
				var select_tarif = tarifs[name_tarif];
				if( $('#CheckEconom1:checked').length && tarifs[name_tarif_econom] )
				{
					var select_tarif = tarifs[name_tarif_econom];
					$('.print_link_rights_out').hide();
				}
				else
				{
					$('.print_link_rights_out').show();
					$("#link_econom_out").hide();
	            	$("#link_econom_out2").hide();
				}

				if( $('#Checknonres1:checked').length && tarifs[name_tarif_nonres] )
				{
					var select_tarif = tarifs[name_tarif_nonres];
					$('#econom_out').css('display','none');
				}
				else if( $('#Checknonres0:checked').length && tarifs[name_tarif_econom]  )
				{
					$('#econom_out').css('display','');
				}

				var select_curses = curses[valute_otdaete+'-'+valute_def];

         		if(!select_tarif || !select_curses){

        			$('#error_form').html(text_no_obmen);

        			return false;
        		}

                updatelink('out', 'inout', select_tarif.id_tarif);
                $('.typicalChange').attr('href','http://wmt24.ru/?wm_mod=wm_notice&type=inout&return=rules&id_tarif='+select_tarif.id_tarif);
			    if( napr_calc == 'line' )
			    {
                	wm_out = RoundMoney ( OrderCalcLine(select_tarif, select_curses, type_i, otdaete) );
                	if(isNaN(wm_out)) wm_out = "";
                	$('#poluchite').val( wm_out );
                	DopFunctionCalc('line', $('#poluchite').val(), valute_poluchite);
                }
                else if ( napr_calc == 'comission' )
                {
                	//otdaete_komission
                	var comiss = commission_input_reverse(otdaete_komission,valute_otdaete, select_curses);
                	var otdaete=RoundMoney(otdaete_komission-comiss);
                	if(isNaN(otdaete)) otdaete = "";
                	$('#otdaete').val(otdaete);
                	wm_out = RoundMoney ( OrderCalcLine(select_tarif, select_curses, type_i, otdaete) );
                	if(isNaN(wm_out)) wm_out = "";
                	$('#poluchite').val( wm_out );
                	DopFunctionCalc('line', $('#poluchite').val(), valute_poluchite);
                }
                else
                {
                 	poluchite = DopFunctionCalc('reverse', poluchite, valute_poluchite);
                 	wm_out = RoundMoney ( OrderCalcReverse(select_tarif, select_curses, type_i, poluchite) );
                 	if(isNaN(wm_out)) wm_out = "";
                 	$('#otdaete').val( wm_out );
                }

                if ( napr_calc != 'comission' )
                {
                	var comission = RoundMoney( commission_input($('#otdaete').val(),valute_otdaete, select_curses) );
                	if(isNaN(comission)) comission = "";
                	$('#otdaete_komission').val( comission );
                }

                var flag_noorder = 0;

                if( select_tarif.reserv_visible == 1 )
                {
                	$("#div_reserves").show(); //изначально включаем
                	$("#div_reserves").html(''+select_tarif.reserv+' '+valutes[select_tarif.reserv_val].name_o);
                	//вывод эконом резерва
                    if( tarifs[name_tarif_econom] )
                    {
                    	var select_tarif_econom =  tarifs[name_tarif_econom];
                    	var select_tarif_main = tarifs[name_tarif];
                    	//$("#div_reserves").html('Обычн: '+select_tarif_main.reserv+' '+valutes[select_tarif_main.reserv_val].name_o);
                    	$("#div_reserves").hide(); //чтоб не отображался если есть эконом
                    	$("#div_reserves_econom").html('<br>'+select_tarif_econom.reserv+' '+valutes[select_tarif_econom.reserv_val].name_o);
                    }

                    $("#reserv_value").val(select_tarif.reserv);
                    if( parseFloat($('#poluchite').val()) >  parseFloat(select_tarif.reserv) )
                    {
                    	$("#poluchite_alert_inout").html('Резерва недостаточно');
                    	flag_noorder = 1;
                    }
                }
                $("#minimalSum").css('display','');
                $("#minimalSum").html('от '+select_tarif.minimalSum+' '+e_valutes[select_tarif.minimalSum_valute].name_o);


				result = 'ordern';

			    if($('#otdaete').val()!='' && $('#poluchite').val()!='' && flag_noorder == 0 )
			    {
			    	$('.submit_order').removeAttr('disabled');
			    }
			}
			else if( type_poluchite == "el" )
			{
				//фикс банкомат альфабанка за доллары
				if( valute_otdaete == 67 )
				{
					valute_def = 'usd';
				}

				$(".entermoney-info").css('display','');
				$("#type_napr_select").html(': Ввод');
				var type_i = 'in';
				$("#type_i").val(type_i);
                //Проверим на операции за наличные
                if( CheckCash(type_i) == true )
                {
                	$(".btn_oneclick").css('display','none');
                	valute_otdaete = $('#variants_select').val();
                }
                else if( valute_otdaete == 'nal' || valute_poluchite == 'nal')
                {
        			$(".input_sum").css('display','none');
        			$(".right-block").css('display','none');
        			$(".mainhead-right").css('display','none');
        			$(".btn_oneclick").css('display','');
                	return false;
                }

                //var arPunkts=$("#type_i").val('out');
			    var name_tarif = valute_def+'-'+valute_poluchite+'-'+valute_otdaete;
			    var name_tarif_owner = 'nonResident-'+valute_def+'-'+valute_poluchite+'-'+valute_otdaete;

                if( tarifs[name_tarif_owner] )
                {
                	$('#owner_in').css('display','');

                	if( valute_otdaete == '45')
                	{
                		$('.ownerTextCheckbox').html( 'Если кошелек не принадлежит ИП' );
                	}
                	else
                	{
                		$('.ownerTextCheckbox').html( 'Если кошелек не принадлежит Юр. лицу' );
                	}
                }

			    var select_tarif = tarifs[name_tarif];
				if( $('#CheckOwner1:checked').length && tarifs[name_tarif_owner] )
				{
					var select_tarif = tarifs[name_tarif_owner];
				}

			    var select_curses = curses[valute_def+'-'+valute_poluchite];

         		if(!select_tarif || !select_curses){

        			$('#error_form').html(text_no_obmen);

        			return false;
        		}

                updatelink('in', 'inout', select_tarif.id_tarif);
                $('.typicalChange').attr('href','http://wmt24.ru/?wm_mod=wm_notice&type=inout&return=rules&id_tarif='+select_tarif.id_tarif);
			    if ( napr_calc == 'line' )
			    {
                	wm_out = RoundMoney( OrderCalcLine(select_tarif, select_curses, type_i, otdaete) );
                	if(isNaN(wm_out)) wm_out = "";
                	$('#poluchite').val( wm_out );
                }
                else
                {
                 	wm_out = RoundMoney( OrderCalcReverse(select_tarif, select_curses, type_i, poluchite) );
                 	if(isNaN(wm_out)) wm_out = "";
                 	$('#otdaete').val( wm_out );
                }
                 var flag_noorder = 0;
                if( select_tarif.reserv_visible == 1 )
                {
                	$("#div_reserves").show();
                	$("#div_reserves").html(''+select_tarif.reserv+' '+e_valutes[select_tarif.reserv_val].name_o);
                    $("#reserv_value").val(select_tarif.reserv);
                    $("#div_reserves").addClass(valute_poluchite);

                    if(parseFloat($('#poluchite').val()) >  parseFloat(select_tarif.reserv))
                    {
                    	$("#poluchite_alert_inout").html('Резерва недостаточно');
                    	flag_noorder = 1;
                    }
                }
                $("#minimalSum").css('display','');

                if( valute_otdaete == 46 )
                {
                	$("#minimalSum").html('от '+select_tarif.minimalSum+' '+valutes[select_tarif.minimalSum_valute].name_o+' и не более 15 т.р. за одну операцию');
                }
                else
                {
                	$("#minimalSum").html('от '+select_tarif.minimalSum+' '+valutes[select_tarif.minimalSum_valute].name_o);
                }

			    result = 'ordern';

			    if($('#otdaete').val()!='' && $('#poluchite').val()!='' && flag_noorder == 0 )
			    {
			    	$('.submit_order').removeAttr('disabled');
			    }
			}
			else
			{
				return false;
			}
            //alert(name_tarif);

		}

        if(result!='')
        {
        	$("#wm_mod").val(result);
        }


	    return result;
	}


	//Прямой калькулятор на ввод
	function OrderCalcLine(select_tarif, select_curses, type, wm_in)
	{


    	var wm_out;
        var wm_in_schet;

        	if(!select_tarif || !select_curses){

        		$('#error_form').html('В этом направлении данный пункт обмена обмен не производит!');

        		return wm_out;
        	}

        if(type=="out")
        {
        	wm_in_schet=wm_in*select_curses.curse;

        }
        else
        {
        	wm_in_schet = wm_in;
        }


      //   alert(select_tarif.type_calc);
    	jQuery.each(select_tarif, function(i, val) {

        	if (wm_in>= val.ot && wm_in < val.do)
        	{

        		if( select_tarif.type_calc == 1 )
        		{
        			var comissionTest = wm_in_schet*(1+(val.procent/100)) - wm_in_schet;
        			if( Math.abs( comissionTest ) > Math.abs( val.komission ) )
        				wm_out = wm_in_schet*(1+(val.procent/100));
        			else
        				wm_out = wm_in_schet+val.komission;
        		}
        		else
        		{
        			wm_out = wm_in_schet*(1+(val.procent/100))+val.komission;
        		}

        	}

    	});


       // alert(select_curses.curse);
        if(type=="in")
        {
        	wm_out=wm_out*select_curses.curse;
        	//alert(wm_in);
        }

		return wm_out;

	}

	function OrderCalcReverse(select_tarif, select_curses, type, wm_in)
    {

    	var wm_out;
        var wm_in_schet;

        if(!select_tarif || !select_curses)
        {
        	$('#error_form').val('В этом направлении данный пункт обмена обмен не производит!');
        	return wm_out;
        }

        if(type=="in")
        {
        	wm_in_schet=wm_in/select_curses.curse;
        	//alert(wm_in);
        }
        else
        {
        	wm_in_schet=wm_in;
        }

					var LeftMinSum=0; // левое минимальное
                    var RightMinSum=0; // правое минимальное
                    var LeftNumTarif; // левый тариф
                    var RightNumTarif; // правый тариф
                    var flag=0; //флаг того что тариф нашелся


        jQuery.each(select_tarif, function(i, val) {

        	if (wm_in>= val.minSummItog && wm_in < val.maxSummItog)
        	{
              // $('#testjs').html(val.do);
                 if( select_tarif.type_calc == 1 )
                 {
                 	var TestComission = wm_in_schet / ( 1+( val.procent / 100 ) ) - wm_in_schet;

                 	if( Math.abs( TestComission ) > Math.abs( val.komission ) )
                 	{
                 		wm_out = ( wm_in_schet ) / ( 1 + ( val.procent / 100 ) );
                 	}
                 	else
                 	{
                 		wm_out=(wm_in_schet-val.komission);
                 	}


                 }
                 else
                 {
        		 	wm_out=(wm_in_schet-val.komission)/(1+(val.procent/100));
        		 }
        	}
			else
			{

				if( wm_in >= val.minSummItog )
				{
					LeftMinSum=Math.max(LeftMinSum, val.minSummItog);

					if(LeftMinSum==val.minSummItog)
					{
							LeftNumTarif=val.do;
					}
				}

				if( wm_in < val.maxSummItog )
				{
						if(RightMinSum==0)
						{
								RightMinSum = val.maxSummItog;
						}

						RightMinSum=Math.min(RightMinSum, val.maxSummItog);

						if(RightMinSum==val.maxSummItog)
						{
								RightNumTarif=val.ot;
						}
				}


			}




    	});

    			if(!wm_out)
       			{

          				if(LeftNumTarif && RightNumTarif)
                        {
                        	wm_out = LeftNumTarif;
                        }
          		}
        if(type=="out")
        {
        	wm_out = wm_out/select_curses.curse;
        	//alert(wm_in);
        }

    	return wm_out;

    }


    function calcLine()
    {
    	var TypeOperation = DetectTypeOperation('line', '4');
    }

    function calcComission()
    {
    	var TypeOperation = DetectTypeOperation('comission', '4');
    }

    function calcReverse()
    {
    	var TypeOperation = DetectTypeOperation('reverse', '4');
    }

	function ChangeSelect(detect_select)
	{
		if($("#valute_otdaete").val()=="wmr" || $("#valute_otdaete").val()=="wmz" || $("#valute_otdaete").val()=="wme")
		{
			$('.tr_comission').css('display','');
			$('#text_for_comission').html('отдаете +0,8%');
		}
		else if($("#valute_otdaete").val()=="yad")
		{
			$('.tr_comission').css('display','');
			$('#text_for_comission').html('отдаете +0,5%');
		}
		else
		{
			$('.tr_comission').css('display','none');
			$('#text_for_comission').html('');
		}

     if ( detect_select == 3 )
     {
     	ChangeStranaValue();

     }


     //Определение типа операции
     var TypeOperation = DetectTypeOperation('line', detect_select);
	}

  	function select_curses(valute_otdaete, valute_poluchite)
	{

    	r = curses[valute_otdaete + "-" + valute_poluchite];
       	return r;
	}


	function select_tarif(valute_otdaete, valute_poluchite, punkt)
	{

		var r;
		//$('#testjs').html(valute_otdaete + "-" + valute_poluchite + "-" +punkt);
		if(punkt!=0)
		{
        	r = tarifs[valute_otdaete + "-" + valute_poluchite + "-" +punkt];

            var r_tes=  tarifs[valute_otdaete + "-" + valute_poluchite + "-" +punkt];
		}


		return r;
	}

	function select_op(from,to, checke)
	{

  		$('#valute_otdaete option[value="' + from + '"]').prop('selected',true);
  		$('#valute_poluchite option[value="'+to+'"]').prop('selected',true);

  		if( checke == 1 )
  		{
  			$("#CheckEconom1").attr("checked", "checked");
  			$("#CheckEconom0").removeAttr("checked");
  		}
  		else
  		{
  			$("#CheckEconom0").attr("checked", "checked");
  			$("#CheckEconom1").removeAttr("checked");
  		}

  		ChangeSelect(4);
  		return false;
	}
	function select_op_text(from,to)
	{


  		var sel_otdaete = $('#valute_otdaete :contains('+from+')');
  		$.each(sel_otdaete, function(){
  			if( $(this).text() == from )
  			{
  				$(this).prop('selected',true);
  			}
  		});


  		$('#valute_poluchite :contains('+to+')').prop('selected',true);
  		ChangeSelect(4);
  		return false;
	}

	function sub_select_punkt( punkt )
	{

		$('#variants_select option[value="' + punkt + '"]').prop('selected',true);
  		ChangeSelect(4);
  		return false;
	}

    function comebackMenu()
    {
    	resetForm();
    }
	function resetForm()
	{
		//Сбросс данных кеширования
		$("#valute_otdaete :first").attr("selected", "selected");
		$("#valute_poluchite :first").attr("selected", "selected");
		$("#valute_poluchite option[value='28']").removeAttr("disabled"); 
$("#valute_poluchite option[value='10']").removeAttr("disabled"); 
$("#valute_poluchite option[value='128']").removeAttr("disabled"); 
$("#valute_poluchite option[value='47']").removeAttr("disabled"); 
$("#valute_poluchite option[value='11']").removeAttr("disabled"); 
$("#valute_poluchite option[value='32']").removeAttr("disabled"); 
$("#valute_poluchite option[value='34']").removeAttr("disabled"); 
$("#valute_poluchite option[value='48']").removeAttr("disabled"); 
$("#valute_poluchite option[value='169']").removeAttr("disabled"); 
$("#valute_poluchite option[value='50']").removeAttr("disabled"); 
$("#valute_poluchite option[value='49']").removeAttr("disabled"); 
$("#valute_poluchite option[value='181']").removeAttr("disabled"); 
$("#valute_poluchite option[value='57']").removeAttr("disabled"); 
$("#valute_poluchite option[value='12']").removeAttr("disabled"); 
$("#valute_poluchite option[value='nal']").removeAttr("disabled"); 
$("#valute_poluchite option[value='44']").removeAttr("disabled"); 
		$("#valute_otdaete option[value='nal']").removeAttr("disabled"); 
		$("#otdaete").removeAttr("disabled");
		$("#poluchite").removeAttr("disabled");
		$("#otdaete_komission").removeAttr("disabled");
		$("#otdaete").val('');
		$("#poluchite").val('');
		$("#otdaete_komission").val('');
		$("#otdaete_city_in").val('');
		$('#out_variants').css('display','none');
		$('.inout_visible').css('display','none');
		$('.inout_visible_37').css('display','none');
        $('.tr_comission').css('display','none');
        $('#div_reserves').html('');
        $('#div_reserves_econom').html('');
        $('#div_reserves_owner').html('');
        $('#error_form').html('');
        $('#type_napr_select').html('');
        $(".entermoney-service-info").css('display','none');
        $("#notice_error_form").html('');
        $('#minimalSum').html('');
        $("#poluchite_alert").html('');
        $("#poluchite_alert_inout").html('');
        $('.change-block').css('display','none');
        $('.input_sum').css('display','none');
        $('.inout_visible_submit').css('display','none');
        $('.popular-block').css('display','');
        $('.right-block').css('display','none');
        $('.btn_oneclick').css('display','none');
        $('#econom_out').css('display','none');
        $('#nonres_out').css('display','none');
        $('#owner_in').css('display','none');
        $(".sberhtml").html("");
        $("#sw_type").hide();
        $("#buy_all_change").hide();
        $("#buy_all_inout").hide();
        $(".economNal").html('Экономичный обмен (от ИП)');
 		return false;
	}

     function ajax_table_punkts()
     {
             	setTimeout(function() {

          		var options = {
                url: '/wm_soft/modules/ajax_table_punkts_tarifs.php?RND='+Math.random(),
                type: "POST",
                data: $(".private_calc").serializeArray(),
                beforeSend: function()
                {
              		//$('#divCheckLoad').css('display','block');

            	},
                  error: function()
                {
              		$("#out_select_region").html('Сервис временно не доступен');
            	},

              	success:
                  function(msg) {


                  $(".out_variants_div").html(msg);
                                                       }
                };


                $.ajax(options);

                },500);

                              if($('#otdaete_city_in').val()!="")
                              {
                                $("#notice_error_form").html( '' );
                                $('#out_variants').css('display','');
                              }
                              else
                              {
                              	$('#out_variants').css('display','none');
                              }

                          return false;


     }

$(document).ready(function() {

             resetForm();
                                              		$("#strana_value_in").change( function(){
       			$('#otdaete_city_in').val('');
       			$("#variants_select :first").attr("selected", "selected");
       			$('#out_variants').css('display','none');
            	$("#notice_error_form").css('display','');
            	notice_country();


       		});

    $('.backlisthref').click(function(){
      resetForm();
      return false;
    });


	$("#otdaete_city_in").focusout(function(){ ajax_table_punkts(); });




////Проверка перед отправкой формы
	 $("#private_calc").submit(function(){

     	if($("#wm_mod").val()=="wm_change")
     	{
      		if($("#poluchite").val() < 1.25)
      		{
      			$("#error_form").html('Минимальная сумма к получению 1.25 wm');
      			return false;
        	}
      		if($("#e-mail").val() =="")
      		{
      			$("#error_form").html('Не заполнено поле e-mail');
      			return false;
        	}
       		if($("#cash-num").val() =="")
      		{
      			$("#error_form").html('Не заполнено поле номер кошелька');
      			return false;
        	}
        	if($("#listid").val() =="")
      		{
      			$("#error_form").html('Извините, данное направление обмена сейчас недоступно');
      			return false;
        	}
        }
        else if($("#wm_mod").val()=="ordern")
        {
        	if(parseFloat($("#poluchite").val()) > parseFloat($("#reserv_value").val()))
        	{
      			$("#error_form").html('Извините, резерва не достаточно');
      			return false;
        	}
        }

	 });

});

