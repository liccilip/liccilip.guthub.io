<script>
(function($) {
$(function() {

  $('.exchange-punkts-items').delegate('.exchange-punkt-item:not(.current)', 'click', function() {
    var num=$(this).index()+1;
    console.log(num);
    $(this).addClass('current').siblings().removeClass('current')

      .parents('div#fragment-6').find('div.left-fragment').hide().eq(num).fadeIn(150);
  })

})
})(jQuery)
</script>

