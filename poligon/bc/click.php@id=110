$(document).ready(function(){
$('.out_country_select').hide();
     //juridical lica in
  $("#fragment-2 .person-type").change(function()
  {
      if($("#fragment-2 input:radio[name=person-type]:checked").val()=="ur"
      || $("#fragment-2 input:radio[name=person-type]:checked").val()=="ip")
      {
      $(".country_select").hide();
      $(".in_variants").hide();
      $(".in_selelctOtdaete").hide();
      $(".in_title_otdaete").hide();
      $("#vvod-submit").removeAttr("disabled");

      }
      else
      {
        $(".in_selelctOtdaete").show();
        $(".in_title_otdaete").show();
        $(".in_selelctOtdaete :first").attr("selected", "selected");
        $("#vvod-submit").attr("disabled", "true");
      }
    });


  $(".in_selelctOtdaete :first").attr("selected", "selected");
  $("#vvod-submit").attr("disabled", "true");

  $(".in_kak_eto").click(function()
  {
    $("#kak_eto_block").show();
    return false;
  });

  $("#kak_eto_block_hide").click(function()
  {
    $("#kak_eto_block").hide();
    return false;
  });

  $(".in_selelctOtdaete").change(function()
  {
    if($(".in_selelctOtdaete option:selected").val()=="nal")
    {
      $(".country_select").show();

      $("#vvod-submit").attr("disabled", "true");
    }
    else
    {
      $(".country_select").hide();
      $(".in_variants").hide();
      $("#vvod-submit").removeAttr("disabled");
    }
  });



        jQuery("#u245").focusout
        (


            function(){
                    setTimeout(function() {
          var options = {
                url: '/wm_soft/modules/table_tarifs/ajax_table_punkts.php?RND='+Math.random(),
                type: "POST",
                data: jQuery(".form_in_imenu").serializeArray(),
                beforeSend: function()
                {
              //jQuery('#divCheckLoad').css('display','block');

            },
                  error: function()
                {
              jQuery("#select_region").html('Сервис временно не доступен');
            },

              success:
                  function(msg) {


                  //jQuery('#divCheckLoad').css('display','none');
                  jQuery(".in_variants_div").html(msg);


                          }
                };
                        // alert($('#u245').val());
                        jQuery.ajax(options);
                              },500);

                              if($('#u245').val()!="")
                              {
                                $(".in_variants").show();
                              }

                          return false;
              }

              );

});

</script>

