                    function sberbank_out(type)
                    {

                    	if($('#resident0:checked').length)
                    	{
                    		var SBprocent = 1;
                    		var SBcomMax = 1000;
                        }
                        else
                        {                        	var SBprocent = 1;
                        	var SBcomMax = 1000;                        }

                    	if(type=='reverse')
                    	{
                        	var val_in = parseFloat($('#poluchite').val().replace(',','.'));
                        	var SBcomis = (1+SBprocent/100)*val_in-val_in;
                        	if( SBcomis > SBcomMax )
                        	{
                        		SBcomis = SBcomMax;
                        	}
                            SBcomis = Math.round(SBcomis * 100)/100;
                        	if( SBcomis > 0 )
                        	{
                        		if( SBcomis < 0)
                        			SBcomis = 0
                        		$('.sberhtml').html('комиссия банка '+SBcomis+' руб.');
                        	}
                        	else
                        	{                        		$('.sberhtml').html('');                        	}
                        	s_out = val_in*1 + SBcomis;

                        	return s_out;
                    	}
                    	else
                    	{
                        	var val_out = parseFloat($('#poluchite').val().replace(',','.'));
                        	var SBcomis = val_out - val_out/(1+( SBprocent / 100 ) );
                    		if(SBcomis> SBcomMax )
                    		{
                    			SBcomis = SBcomMax;
                    		}
                    		SBcomis = Math.round(SBcomis * 100)/100;
                        		if( SBcomis < 0)
                        			SBcomis = 0
                    		var SBreturn = Math.round((val_out - SBcomis)*100)/100;
                            if( SBreturn > 0 )
                            {
                    			$('#poluchite').val(SBreturn);
                    			$('.sberhtml').html('комиссия банка '+SBcomis+' руб.');
                            }
                            else
                            {                            	$('.sberhtml').html('');                            }
                    	}

                    }      
                    function sberbank_out_nal(type)
                    {

                    	if($('#resident0:checked').length)
                    	{
                    		var SBprocent = 1;
                    		var SBcomMax = 1000;
                        }
                        else
                        {                        	var SBprocent = 1;
                        	var SBcomMax = 1000;                        }

                    	if(type=='reverse')
                    	{
                        	var val_in = parseFloat($('#poluchite').val().replace(',','.'));
                        	var SBcomis = (1+SBprocent/100)*val_in-val_in;
                        	if( SBcomis > SBcomMax )
                        	{
                        		SBcomis = SBcomMax;
                        	}
                            SBcomis = Math.round(SBcomis * 100)/100;
                        	if( SBcomis > 0 )
                        	{
                        		if( SBcomis < 150)
                        			SBcomis = 150
                        		$('.sberhtml').html('комиссия банка '+SBcomis+' руб.');
                        	}
                        	else
                        	{                        		$('.sberhtml').html('');                        	}
                        	s_out = val_in*1 + SBcomis;

                        	return s_out;
                    	}
                    	else
                    	{
                        	var val_out = parseFloat($('#poluchite').val().replace(',','.'));
                        	var SBcomis = val_out - val_out/(1+( SBprocent / 100 ) );
                    		if(SBcomis> SBcomMax )
                    		{
                    			SBcomis = SBcomMax;
                    		}
                    		SBcomis = Math.round(SBcomis * 100)/100;
                        		if( SBcomis < 150)
                        			SBcomis = 150
                    		var SBreturn = Math.round((val_out - SBcomis)*100)/100;
                            if( SBreturn > 0 )
                            {
                    			$('#poluchite').val(SBreturn);
                    			$('.sberhtml').html('комиссия банка '+SBcomis+' руб.');
                            }
                            else
                            {                            	$('.sberhtml').html('');                            }
                    	}

                    }
			function DopFunctionCalc(type, wm_out, valute_poluchite)
			{
				var r_out = wm_out;

                if( valute_poluchite == 28 )
				{
					if($("#CheckEconom1:checked").length == 0 || !$("#CheckEconom1"))
					{
						r_out = sberbank_out(type);                    }
                    else
                    {
    					$(".sberhtml").html("");
                    }
                }
                else if( valute_poluchite == 44 )
                {
                	if($("#CheckEconom1:checked").length == 0 || !$("#CheckEconom1"))
                	{
                		r_out = sberbank_out_nal(type);                	}
                    else
                    {
    					$(".sberhtml").html("");
                    }
                }
    			return r_out;
			}
		function strtolower( str )
{
	return str.toLowerCase();
}

function comission(type_c, val_e, wm_in)
{
	var wm_out;
	type_c=(type_c).toString();

	switch (type_c)
	{

		case '0':

var wm_com=wm_in*0.008;   //РїРѕРґСЃС‡РµС‚ РєРѕРјРёСЃРёРё
if(wm_com<0.01){wm_com=0.01;}
 var val_es=strtolower(val_e);
// РѕРїСЂРµРґРµР»РµРЅРёРµ РІ Р·Р°РІРёСЃРёРјРѕСЃС‚Рё РѕС‚ РІР°Р»СЋС‚С‹ РјР°РєСЃРёРјР°Р»СЊРЅР°СЏ РєРѕРјРёСЃСЃРёСЏ

if(val_es=="wmr"){var max_lim=1500;}
else if(val_es=="wmz"){var max_lim=50;}
else if(val_es=="wme"){var max_lim=50;}
else if(val_es=="wmu"){var max_lim=250;}
else if(val_es=="wmb"){var max_lim=100000;}
else if(val_es=="wmy"){var max_lim=55000;}
else if(val_es=="wmg"){var max_lim=2;}
else{var max_lim=50;}
if(wm_com>=max_lim){wm_out=max_lim;}else{wm_out=Okruglb(wm_com*100)/100;}
break;case '1': 
 wm_out=0; 
 break; 
case '2': 
 wm_out=Okrugl((wm_in*1.005-wm_in)*100)/100; 
 break; 
case '3': 
 wm_out=wm_in*0.001; 
 break; 
default: 
 wm_out=0;  
 break; 

	}


	return (wm_out);
}		function strtolowers( str ) {	// Make a string lowercase

	return str.toLowerCase();
}
function comission_obr(type_c, val_e, wm_in)
{
	var wm_out;
    type_c=(type_c).toString();
		switch (type_c)
		{

				case '0':
			var wm_com=wm_in-wm_in/1.008;   //РїРѕРґСЃС‡РµС‚ РєРѕРјРёСЃРёРё

              var val_es=strtolowers(val_e);
			// РѕРїСЂРµРґРµР»РµРЅРёРµ РІ Р·Р°РІРёСЃРёРјРѕСЃС‚Рё РѕС‚ РІР°Р»СЋС‚С‹ РјР°РєСЃРёРјР°Р»СЊРЅРѕР№ РєРѕРјРёСЃСЃРёРё
			if(val_es=="wmr"){var max_lim=1500;}
			else if(val_es=="wmz"){var max_lim=50;}
			else if(val_es=="wme"){var max_lim=50;}
			else if(val_es=="wmu"){var max_lim=250;}
			else if(val_es=="wmb"){var max_lim=100000;}
			else if(val_es=="wmy"){var max_lim=55000;}
			else if(val_es=="wmg"){var max_lim=2;}
			else{var max_lim=50;}
			if(wm_com>=max_lim){wm_out=max_lim;}else{wm_out=Okrugl(wm_com*100)/100;}

			if(wm_out<0.01){wm_out=0.01;}
		break;
case '1': 
 wm_out=0; 
 break; 
case '2': 
 wm_out=Okrugl((wm_in*0.005)*100)/100; 
 break; 
case '3': 
 wm_out=wm_in*0.001; 
 break; 
default: 
 wm_out=0;  
 break; 

		}


	return (wm_out);
}
		 function Okrugl(vved)
		{
  			if(vved < 0) vved=0;

  			return (Math.round(vved) );
 		}

 function Okruglb(vved)
		{
  			if(vved < 0) vved=0;
  			return (Math.ceil(vved) );
 		}
 function Okruglm(vved)
		{
  			if(vved < 0) vved=0;
  			return (Math.floor(vved) );
 		}
 function RoundMoney(vved)
		{
  			if(vved < 0) vved=0;

  			vved = Math.round(vved*10000)/10000;

  			vved = Math.round(vved*1000)/1000;

  			return (Math.round(vved*100)/100 );
 		}
 function RoundMoneyCeil(vved)
		{
  			if(vved < 0) vved=0;
  			//vved = Math.round(vved*10000)/10000;

  			//vved = Math.round(vved*1000)/1000;

  			return (Math.ceil(vved*100)/100 );
 		}
 function RoundMoneyFloor(vved)
		{
  			if(vved < 0) vved=0;
  			//vved = Math.round(vved*10000)/10000;

  			//vved = Math.round(vved*1000)/1000;

  			return (Math.floor(vved*100)/100 );
 		}
 function RoundCurse(vved)
		{
  			if(vved < 0) vved=0;
  			return (Math.round(vved*10000)/10000 );
 		}
 function RoundCurseCeil(vved)
		{
  			if(vved < 0) vved=0;
  			return (Math.ceil(vved*10000)/10000 );
 		}
