    <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
        function click_custom(comment_id) {
            jQuery.ajax({
                url: 'https://dbchange.ru/wp-admin/admin-ajax.php',
                type: 'POST',
                data: {
                    'action': 'comment_load_ajax',
                    'comment_id': comment_id
                },
                dataType: "json",
                success: function (myResult) {
                    jQuery('.popup.popup-review .popup-review__name').html(myResult['name']);
                    jQuery('.popup.popup-review .popup__content time').html(myResult['date']);
                    jQuery('.popup.popup-review .text p').html(myResult['comment']);
                },
                error: function (error) {
                    console.log(error);
                }
            });
        };
    </script>

