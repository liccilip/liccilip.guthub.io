<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {

$(function() {
   $('.FromCurrencies ul li').on('click', function(){

    $.ajax({
  type: 'POST',
  url: '/site/ajaxUpdate',
  data: { 'currency_id' : $(this).data('currency_id')},
  success: function(data){
    $('#second_list').html(data);
  }
  });
  });

   $('.FromCurrencies ul li').first().click();

});


});
/*]]>*/
</script>
