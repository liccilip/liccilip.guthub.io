<script src="/assets/5404ba91/yii.js"></script>
<script src="/js/libs/bootstrap-transition.min-2.0.3.js"></script>
<script src="/js/libs/bootstrap-modal.min-2.0.4.js"></script>
<script src="/js/functions.js?ver=0.6.7"></script>
<script src="/js/main.js?ver=0.6.7"></script>
<script>jQuery(function ($) {

 function getAjaxForm(fromSignature, toSignature){
    let formCont = $('[data-target="main-form-cont"]');
    formCont.append('<div class="loading-form"></div>');
    
     $.ajax({
            url: '/get-by',
            dataType: 'json',
            type: 'GET',
            data: 'from='+fromSignature+'&to='+toSignature,
            success: function (result) {
                if(result.status === 'success'){
                    formCont.html(result.html);
                    focusInput();
                }
                else if (result.status === 'reload') {
                    location.reload();
                }
                else if (result.status === 'error') {
                    modalInfo(result.message, {isError: true, reload: true,});
                }
            },
            error: function () {
                modalInfo('Error', {isError: true, reload: true,});
            }
        });
 }
 
 let fromSignature = $('.active-currency[data-type="changeFrom"]').data('href');
 let toSignature = $('.active-currency[data-type="changeTo"]').data('href');
 //getAjaxForm(fromSignature,toSignature);
 
 
   $( document ).on( 'click','[data-target="change"]', function(e) {
     
        e.preventDefault();
        
        let type = $(this).data('type');
        if (type === 'changeFrom' && !$(this).hasClass('active-currency')) {
            let from = $(this).data('href');
            $('[data-target="changeTabCont"]').hide();
            $('[data-target="changeTabCont"][data-from="' + from + '"]').fadeIn(300);
            $('[data-type="changeTo"]').removeClass('active-currency');
        }
        
        $('[data-type="' + type + '"]').removeClass('active-currency');
        $(this).addClass('active-currency');
        let fromSignature = $('.active-currency[data-type="changeFrom"]').data('href');
        let toSignature = $('.active-currency[data-type="changeTo"]').data('href');
        
        if (fromSignature && toSignature) {
           getAjaxForm(fromSignature, toSignature);
        }
   });
 
});</script>    <!-- BEGIN JIVOSITE CODE {literal} -->
    <script type='text/javascript'>
        (function() {
            var widget_id = 'vVO8ROPkCA';
            var d = document;
            var w = window;
            
            function l()
            {
                var s = document.createElement('script');
                s.type = 'text/javascript';
                s.async = true;
                s.src = '//code.jivosite.com/script/widget/' + widget_id
                ;var ss = document.getElementsByTagName('script')[0];
                ss.parentNode.insertBefore(s, ss);
            }
            
            if (d.readyState == 'complete') {
                l();
            } else {
                if (w.attachEvent) {
                    w.attachEvent('onload', l);
                } else {
                    w.addEventListener('load', l, false);
                }
            }
        })();
    </script>
    <!-- {/literal} END JIVOSITE CODE -->
