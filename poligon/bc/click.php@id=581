<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.15/jquery.mask.min.js"></script>

<script type="text/javascript">
function check_mail(){
	var id = $("#three_email").val();
	var data_url = "/requests/check_mail.php?mail=" + id;
	$.ajax({
		type: "GET",
		url: data_url,
		dataType: "html",
		success: function (data) {
			if (data=="SUCCESS"){$("#email_error").hide();}
			else {
			$("#email_error").show();
		}
		}
	});

}

function dispay_this(elem) {
	$(elem).show();
}

function undispay_this(elem) {
	$(elem).hide();
}

function remove_focus(input) {
	console.log('INPUT: ', input);
	$(input).removeClass("focused");
}

/*
 * Функция обновляющая маски
 * accin - Отправляемое
 * accout - Получаемое
 */
function update_type(type){
	//console.log(type);
	if (type == "#accin") {
		var databody = $("#bit_gateway_send").val();
	}
	else {
		var databody = $("#bit_gateway_receive").val();
		//var databody = $(".main_k0X").data('body');
	}
	console.log('DATABODY:', databody);
	// Если отправляемое QIWI, то применяется маска телефона +9 (999) 999-9999
	if (databody == '2') {
		 $(type).mask("+99 (999) 999-9999", {
			 /* translation: {
				  'Z': {
					  pattern: /[0-9]/, optional: true
				  }
			 }, */
			 placeholder: "+__ (___) ___-____"
	  	 });
	}
	// Если отправляемое СБЕРБАНК, то применяется маска карты (16-18 цифр)
	else if (databody == '11') {
		$(type).mask("9999 9999 9999 9999 99", {placeholder: "____ ____ ____ ____ __"});
	}
	// Если отправляемое АЛЬФАБАНК - 12, ВТБ - 15, то применяется маска карты (16-20 цифр)
	else if (databody == '12' || databody == '15') {
		$(type).mask("9999 9999 9999 9999 9999", {placeholder: "____ ____ ____ ____ ____"});
	}
	// Если отправляемое Тинькофф - 14, Открытие - 17, Газпром - 18, то применяется маска карты (16 цифр)
	else if (databody == '14' || databody == '18' || databody == '17') {
		$(type).mask("9999 9999 9999 9999", {placeholder: "____ ____ ____ ____"});
	}
	// Если отправляемое PerfectMoney - 8, то применяется маска U99999999
	else if (databody == '8') {
		$(type).mask("U99999999", {placeholder: "U________"});
	}
	// Если отправляемое AdvCash - 9, то применяется маска U99999999
	else if (databody == '9') {
		$(type).mask("U 9999 9999 9999", {placeholder: "U ____ ____ ____"});
	}
	// Если отправляемое ЯндексДеньги - 3, то применяется маска 16 цифр
	else if (databody == '3') {
		$(type).mask("9999999999999999", {placeholder: "________________"});
	}
	// Если отправляемое Payeer - 5, то применяется маска P9999999999
	else if (databody == '5') {
		$(type).mask("P9999999999", {placeholder: "P__________"});
	}
	// Если отправляемое Мегафон - 20, то применяется маска 16 цифр
	else if (databody == '20') {
		$(type).mask("9999 9999 9999 9999", {placeholder: "____ ____ ____ ____ __"});
	}
	// Если отправляемое Биткоин - 24
	else if (databody == '24') {
		$(type).mask("ZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZ", {
		    translation: {
				'Z': {
					pattern: /[a-zA-Z0-9]/, optional: true
				}
		    }
		});
	}
	// Если отправляемое Ethereum - 6
	else if (databody == '6') {
		$(type).mask("ZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZ", {
		    translation: {
				'Z': {
					pattern: /[a-zA-Z0-9]/, optional: true
				}
		    }
		});
	}
	// Если отправляемое Telegram, то применяется маска @
	else if (databody == '27' || databody == '28') {
		$(type).mask('@ZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZ', {
		    translation: {
				'Z': {
					pattern: /[a-zA-Z0-9\_-]/, optional: true
				}
		    },
			placeholder: "@__________"
		});
	}
	// Если отправляемое VISA/MC, то применяется маска 16 цифр
	else if (databody == '29') {
		$(type).mask("9999 9999 9999 9999", {placeholder: "____ ____ ____ ____"});
	}
	// Если отправляемое WMZ, то применяется маска Z999999999999
	else if (databody == '10') {
		$(type).mask("Z999999999999", {placeholder: "Z____________"});
	}
	else {
		$(type).unmask();
	}
}

$(document).ready(function() {
    update_type('#accin');
	update_type('#accout');


	$("#three_email").mask("ZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZ", {
		translation: {
			'Z': {
				pattern: /[a-zA-Z0-9\_\-\@\.]/, optional: true
			}
		}
	});

	$("#fio").mask("ZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZ", {
		translation: {
			'Z': {
				pattern: /[а-яА-Яa-zA-Z\s]/, optional: true
			}
		}
	});
});

window.onload = function() {
	// При зигрузке документа проверям лимиты
	//check_limits();
  };

</script>

