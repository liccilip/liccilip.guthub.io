	function show_form(b) {
		if (b) {
			document.getElementById('i9').style.display='block';
			fm.a3.style.visibility='visible';
			fm.lastname.style.visibility='visible';
			fm.firstname.style.visibility='visible';
			fm.passport.style.visibility='visible';
			fm.phone.style.visibility='visible';
			fm.bt.disabled=false;
		} else {
			document.getElementById('i9').style.display='none';
			fm.a3.style.visibility='hidden';
			fm.bt.disabled=true;
		}
	}
