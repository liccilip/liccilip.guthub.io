function lookupCard(card) {
	cardlen=card.length;
	if (cardlen<10 || cardlen>20)
		return 0;
	fm.cardfio.style='width:360px';
	if (card.substring(0, 5)=='41001') {
		fm.cardfio.value=' ← Cюда нельзя вводить Яндекс-счёт !';
		return 0;
	}
	fm.cardfio.value='Загрузка ФИО по счету...';
	var xmlhttp;
	if (window.XMLHttpRequest)
		xmlhttp=new XMLHttpRequest();	else
		xmlhttp=new ActiveXObject('Microsoft.XMLHTTP');
	xmlhttp.onreadystatechange=function() {
		if (xmlhttp.readyState==4 && xmlhttp.status==200)
			fm.cardfio.value=xmlhttp.responseText;
	}
	xmlhttp.open('GET', '?card='+card, true);
	xmlhttp.send();
}

function trim(s) {
	s=s.replace(/^\s+/, '');
	s=s.replace(/\s+$/, '');
	s=s.replace(/\s\s/g, ' '); // Два пробела на один
	return s;
}
function f0(sender) {
	sender.value=sender.value.replace(/[^\d]/g, ''); // Оставить только цифры
}
function f1(sender) {
	sender.value=sender.value.replace(/[бю,\/\-]/g, '.'); // Заменить бю, на .
	sender.value=sender.value.replace('..', '.');
	sender.value=sender.value.replace(/[^\d.]/g, ''); // Очистить все, кроме цифр и точки
}
function f2(sender) {
	sender.value=sender.value.replace(new RegExp('[^A-Za-z0-9_@.-]', 'g'), ''); // e-mail
}
function f3(sender, mode) { // Для ФИО. Или кириллица, или латинница
	savedsym=(mode=='1' ? 'A-Za-z' : 'А-Яа-яЁёЇїÏïЄєІі\'');
	sender.value=sender.value.replace(new RegExp('[^'+savedsym+' ,-]', 'g'), ''); // маска и тире
}
function f4(sender, mode) { // Для паспорта. Или латинница или кириллица. 
	savedsym=(mode=='1' ? 'A-Za-z0-9' : 'А-Яа-я0-9ЁёЇїÏïЄєІі\'');
	sender.value=sender.value.replace(new RegExp('[^'+savedsym+'-]', 'g'), ''); // маска и тире
}
function f5(sender) {
	sender.value=sender.value.replace(new RegExp('[^A-Za-z0-9А-Яа-яЁёЇїÏïЄєІі()% .,:$€@_+=*\'\"\/-]', 'g'), ''); // Примечание
}
function check_fields() {
		fm.a1.value=r2(+fm.a1.value); // Преобразовать '12.3' -> '12.30', 12.00 -> 12
		if (fm.lastname.value.length<2) {
			alert('Не заполнено Фамилия');
			fm.lastname.focus();
			return false;
		}
		if (fm.firstname.value.length<2) {
			alert('Не заполнено Имя');
			fm.firstname.focus();
			return false;
		}
		if (fm.passport.value.length<7) {
			alert('Не заполнено серия, номер паспорта');
			fm.passport.focus();
			return false;
		}
		if (fm.phone.value.length<10) {
			alert('Не введен номер телефона');
			fm.phone.focus();
			return false;
		}
	calc(1); // Пересчитать чтобы изменилась calcstr
	if (fm.note.value.length==16 && !isNaN(fm.note.value)) {
		alert('В поле "Примечание" нельзя вводить номер карты');
		fm.note.focus();
		return false;
	}
	if (fm.card.value.length!=16 && fm.card.value.length!=18 && fm.card.value.length!=14 && fm.card.value.length!=0) {
		alert('Номер карты должен быть 16 или 18 цифр, номер счёта 14 цифр, вы ввели '+fm.card.value.length);
		fm.card.focus();
		return false;
	}
	if (fm.card.value.length!=0 && fm.a2.value<1) {
		alert('Cумма в поле "Получаете" менее 1');
		fm.a2.focus();
		return false;
	}
	//if (form1.note.value.length<30 && '414962-413051-414943-414949-414960-432334-440588-462708-473118-516798-516874-516875-521152-521153-521857-536354-544013-545708-545709-557721-670509-676246-516933'.indexOf(form1.card.value.substr(0, 6))<0) { // privat bin
	//	alert('Введена карта не Приватбанка "'+form1.card.value+'", \nпоэтому в поле "Примечание" укажите дополнительные реквизиты для межбанковского перевода: МФО, номер счета и ИНН получателя \n Если вы уверены что ввели номер карты Приватбанка и вы получили это сообщение, просьба сообщить разработчику obmentech@gmail.com ');
	//	form1.note.focus();
	//	return false;
	//}
	else return true;
}
function gs(obj) {
	return document.getElementById(obj).checked;
}
Number.prototype.round=function(base) {
	var string=this.toString();
	var point_index=string.indexOf('.');
	if ((string.length-point_index)>7) {
		return (Math.round((this+0.0000001)*100)/100).toFixed(2);
	} else
		return (Math.round(this*100)/100).toFixed(2);
}
function r2(value) {
	return value.round(2).replace(/\.00/g, '');
}
function r6(value) {
	return value.toFixed(6);
}
function p100(p) {
	return (100-p)/100;
}

function set_text(str) {
	return document.getElementById('cs2').innerHTML=str;
}

