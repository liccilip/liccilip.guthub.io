jQuery(function($){
	$('.captcha_reload').live('click',function(){

		var dataString='have=reload';
		$.ajax({
		type: "POST",
		url: "https://fehupay.com/wp-content/plugins/exchangebox/post/toajax.php?notcsrf=c7ce2f06c6&myaction=sc_reload",
		dataType: 'json',
		data: dataString,
		success: function(ht)
		{
			if(ht['ncapt1']){
				$('.captcha1').attr('src',ht['ncapt1']);
			}
			if(ht['ncapt2']){
				$('.captcha2').attr('src',ht['ncapt2']);
			}				
		}
		});
		
		return false;
	});
});	
