jQuery(function($){
	
    $('.toolinput').live('mouseenter', function(){
        $(this).parents('.parenttool').find('.exchboxtool').slideDown(200);
    });	
    $('.toolinput').live('mouseleave', function(){
        $(this).parents('.parenttool').find('.exchboxtool').hide();
    });		
	
    $('input').live('click', function(){
        $(this).parents('.changewoerr').removeClass('err');
    });
    $('input').live('keyup', function(){
        $(this).parents('.changewoerr').removeClass('err');
    });		
	
	
	function add_error_in(id, text){
		if(text.length > 0){
			id.parents('.changewoerr').addClass('err');
			id.parents('.changewoerr').find('.wowarnerr').html(text);
		}
	}

    $('.ajax_post_bids').ajaxForm({
        dataType: 'json',
		beforeSubmit: function(a,f,o) {
			
			if(curs1 != curs1_start || curs2 != curs2_start){
				
				var message = "Внимание! За время оформления заявки курс изменился!";
				
				message+="\r\n";
				
				if(curs1_start != curs1){
					message+= "С "+curs1_start+" на "+curs1;
				}
				
				if(curs2_start != curs2){
					message+= "С "+curs2_start+" на "+curs2;
				}
				
				message+="\r\n Нажмите 'OK' чтобы пересчитать по новому курсу";
				
				alert(message);
				curs1_start = curs1;
				curs2_start = curs2;
				
				var vale = $('#wsumm1').val().replace(/,/g,'.');
				var par = 1;
				if($('#wsumm1').attr('round') || $('#wsumm2').attr('round')){
					if($('#wsumm1').attr('round')){
						var vale = $('#wsumm1').val().replace(/,/g,'.');
					} else {
						var vale = $('#wsumm2').val().replace(/,/g,'.');
						par = 2;
					}
				} else {
					var vale = $('#wsumm1').val().replace(/,/g,'.');
				}
				
				if (checknumbr(vale)) {	
					
					console.log('CHECK '+par+' '+vale);
					
					count_peremen(vale, par);	
					$('#wsumm1').blur();
					if(par == 1){
						count_timer1 = 700;
						count1 = true;
					} 
					if(par == 2){
						count_timer2 = 700;
						count2 = true;
					} 
				}
				
				return false;
			}
			
			f.addClass('thisactive');
			$('.thisactive input[type=submit]').attr('disabled',true);
			
        },		
        success: function(ht) {
			    if(ht['otv'] == 'success'){
				    window.location.href = ht['url']; 
			    } else if(ht['otv']== 'error'){
			    	
			    	var text = "";
			    	
					if (ht['errftext']) {
						add_error_in($('#obmen_f'), ht['errftext']);
						text+=ht['errftext']+"\r\n";
					 }
					if (ht['erritext']) {
						add_error_in($('#obmen_i'), ht['erritext']);
						text+=ht['erritext']+"\r\n";
					}
				    if (ht['errotext']) {
						add_error_in($('#obmen_o'), ht['errotext']);
						text+=ht['errotext']+"\r\n";
					}
					if (ht['errs1text']) {
						add_error_in($('#wschet1'), ht['errs1text']);
						text+=ht['errs1text']+"\r\n";
					}					
					if (ht['errs2text']) {
						add_error_in($('#wschet2'), ht['errs2text']);
						text+=ht['errs2text']+"\r\n";
					}					
					if (ht['erremailtext']) {
						add_error_in($('#obmen_email'), ht['erremailtext']);
						text+=ht['erremailtext']+"\r\n";
					}					
					if (ht['errteltext']) {
						add_error_in($('#obmen_tel'), ht['errteltext']);
						text+=ht['errteltext']+"\r\n";
					}					
					if (ht['errskypetext']) {
						add_error_in($('#obmen_skype'), ht['errskypetext']);
						text+=ht['errskypetext']+"\r\n";
					}					
					if (ht['errin1text']) {
						add_error_in($('#wsumm1'), ht['errin1text']);
						text+=ht['errin1text']+"\r\n";
					}					
					if (ht['errin2text']) {
						add_error_in($('#wsumm2'), ht['errin2text']);
						text+=ht['errin2text']+"\r\n";
					}					
										
					if (ht['narr']) {
						var narr = ht['narr'];
						for (var i = 0; i < narr.length; i++) {
							var dpid = narr[i];
							text+=ht['dpid']+"\r\n";
							add_error_in($('#doppole'+dpid), 'Ошибка! ');
						}
					}
					
					alert(ht['text']+"\r\n"+text);
			    } else {
				    alert(ht['text']+" Unknown");
				}
				
				if(ht['ncapt1']){
				    $('.captcha1').attr('src',ht['ncapt1']);
				}
				if(ht['ncapt2']){
				    $('.captcha2').attr('src',ht['ncapt2']);
				}	
		    $('.thisactive input[type=submit]').attr('disabled',false);
			$('.thisactive').removeClass('thisactive');				
        }
    });	
	
	function checknumbr(mixed_var) {
		return ( mixed_var == '' ) ? false : !isNaN( mixed_var );
	}		
	
	
	function count_peremen(summ, dej,no_com_summ1){
		
				var my_summ1 = 0;
				var my_summ1c = 0;
				var my_summ2 = 0;
				var my_summ2c = 0;
				
				console.log(' COUNT CURS1 = ' + curs1);
				console.log(' COUNT CURS2 = ' + curs2);
				console.log(' CURS CURRENT = ' + curs_current);
				
				curs1_start = curs1;
				curs2_start = curs2;
				
				if(dej == 1){				
								
						my_summ1 = summ;
						my_summ1c = my_summ1 + my_summ1*(v1com/100);
							
						if(v1_type != v2_type){
						
							if(curs1 < curs2){
										
								if(skidka > 0){
									curs1 = 1;
									curs2 = curs_current;
								} else {
									curs1 = 1;
									curs2 = curs2;
								}
								
								my_summ2 = my_summ1 * curs2;
								
							} else {
								if(skidka > 0){
									curs1 = curs_current;
									curs2 = 1;
								} else {
									curs1 = curs1;
									curs2 = 1;
								}								
								my_summ2 = my_summ1 / curs1;								
							}
						
						} else {
							
							if(skidka > 0){
								curs1 = curs_current;
								curs2 = 1;
							} else {
								curs1 = curs1;
								curs2 = 1;
							}
								
							my_summ2 = my_summ1 / curs1;
						}
						
																						
					} 
					
					
					if(dej == 2){
					
						my_summ2 = summ;
						
						if(v1_type != v2_type){
						
							if(curs1 < curs2){
								
								if(skidka > 0){
									curs1 = 1;
									curs2 = curs_current;
								} else {
									curs1 = 1;
									curs2 = curs2;
								}
								
								my_summ1 = my_summ2 / curs2;		
														
							} else {
								
								if(skidka > 0){
									curs1 = curs_current;
									curs2 = 1;
								} else {
									curs1 = curs1;
									curs2 = 1;
								}
								
								my_summ1 = my_summ2 * curs1;
								
							}
											
							$curs1 = 1;
							$curs2 = 1/curs_current;
							$summ1c = my_summ1 + my_summ1*(v1com/100);
							
						} else {
							
							if(skidka > 0){
								curs1 = curs_current;
								curs2 = 1;
							} else {
								curs1 = curs1;
								curs2 = 1;
							}
							
							my_summ1 = my_summ2 * curs1;
							my_summ1c = my_summ1 + my_summ1*(v1com/100);
						}
													
					}
					
					my_summ1 = Math.round(my_summ1*100)/100;
					my_summ2 = Math.round(my_summ2*100)/100;
					
					if(dej == 2) $('#wsumm1').val(my_summ1);
					if(dej == 1) $('#wsumm2').val(my_summ2);
		
					console.log('SUMM1 '+my_summ1);
					console.log('SUMM2 '+my_summ2);
	}
	  
	function goed_peremen(summ, dej,no_com_summ1){
		
		var id = $('#wnapobmenid').val();
		
		var dataString = 'id='+id+'&summ='+summ+'&dej='+dej;
        $.ajax({
            type: "POST",
            url: "https://fehupay.com/ajax-exchange_peremen.html?notcsrf=0f9576011c",
            data: dataString,
	        dataType: 'json',
            success: function(ht){
            	
                /*
				if(dej !== 1){
					$('#wsumm1').val(ht['s1']);
				}
				if(dej !== 2){
					$('#wsumm2').val(ht['s2']);
				}
				*/
				
				console.log('C1 Ajax =  '+ht['curs1']*1);
				curs1 = ht['curs1']*1;
				console.log('C2 Ajax =  '+ht['curs2']*1);
				curs2 = ht['curs2']*1;
				console.log('C_CUR Ajax =  '+ht['curs_current']*1);
				curs_current = ht['curs_current']*1;
				
				if(dej !== 3){
					if(!no_com_summ1) $('#wsummcom1').val(ht['sc1']);
				}
				if(dej !== 4){
					$('#wsummcom2').val(ht['sc2']);
				}
				
				//$('#wcomtext-1').html(ht['sctxt1']);
				$('#wcomtext-2').html(ht['sctxt2']);
				
				if(ht['summ1_error'] == 1){
					$('#wsumm1').parents('.changewoerr').addClass('err');
					$('#wsumm1').parents('.changewoerr').find('.wowarnerr').html(ht['summ1_error_text']);
				} else {
					$('#wsumm1').parents('.changewoerr').removeClass('err');					
				}
				if(ht['summ2_error'] == 1){
					$('#wsumm2').parents('.changewoerr').addClass('err');
					$('#wsumm2').parents('.changewoerr').find('.wowarnerr').html(ht['summ2_error_text']);
				} else {
					$('#wsumm2').parents('.changewoerr').removeClass('err');
				}
				if(ht['summ1c_error'] == 1){
					$('#wsummcom1').parents('.changewoerr').addClass('err');
					$('#wsummcom1').parents('.changewoerr').find('.wowarnerr').html(ht['summ1c_error_text']);
				} else {
					$('#wsummcom1').parents('.changewoerr').removeClass('err');
				}
				if(ht['summ2c_error'] == 1){
					$('#wsummcom2').parents('.changewoerr').addClass('err');
					$('#wsummcom2').parents('.changewoerr').find('.wowarnerr').html(ht['summ2c_error_text']);
				} else {
					$('#wsummcom2').parents('.changewoerr').removeClass('err');
				}
				
				
				
            }
		});					    
		
	}
	
	var count_timer1 = 0;
	var count1 = false;
	var round1_mode = false;
	
	window.setInterval(function(){
		if(count1){
			
			count_timer1 -= 30;
			if(count_timer1<0){
				count1 = false;
				count_timer1 = 0;
				var vale = $('#wsumm1').val().replace(/,/g,'.');
				if (checknumbr(vale)) {					
					goed_peremen(vale, 1);
				}
			}
		}
	},30);
	
	window.setInterval(function(){		
		var vale = $('#wsumm1').val().replace(/,/g,'.');
		if (checknumbr(vale)) {			
			goed_peremen(vale, 1);
		}
	},15000);
	
	
	
	var count_timer2 = 0;
	var count2 = false;
	var round2_mode = false;
	
	window.setInterval(function(){
		if(count2){
			
			count_timer2 -= 30;
			if(count_timer2<0){
				count2 = false;
				count_timer2 = 0;
				var vale = $('#wsumm1').val().replace(/,/g,'.');
				if (checknumbr(vale)) {					
					goed_peremen(vale, 1);
				}
			}
		}
	},30);
	
	var count_timer3 = 0;
	var count3 = false;
	
	window.setInterval(function(){
		
		if(count3){
			 
			count_timer3 -= 30;
			if(count_timer3<0){
				count3 = false;
				count_timer3 = 0;
				var vale = $('#wsummcom1').val().replace(/,/g,'.');
				if (checknumbr(vale)) {					
					var comission = $('#wsummcom1').attr('comission');
					var new_value = (vale*100)/(100+comission*100);		
					new_value = Math.round(new_value*100)/100;
					console.log('test');
					goed_peremen(new_value, 1,true);
				}
			}
		}
	},30);
	
		$('#wsumm1').live('keyup',function(){
			var vale = $(this).val().replace(/,/g,'.');
			if (checknumbr(vale)) {				
				count_peremen(vale, 1);				
				//goed_peremen(vale, 1);
				count_timer1 = 1000;
				count1 = true;
			} else {
				$(this).parents('.changewoerr').addClass('err');
			}

		    return false;
		    
		});
		
		
		$('#wsumm2').live('keyup',function(){
			
			console.log('changed2');
			
			
			var vale = $(this).val().replace(/,/g,'.');
			if (checknumbr(vale)) {				
				count_peremen(vale, 2);	
				//goed_peremen(vale, 2);			
				count_timer2 = 1000;
				count2 = true;
			} else {
				$(this).parents('.changewoerr').addClass('err');
			}
			
		    return false;
		    
		});	
		
		$('#wsummcom1').live('keyup',function(){
			
			var vale = $(this).val().replace(/,/g,'.');
			var comission = $(this).attr('comission');
			var new_value = (vale*100)/(100+comission*100);		
			new_value = Math.round(new_value*100)/100;
			   
			$('#wsumm1').val(new_value);
			
			count_peremen(vale, 1);			
			console.log('keyup');
			count_timer3 = 1000;
			count3 = true;
			
		});
		
		
		$('#wsummcom2').live('keyup',function(){
			
			var vale = $(this).val().replace(/,/g,'.');
			
			if (checknumbr(vale)) {
				count_peremen(vale, 2);
				goed_peremen(vale, 4);
			} else {
				$(this).parents('.changewoerr').addClass('err');
			}
			
		    return false;
		});		
	
});
