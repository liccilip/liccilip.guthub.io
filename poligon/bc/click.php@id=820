	<!-- paralax start -->
	<script>
		function parallax() {
			var elipses_1_1_items = $('.p-elipse.i1');
			var elipses_1_2_items = $('.p-elipse.i2');
			var elipses_2_1_items = $('.p-elipse.i2i1');
			var elipses_2_2_items = $('.p-elipse.i2i2');
			var rectangles_1_items = $('.rectangle.i1');
			var rectangles_2_items = $('.rectangle.i2');

			elipses_1_1_items.each(function() { 
				$(this).css('top', (470 + (event.clientY / 40)) + "px");
				$(this).css('right', (-40 + (event.clientX / 40)) + "px");
			});

			elipses_1_2_items.each(function() { 
				$(this).css('top', (270 + (event.clientY / 30)) + "px");
				$(this).css('right', (-20 + (event.clientX / 30)) + "px");
			});

			elipses_2_1_items.each(function() { 
				$(this).css('top', (470 + (event.clientY / 40)) + "px");
				$(this).css('left', (-40 + (event.clientX / 40)) + "px");
			});

			elipses_2_2_items.each(function() { 
				$(this).css('top', (270 + (event.clientY / 30)) + "px");
				$(this).css('left', (-20 + (event.clientX / 30)) + "px");
			});

			rectangles_1_items.each(function() { 
				$(this).css('top', (210 + (event.clientY / 30)) + "px");
				$(this).css('left', (-520 + (event.clientX / 30)) + "px");
			});	
			
			rectangles_2_items.each(function() { 
				$(this).css('top', (210 + (event.clientY / 30)) + "px");
				$(this).css('right', (-520 + (event.clientX / 30)) + "px");
			});
		}
		document.onmousemove = parallax();
	</script>

	<!-- show/hide rewiew	 -->
	<script>
		function show_full_rev(id) {
			$( ".rewiew-text.short." + id ).hide().fadeOut();
			$( ".rewiew-text.full." + id ).show().fadeIn();
			$( ".rewiew-text." + id ).css('height', 'auto');
		}
		function show_short_rev(id) {
			$( ".rewiew-text.full." + id ).hide().fadeOut();
			$( ".rewiew-text.short." + id ).show().fadeIn();
			$( ".rewiew-text." + id ).css('height', 'auto');
		}
	</script>

	<script>
		$(document).ready(function(){
			// Add smooth scrolling to all links
			$("a[anchor='true']").on('click', function(event) {
				if (this.hash !== "") {
					event.preventDefault();
					var hash = this.hash;
			
					$('html, body').animate({
						scrollTop: $(hash).offset().top
						}, 800, function(){
							window.location.hash = hash;
						});
				} // End if
			});
		});
	</script>


				
