                if(pairs.lenght == 0){
                        $('#operator-exchange').html('<div class="exchanges-not-exist"><strong>Unfortunately you have chosen unavailable operation in the section «Manual exchange», please visit us later.</strong></div>');
                }
                
                function setListToExchangeCurrency(){
                    $('#to_curr').find('option').remove();
                    $.each(pairs[$('#from_curr').val()], function(i, val) {
                        $('#to_curr').append('<option value="' + i +'">' +  val.curr_out + '</option>').val(i);
                    });
                    var pair_item = pairs[$('#from_curr').val()][$('#to_curr').val()];

                    if(pair_item.ps_in_id == pair_item.ps_out_id){
                        $.each($('#to_curr').find('option'), function(i, val) {
                            pair_item = pairs[$('#from_curr').val()][$(val).val()];
                            if(pair_item.ps_in_id != pair_item.ps_out_id){
                                $(val).prop('selected', true);
                                return false;
                           }
                        });
                    }
                    // апдейт селекта в плагине chosen
                    $('#to_curr').trigger("chosen:updated");
                }

            
                function setExchangeParams(){
                    payin = $('#from_curr').val();
                    payout = $('#to_curr').val();
                    pair = pairs[payin][payout];
                    if(pair != undefined){
                        initSystemLogo();
                        $('#payin_rate').html(pair.rate_in);
                        $('#payin_curr').html(pair.curr_in);
                        $('#payout_rate').html(pair.rate_out);
                        $('#payout_curr').html(pair.curr_out);
                        $('#reserve').html(pair.reserve + ' ' + pair.ps_curr_out);
                        $('#reserve_out_ps').html(pair.ps_out_name);
                        $('#exchange_fee').html(pair.operation_fees);
                        $('#type_terms').html(pair.type);
                    }
                }
                
                function initSystemLogo(){
                    $('.logo-payin').html(ps_logo[pair.ps_in_id]);
                    $('.logo-payout').html(ps_logo[pair.ps_out_id]);
                }
                
                function setFormTarget(){
                    var action = 'https://my-xchange.com/en/oprequest';
                    action = action + '/' + payin + '/' + payout;
                    $('#exchangeForm').prop('action', action)
                }

                $('body').on('change', '#from_curr', function(event){
                    setListToExchangeCurrency();
                    setExchangeParams();
                    setFormTarget();
                });  

                $('body').on('change', '#to_curr', function(event){
                    setExchangeParams();
                    setFormTarget();
                }); 
                setListToExchangeCurrency();
                setExchangeParams();
                initSystemLogo();
                setFormTarget();
            });        
        </script>

