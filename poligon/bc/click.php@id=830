<script type="text/javascript">
jQuery(function($){

var auto_load = 1;
function globalajax_timer(){

	if(auto_load == 1){
		auto_load = 0;
		
		var param = 'link=%2F%3Fref%3D10&ltime=1552910338';
		$('.globalajax_ind').addClass('active');
		$.ajax({
			type: "POST",
			url: "https://eliteobmen.com/ajax-globalajax_wp_check.html?meth=post&yid=a3707f8fd97b&ynd=0&lang=ru",
			dataType: 'json',
			data: param,
			error: function(res, res2, res3){
				
			var res_content = res2;
		
	$(document).JsWindow('show', {
		id: 'js_error_window',
		div_class: 'error_window',
		title: 'An error has occurred',
		content: res_content,
		shadow: 1
	});	
	
			},			
			success: function(res)
			{		
				if(res['status'] == 'success'){
					auto_load = 1;						
						
	var c_count = res['courselogs_count'];
	var courselogs = res['courselogs'];
	for (var c_key in courselogs){
		var c_data = courselogs[c_key];
		if($('#levents_' + c_key).length == 0){
			$('#last_events').append(c_data);
			var c_len = $('#last_events .levents').length;	
				
			$('#levents_' + c_key).fadeIn(800);
			if(c_len > c_count){
				$('.levents:first').remove();
			}
		}		
	}
				}	
				$('.globalajax_ind').removeClass('active');
			}
		});
	}

}	
	setInterval(globalajax_timer, 60000);
	globalajax_timer();
});	
</script>

