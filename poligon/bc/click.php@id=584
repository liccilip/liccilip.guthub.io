<script type="text/javascript">
    var $modal_content_data = "";
    var $dopclass_window = "";
    
    function modal_heigt_normalize(){
        $content_height = $("div.modal_window .modal_content_wrap").height() + 12;
        $("div.modal_window").css("height", $content_height+"px");
        }
    
    function show_modal(){
        if ($dopclass_window!="") $(".modal_window").addClass( $dopclass_window );
            
        $(".modal_window .modal_content_wrap").html($modal_content_data);
        
        $(".modal_back").fadeIn(50);
        $(".modal_window").fadeIn(200);
        
        setTimeout(modal_heigt_normalize(), 1);
        }
	
	$(".modal_back, .close_modal_win_btn").click(function(){
	   $(".modal_back").css("display", "none");
       $(".modal_window .modal_content_wrap").html("");
	   $(".modal_window").css("display", "none");
	});
    
    $(document).ready(function(){
        //Поля для ввода номера банковских карт - помогаем с набором
        $(".input_card_number").live("input", function(){
            var cardCode = this.value.replace(/[^\d]/g, '').substring(0,20);
            cardCode = cardCode !== '' ? cardCode.match(/.{1,4}/g).join(' ') : '';
            this.value = cardCode;
            }).live("change", function(){
            var cardCode = this.value.replace(/[^\d]/g, '').substring(0,20);
            cardCode = cardCode !== '' ? cardCode.match(/.{1,4}/g).join(' ') : '';
            this.value = cardCode;
            }).live("blur", function(){
            var cardCode = this.value.replace(/[^\d]/g, '').substring(0,20);
            cardCode = cardCode !== '' ? cardCode.match(/.{1,4}/g).join(' ') : '';
            this.value = cardCode;
            }).live("keyup", function(){
            var cardCode = this.value.replace(/[^\d]/g, '').substring(0,20);
            cardCode = cardCode !== '' ? cardCode.match(/.{1,4}/g).join(' ') : '';
            this.value = cardCode;
            });
        
        /*Начало: открыть/закрыть список переключения языков сайта*/
        $(".current_lang_btn").click(function(){
            $("div.choise_other_lang").addClass("click_opened");
            return false;
            })
            
        $("html").click(function(){
            $(".choise_other_lang").removeClass("click_opened");
            })
        /*Конец: отурыть/закрыть список переключения языков сайта*/
        
        /*начало: Показ всплывающего окна при плике на кнопку*/
        $(".modal_window_link").live("click", function(){
            if ($dopclass_window!=""){
                $(".modal_window").removeClass( $dopclass_window );
                $dopclass_window = "";
                }
            if (void 0 !== $(this).attr('dop_class')) if ( $(this).attr("dop_class")!="" ) $dopclass_window = $(this).attr("dop_class");
            
            $modal_content_data = $("#"+$(this).attr("modal_content")).html();
            
            show_modal();
            
            if (!(void 0 !== $(this).attr('need_return_false'))) return false;
        });
        /*Конец: Показ всплывающего окна при плике на кнопку*/
        
        /*начало: Ajax отправка письма обратной связи*/
        var $page_form_id = 0;
        
        $("form.callback_form").submit(function(e){
            //отменяем стандартное действие при отправке формы
            e.preventDefault();
            
            var $form_method = $(this).attr('method');
            var $form_vals = $(this).find("input").serialize();
            if ( $(this).find("textarea").length ) $form_vals = $form_vals+"&"+$(this).find("textarea").serialize();
            var $url = "/send_admin_email";
            
            if ( $(this).find("input[type=submit]").length ) $form_vals = $form_vals+"&"+$(this).find("input[type=submit]").attr("name")+"=1";
            $page_form_id = $(this);
            
            $.ajax({ type: $form_method, url: $url, data: $form_vals, success: function($ajax_data){
                $ajax_data = $.parseJSON( $ajax_data );
                
                if ($ajax_data.status == 0){
                    $page_form_id.find(".ajax_form_validate_errors").html("<div class='form_errors'>"+$ajax_data.text+"</div>").slideDown(200);
                    $page_form_id.find(".ajax_form_suces_send_wrap").slideUp(0);
                } else {
                    $page_form_id.find(".ajax_form_suces_send_wrap").html("<div class='ajax_sucess_form_send'>"+$ajax_data.text+"</div>").slideDown(200);
                    $page_form_id.find(".ajax_form_validate_errors").slideUp(0);
                    $page_form_id.find("input[type='submit']").slideUp(0);
                }
                }});
        })
        /*Конец: Ajax отправка письма обратной связи*/
        
        /*начало: Универсальная плавающая подсказка*/
        var mouse_x = 0;
        var mouse_y = 0;
        $(document).mousemove(function(e) {
        mouse_x = e.pageX;
        mouse_y = e.pageY;
        if ($( '.mouse_move_hint' ).is(":visible")) $( '.mouse_move_hint' ).css( {left: mouse_x+5, top: mouse_y+5} )
        });
        $("[hint_text]").live("mouseenter", function(){
        $hint_text = $(this).attr("hint_text")
        while($hint_text.indexOf('||||') + 1) $hint_text = $hint_text.replace("||||", "<br/>");
        
        $(".mouse_move_hint").html( $hint_text ).show(0).css( {left: mouse_x+5, top: mouse_y+5} );
        }).live("mouseout", function(){
        $(".mouse_move_hint").hide(0);
        });
        /*Конец: Универсальная плавающая подсказка*/
    });
    
    $(window).scroll(function() {
      var top = $(document).scrollTop();
      if (top > 150) $('.lk_personal_menu').addClass('scroll_lk');
      else $('.lk_personal_menu').removeClass('scroll_lk');
      
      if (top > 126) $('ul.main_menu').addClass('scroll_menu');
      else $('ul.main_menu').removeClass('scroll_menu');
     });
</script>
