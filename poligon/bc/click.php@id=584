<noindex>
<script type="text/javascript">
/*начало: обновление обменов, резервов и статистики*/
    var $renew_last_events_in_progress = false;
    var $count_show_operations = 0;
    
    function renew_last_events(){
        if (!$renew_last_events_in_progress){
            $renew_last_events_in_progress = true;
            
            $.ajax({url: "/renew_last_events", success: function($ajax_data){
                $ajax_data = $.parseJSON( $ajax_data );
                
                //Обновляем последние резервы
                if ($ajax_data.site_rezerves !== undefined){
                    $.each($ajax_data.site_rezerves, function($pay_system, $one_site_rezerv) {
                        $one_site_rezerv_container = $(".site_rezerves_wrap .one_site_rezerv[pay_system='"+$pay_system+"']");
                        //Если резерв изменился
                        if ($one_site_rezerv_container.length && $one_site_rezerv_container.attr("rezerv") != $one_site_rezerv.rezerv){
                            $(".temp_div_block").html($one_site_rezerv.html);
                            $one_site_rezerv_container.attr("rezerv", $one_site_rezerv.rezerv).addClass("now_element_renew").html($(".temp_div_block > div").html());
                            }
                        });
                    }
                    
                //Обновляем последние обмены
                if ($ajax_data.last_obmens !== undefined){
                    $.each($ajax_data.last_obmens, function($order_num, $new_order) {
                        $count_show_operations = $new_order.count_show_operations;
                        
                        $exist_order = $(".last_orders_wrap .one_last_obmen[order_num='"+$order_num+"']");
                        
                        //Если поступил новый обмен
                        if (!$exist_order.length){
                            //Создаем новый обмен на странице и разворачиваем
                            
                            $(".temp_div_block").html($new_order.html);
                            $(".temp_div_block > div").css("display", "none");
                            $($(".temp_div_block").html()).prependTo('.last_orders_wrap .last_events_content_wrap').addClass("now_element_renew").slideDown(300);
                            
                            //$($new_order.html).css("display", "none").prependTo('.last_orders_wrap .last_events_content_wrap').addClass("now_element_renew").slideDown(300);
                            }
                        });
                    
                    //Сворачиваем и убираем показанный на странице последний обмен
                    setTimeout(function(){
                        $count_operations_on_page = 0;
                        
                        $(".last_orders_wrap .one_last_obmen").each(function(){
                            $count_operations_on_page = $count_operations_on_page + 1;
                            if ($count_operations_on_page > $count_show_operations) $(this).css("display", "none");
                            });
                        }, 10);
                    }
                
                //Обновляем статистику
                if ($ajax_data.stat_for_users !== undefined){
                    $.each($ajax_data.stat_for_users, function($class_name, $html_value) {
                        $stat_container = $(".obmen_stats_wrap ."+$class_name);
                        
                        //Если резерв изменился
                        if ($stat_container.length && $stat_container.html() != $html_value){
                            $stat_container.html($html_value).addClass("now_element_renew");
                            }
                        });
                    }
                
                setTimeout(function(){
                    $(".last_events_wrap .now_element_renew, .obmen_stats_wrap .now_element_renew").removeClass("now_element_renew");
                }, 700);
                
                $renew_last_events_in_progress = false;
                }});
            }
        }
    
    setInterval(function(){
        renew_last_events();
        }, 25000);
/*конец: обновление обменов, резервов и статистики*/

    //Включать ли исходящую комиссию в курс
    $include_out_pay_komiss_in_course = 1;
    $exist_recive_hidden_komiss = 0;
    
    $summ_limit_errors_min_text = 'Мин. сумма';
    $summ_limit_errors_max_text = 'Макс. сумма';
    
    var $merchant_post_sended = false;
        
    $('.user_obmen_form').submit(function(e){
        //отменяем стандартное действие при отправке формы
        e.preventDefault();
            
        if (!$merchant_post_sended){
            $merchant_post_sended = true;
            
            //берем из формы метод передачи данных
            var m_method=$(this).attr('method');
            //получаем адрес скрипта на сервере, куда нужно отправить форму
            var m_action=$(this).attr('action');
            //получаем данные, введенные пользователем в формате input1=value1&input2=value2...,
            //то есть в стандартном формате передачи данных формы
            var m_data=$(this).find("input").serialize();
                
            $.ajax({
                type: m_method,
                url: m_action,
                data: m_data,
                success: function($ajax_data){
                    $ajax_data = $.parseJSON( $ajax_data );
                    
                    if ($ajax_data.error == 1){
                        $(".user_actions_wrap tr.form_errors_wrap div.form_errors").hide(0).html( $ajax_data.text ).slideDown(300);
                        $(".user_actions_wrap tr.form_errors_wrap").slideDown(0);
                            
                        setTimeout(function(){
                            $(".user_actions_wrap tr.form_errors_wrap div.form_errors").slideUp(300);
                            $(".user_actions_wrap tr.form_errors_wrap").slideUp(300);
                        }, 10000)
                    } else {
                        $dopclass_window = "";
                        $modal_content_data = $ajax_data.text;
                        show_modal();
                    }
                    
                    $merchant_post_sended = false;
                }
                });
            }
        });
    
    
    function floor_ceil( $number, $round, $digits ){
        $number = parseFloat($number);
        
        $mnozitel = 10;
        for($i=1; $i<$digits; $i++) $mnozitel = $mnozitel * 10;
        
        $number = $number * $mnozitel;
        if ($round=="floor") { $number = Math.floor($number);} else $number = Math.ceil($number);
        //alert ($number);
        $number = $number / $mnozitel;
        
        if ( isNaN($number) ) $number = 0;
        
        return $number.toFixed($digits);
        }

    function calculate($field_calc){
        if ($field_calc=="send_summ") { $field_rez = "recive_summ"; $operation="umnoz"; }
        if ($field_calc=="recive_summ") { $field_rez = "send_summ"; $operation="delit"; }
        
        $recive_check = $(".poluchaete_wrap .sys_recivers_wrap .obmen_btns.active");
        $send_check = $(".otdaete_wrap .obmen_btns.active");
        
        if ( $recive_check && $send_check){
            $send_dec_digits = parseInt($send_check.attr("dec_digits"));
            $recive_dec_digits = parseInt($recive_check.attr("dec_digits"));
            $("input[name='input_user_field']").val($field_calc);
            
            $course = parseFloat( $recive_check.attr("recive_course") );
            
            if($course == 1) {
                $course = $recive_check.attr("send_course");
                if ($operation=="delit") { $operation="umnoz"; } else $operation="delit";
                }
            
            $summ = parseFloat( $(".user_actions_wrap input[name='"+$field_calc+"']").val() );
            
            if ($operation=="delit") { $result = $summ / $course; } else $result = $summ * $course;
            if ($field_calc=="send_summ") { $result = floor_ceil( $result, "floor", $recive_dec_digits ); } else {$result = floor_ceil( $result, "ceil", $send_dec_digits ); };
            
            $(".user_actions_wrap input[name='"+$field_rez+"']").val( $result );
            
            $all_for_send = (1 + parseFloat( $(".komiss_from_sender").html() ) / 100) * parseFloat( $("input[name='send_summ']").val() );
            $(".summ_send_from_sender").html( $all_for_send.toFixed($send_dec_digits+1) );
            
            $all_for_recive = (1 - parseFloat( $(".komiss_from_reciver").html() ) / 100) * parseFloat( $("input[name='recive_summ']").val() );
            $(".summ_recive_from_reciver").html( $all_for_recive.toFixed($recive_dec_digits+1) );
            
            /*начало: показываем подсказки минимальных и максимальных сумм*/
            $send_summ = parseFloat( $(".user_actions_wrap input[name='send_summ']").val() );
            $min_send_summ = parseFloat( $send_check.attr("min_summ") );
            $max_send_summ = parseFloat( $send_check.attr("max_summ") );
            if ($send_summ < $min_send_summ) {
                $(".send_summ_limit_errors").html($summ_limit_errors_min_text+": "+$min_send_summ).css("display", "inline");
            } else if ($send_summ > $max_send_summ){
                $(".send_summ_limit_errors").html($summ_limit_errors_max_text+": "+$max_send_summ).css("display", "inline");
            } else $(".send_summ_limit_errors").css("display", "none");
            
            $recive_summ = parseFloat( $(".user_actions_wrap input[name='recive_summ']").val() );
            $min_recive_summ = parseFloat( $recive_check.attr("min_summ") );
            $max_recive_summ = parseFloat( $recive_check.attr("max_summ") );
            if ($recive_summ < $min_recive_summ) {
                $(".recive_summ_limit_errors").html($summ_limit_errors_min_text+": "+$min_recive_summ).css("display", "inline");
            } else if ($recive_summ > $max_recive_summ){
                $(".recive_summ_limit_errors").html($summ_limit_errors_max_text+": "+$max_recive_summ).css("display", "inline");
            } else $(".recive_summ_limit_errors").css("display", "none");
            /*конец: показываем подсказки минимальных и максимальных сумм*/
            
            //Расчитываем сумму скидки
            $user_skidka = parseFloat($recive_check.attr("user_skidka"));
            if ($user_skidka > 0){
                if ($field_rez=="send_summ") $skidka_summ = ($result / (1-$user_skidka)) - $result;
                
                if ($field_rez=="recive_summ") $skidka_summ = $result - ($result / (1+$user_skidka));
                
                if ($skidka_summ.toFixed(2)>0){
                    $skidka_summ = $skidka_summ.toFixed(2)+" ";
                    
                    $("tr.skidka_info_wrap").removeClass("hiden_class");
                    if ($field_rez=="send_summ") {$("tr.skidka_info_wrap .and_dec_send").removeClass("hiden_class"); $skidka_summ = $skidka_summ+$(".user_obmen_tbl .sender_title").html();} else $("tr.skidka_info_wrap .and_dec_send").addClass("hiden_class");
                    if ($field_rez=="recive_summ") {$("tr.skidka_info_wrap .and_add_recive").removeClass("hiden_class"); $skidka_summ = $skidka_summ+$(".user_obmen_tbl .reciver_title").html();} else $("tr.skidka_info_wrap .and_add_recive").addClass("hiden_class");
                    $("tr.skidka_info_wrap .green_color").html( $skidka_summ );
                    } else $("tr.skidka_info_wrap").addClass("hiden_class");
                } else $("tr.skidka_info_wrap").addClass("hiden_class");
            }
        }
    
    $("[name='order_cansel_btn']").live("click", function(){
        $num = $(".submit_wrap input[name='order_num']").val();
        $.ajax({url: "/cansel_order/"+$num, success: function(){}});
        
        $(".modal_back").click();
        })
    
    $(".user_actions_wrap input[name='send_summ'], .user_actions_wrap input[name='recive_summ']").keyup(function(){
        $value = $(this).attr("value");
        $value = $value.replace(",",".");
        if ($value!=$(this).attr("value")) $(this).attr("value", $value);
        
        calculate( $(this).attr("name") );
    })
    
    $(".otdaete_wrap .obmen_btns").click(function(){
        $(".otdaete_wrap .obmen_btns, .poluchaete_wrap .obmen_btns").removeClass("active");
        $(this).addClass("active");
        
        $(".poluchaete_wrap .obmen_btns").removeClass("showed");
        
        $num_recive = $(this).attr("num_recive");
        $(".poluchaete_wrap .sys_recivers_wrap").hide(0);
        $(".poluchaete_wrap .sys_recivers_wrap[num_recive='"+$num_recive+"']").show(0);
        $(".poluchaete_wrap .sys_recivers_wrap[num_recive='"+$num_recive+"'] .obmen_btns").addClass("showed");
        
        $(".obmen_podskazki").html( $(".obmen_podskazki").attr("num_2") );
        if ( $(".obmen_podskazki").hasClass("dop_view") ) { $(".obmen_podskazki").removeClass("dop_view"); } else $(".obmen_podskazki").addClass("dop_view");
        
        $(".user_obmen_form").slideUp(300);
        
        //Составляем список кошельков юзера для подстановки в "Отдаете"
        if ($(".user_wallets_for_choice .one_pay_sys[pay_sys_num='"+$(this).attr("pay_sys_num")+"']").length && $(".user_wallets_for_choice .one_pay_sys[pay_sys_num='"+$(this).attr("pay_sys_num")+"']").html().trim()!=""){
            $(".otdaete_wallet_choice .wallet_for_scroll").html( $(".user_wallets_for_choice .one_pay_sys[pay_sys_num='"+$(this).attr("pay_sys_num")+"']").html() );
            $(".otdaete_choice_strelka").css("display", "block");
        } else {
            $(".otdaete_wallet_choice .wallet_for_scroll").html("");
            $(".otdaete_choice_strelka").css("display", "none");
            }
    })
    
    $(".poluchaete_wrap .obmen_btns").click(function(){
        $(".poluchaete_wrap .obmen_btns").removeClass("active");
        $(this).addClass("active");
        $(".obmen_podskazki").html( $(".obmen_podskazki").attr("num_3") );
        if ( $(".obmen_podskazki").hasClass("dop_view") ) { $(".obmen_podskazki").removeClass("dop_view"); } else $(".obmen_podskazki").addClass("dop_view");
        
        $(".full_title_sender").html( $(".otdaete_wrap .obmen_btns.active").attr("full_title") );
        $(".sender_title").html( $(".otdaete_wrap .obmen_btns.active").attr("title") );
        $sender_komiss = parseFloat( $(".otdaete_wrap .obmen_btns.active").attr("komiss") );
        if ($sender_komiss!=0 && parseInt($(".otdaete_wrap .obmen_btns.active").attr("show_sender_komiss"))!=0) { $(".komiss_from_sender").parents("tr").css("display", "table-row"); } else $(".komiss_from_sender").parents("tr").css("display", "none");;
        $(".komiss_from_sender").html( $sender_komiss );
        $("input[name='send_wallet']").attr( "identificator", $(".otdaete_wrap .obmen_btns.active").attr("identificator") );
        
        $(".full_title_reciver").html( $(".poluchaete_wrap .obmen_btns.active").attr("full_title") );
        $(".reciver_title").html( $(".poluchaete_wrap .obmen_btns.active").attr("title") );
        $recive_komiss = parseFloat( $(".poluchaete_wrap .obmen_btns.active").attr("komiss") );
        if ($recive_komiss!=0 && $include_out_pay_komiss_in_course==0) { $(".komiss_from_reciver").parents("tr").css("display", "table-row"); } else $(".komiss_from_reciver").parents("tr").css("display", "none");
        $sender_need_from_pay_field = parseInt( $(".poluchaete_wrap .obmen_btns.active").attr("need_from_pay_field") );
        $(".komiss_from_reciver").html( $recive_komiss );
        $("input[name='recive_wallet']").attr( "identificator", $(".poluchaete_wrap .obmen_btns.active").attr("identificator") );
        
        $("input[name='course_num']").val( $(".poluchaete_wrap .obmen_btns.active").attr("course_num") );
        
        //Прописываем "Из кошелка", "В кошелек" и подсказочки на полях
        $(".user_actions_wrap input[name='send_wallet']").val("").attr("placeholder", $(".otdaete_wrap .obmen_btns.active").attr("for_place_holder"));
        $(".user_actions_wrap input[name='recive_wallet']").val("").attr("placeholder", $(this).attr("for_place_holder"));
        
        $(".user_actions_wrap td.from_wallet_label").html($(".otdaete_wrap .obmen_btns.active").attr("from_wallet_label"));
        $(".user_actions_wrap td.to_wallet_label").html($(this).attr("to_wallet_label"));
        
        //Для Сбера и Приват24 - делаем поля такими чтоб в них номер карты норм подставлялся (добавляем класс input_card_number)
        if ($(".otdaete_wrap .obmen_btns.active").attr("identificator")=="sberbank_rub" || $(".otdaete_wrap .obmen_btns.active").attr("identificator")=="privat24_uah") {$(".user_actions_wrap input[name='send_wallet']").addClass("input_card_number");} else $(".user_actions_wrap input[name='send_wallet']").removeClass("input_card_number");
        if ($(".poluchaete_wrap .obmen_btns.active").attr("identificator")=="sberbank_rub" || $(".poluchaete_wrap .obmen_btns.active").attr("identificator")=="privat24_uah") {$(".user_actions_wrap input[name='recive_wallet']").addClass("input_card_number");} else $(".user_actions_wrap input[name='recive_wallet']").removeClass("input_card_number");
        
        if ($(".otdaete_wrap .obmen_btns.active").attr("if_set_rekvizite_fio")=="1" && $sender_need_from_pay_field==1){
            $(".user_actions_wrap tr.fio_from_wallet_row").css("display", "table-row");
            $(".user_actions_wrap td.fio_from_wallet_label").html($(".otdaete_wrap .obmen_btns.active").attr("fio_wallet_label"));
            $(".user_actions_wrap input[name='wallet_send_fio']").val("").attr("placeholder", $(".otdaete_wrap .obmen_btns.active").attr("fio_place_holder"));
        } else {
            $(".user_actions_wrap tr.fio_from_wallet_row").css("display", "none");
            $(".user_actions_wrap input[name='wallet_send_fio']").val("");
            }
        
        if ($(this).attr("if_set_rekvizite_fio")=="1"){
            $(".user_actions_wrap tr.fio_to_wallet_row").css("display", "table-row");
            $(".user_actions_wrap td.fio_to_wallet_label").html($(this).attr("fio_wallet_label"));
            $(".user_actions_wrap input[name='wallet_recive_fio']").val("").attr("placeholder", $(this).attr("fio_place_holder"));
        } else {
            $(".user_actions_wrap tr.fio_to_wallet_row").css("display", "none");
            $(".user_actions_wrap input[name='wallet_recive_fio']").val("");
            }
            
        if ($(this).attr("hide_recive_wallet_field")=="0"){
            $(".user_actions_wrap tr.recive_wallet_row").css("display", "table-row");
        } else {
            $(".user_actions_wrap tr.recive_wallet_row").css("display", "none");
            }
            
        if ($(this).attr("if_set_rekvizit_payment_id")=="1"){
            $(".user_actions_wrap tr.payment_id_to_wallet_row").css("display", "table-row");
            $(".user_actions_wrap td.payment_id_to_wallet_label").html($(this).attr("payment_id_wallet_label"));
            $(".user_actions_wrap input[name='wallet_recive_payment_id']").val("").attr("placeholder", $(this).attr("payment_id_place_holder"));
        } else {
            $(".user_actions_wrap tr.payment_id_to_wallet_row").css("display", "none");
            $(".user_actions_wrap input[name='wallet_recive_payment_id']").val("");
            }
        
        $summ = parseFloat( $(this).attr("send_course") );
        if ($summ==1){
            $summ = parseFloat( $(this).attr("recive_course") );
            $summ = floor_ceil( $summ, "floor", 2 );
            $(".user_actions_wrap input[name='recive_summ']").val(100);
            calculate("recive_summ");
            } else {
            $summ = floor_ceil( $summ, "ceil", 2 );
            $(".user_actions_wrap input[name='send_summ']").val(100);
            calculate("send_summ");            
            }
        
        $(".user_obmen_form").slideDown(300);
        if ($sender_need_from_pay_field == 1) {$(".user_obmen_form .from_wallet_row").show(0);} else $(".user_obmen_form .from_wallet_row").hide(0).find("input").val("");
        
        //Составляем список кошельков юзера для подстановки в "Получаете"
        if ($(".user_wallets_for_choice .one_pay_sys[pay_sys_num='"+$(this).attr("pay_sys_num")+"']").length && $(".user_wallets_for_choice .one_pay_sys[pay_sys_num='"+$(this).attr("pay_sys_num")+"']").html().trim()!=""){
            $(".poluchaete_wallet_choice .wallet_for_scroll").html( $(".user_wallets_for_choice .one_pay_sys[pay_sys_num='"+$(this).attr("pay_sys_num")+"']").html() );
            $(".poluchaete_choice_strelka").css("display", "block");
        } else {
            $(".poluchaete_wallet_choice .wallet_for_scroll").html("");
            $(".poluchaete_choice_strelka").css("display", "none");
            }
    })
    
    //Клик по кнопке выбора кошельков
    $(".wallet_choice_strelka").click(function(){
        if ($(this).hasClass("active")) { $(this).removeClass("active"); } else $(this).addClass("active");
        
        $(".otdaete_wallet_choice").slideUp(0);
        $(".poluchaete_wallet_choice").slideUp(0);
        
        $wallet_choice_block = $(".otdaete_wallet_choice");
        if ($(this).hasClass("poluchaete_choice_strelka")) $wallet_choice_block = $(".poluchaete_wallet_choice");
        
        if ($(this).hasClass("active")){
            $wallet_choice_block.slideDown(0);
        } else {
            $wallet_choice_block.slideUp(0);
        }
        
        return false;
    });
    
    //Клик по кошельку из списка
    $(".one_wallet_for_choice").live("click", function(){
        $(this).parents("td").find("input").val( $(this).find("span").html() );
        
        if ( $(this).attr("rekvizit_fio") && $(this).parents("td").find("input").attr("name")=="send_wallet" && !$(this).parents("table").find(".fio_from_wallet_row").is(":hidden") ){
            $(this).parents("table").find(".fio_from_wallet_row input").val($(this).attr("rekvizit_fio"));
            }
        
        if ( $(this).attr("rekvizit_fio") && $(this).parents("td").find("input").attr("name")=="recive_wallet" && !$(this).parents("table").find(".fio_to_wallet_row").is(":hidden") ){
            $(this).parents("table").find(".fio_to_wallet_row input").val($(this).attr("rekvizit_fio"));
            }
    })
    
    $("html").click(function(){
        $(".wallet_choice_strelka").removeClass("active");
        $(".otdaete_wallet_choice").slideUp(0);
        $(".poluchaete_wallet_choice").slideUp(0);
    });
    
        //при загрузке страницы - жмем на первую кнопку "Отдаете"
    $(".otdaete_wrap .obmen_btns[num_recive='recive_3']").click();
    </script>
</noindex>   
        </div>
    </div>
    
