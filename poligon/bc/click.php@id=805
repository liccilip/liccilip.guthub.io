<script src="/assets/2f421011/jquery.js"></script>
<script src="/assets/e3d032d1/yii.js"></script>
<script src="/assets/8be6973c/js/bootstrap.js"></script>
<script src="/js/main.js?v=0.46"></script>
<script src="/assets/11e272a1/js/app.min.js"></script>
<script>jQuery(function ($) {
function checkFilter(){
	var filter1 = getCookie('filter1');
	var filter2 = getCookie('filter2');
	var send_active = getCookie('send_active');
	
	if(filter1 && filter1 != 'fc-all')
	{
		$("#gateways_send > a").hide();
		$("#gateways_send > a." + filter1).show();
		$("#filter1 > a").removeClass('fcactive');
		$("#filter1 > a[name='"+filter1+"']").addClass("fcactive");
	}
	if(filter2 && filter2 != 'fc-all')
	{
		$("#gateways_receive > a").hide();
		$("#gateways_receive > a." + filter2).show();
		$("#filter2 > a").removeClass('fcactive');
		$("#filter2 > a[name='"+filter2+"']").addClass("fcactive");
	}
}

$(document).ready(function(){
		checkFilter();
		
		$("#gateways_send > a").removeClass('active');
		var send_active = getCookie('send_active');
		
	    if(send_active)
	    {
		    $("a[id='"+send_active+"']").addClass('active');
		    $.ajax({
	           url: "https://btcstaff.ru/site/ajax-gateways-main",
	           method: "GET",
	           data: {'id': send_active},
	           beforeSend: function() {
		          $("#gataways_main_loading").show(); 
		          $("#gataways_main").addClass("box");
	           },
	           success: function(data) {
	               //alert(data);
	               $("#gateways_receive").html(data);
	               $("#gataways_main").removeClass("box");
	               $("#gataways_main_loading").hide();
	               checkFilter();
	           }
	        });
		}
	    	else $("#gateways_send > a:visible:first").addClass('active');
		
		$("#filter1 > a").click(function(){
			setCookie('filter1', $(this).attr('name'), 180);
			$("#filter1 > a").removeClass('fcactive');
			$(this).addClass("fcactive");
			
			if($(this).attr('name') == 'fc-all')
			{
				$("#gateways_send > a").show();
			}
			else
			{
				$("#gateways_send > a").hide();
				$("#gateways_send > a." + $(this).attr('name')).show();
			}
			
			$("#gateways_send > a").removeClass('active');
	        $("#gateways_send > a:visible:first").addClass('active');
	        var id = $("#gateways_send > a:visible:first").attr('id');
	        setCookie('filter1_id', id, 180);
	        setCookie('send_active', id);
	        
	        $.ajax({
	           url: "https://btcstaff.ru/site/ajax-gateways-main",
	           method: "GET",
	           data: {'id': id},
	           beforeSend: function() {
		          $("#gataways_main_loading").show(); 
		          $("#gataways_main").addClass("box");
	           },
	           success: function(data) {
	               //alert(data);
	               $("#gateways_receive").html(data);
	               $("#gataways_main").removeClass("box");
	               $("#gataways_main_loading").hide();
	               checkFilter();
	           }
	        });
	        
			return false;
		})
		
		$("#filter2 > a").click(function(){
			setCookie('filter2', $(this).attr('name'), 180);
			$("#filter2 > a").removeClass('fcactive');
			$(this).addClass("fcactive");
			
			if($(this).attr('name') == 'fc-all')
			{
				$("#gateways_receive > a").show();
				return false;
			}
			$("#gateways_receive > a").hide();
			$("#gateways_receive > a." + $(this).attr('name')).show();
			return false;
		})
		
		$("#gateways_send > a").on("click", function(){
	        $("#gateways_send > a").removeClass('active');
	        $(this).addClass('active');
	        setCookie('send_active', $(this).attr('id'), 180);
	        
	        $.ajax({
	           url: "https://btcstaff.ru/site/ajax-gateways-main",
	           method: "GET",
	           data: {'id': $(this).attr('id')},
	           beforeSend: function() {
		          $("#gataways_main_loading").show(); 
		          $("#gataways_main").addClass("box");
	           },
	           success: function(data) {
	               //alert(data);
	               $("#gateways_receive").html(data);
	               $("#gataways_main").removeClass("box");
	               $("#gataways_main_loading").hide();
	               checkFilter();
	           }
	        });
	        
	        return false;
        });
})
});</script></body>
</html>
