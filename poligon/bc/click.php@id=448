           sHTML += '<span>WMR кошелек:</span><input name="aReceive[sWMRPurse]" id="sWMRPurse" type="text" pattern="R[0-9]{12}" maxlength="13" value="" placeholder="Пример: R123456789012" required="required" />';
              if( aToCurrency.sName == 'PAYMER-WMU' )
           sHTML += '<span>WMU кошелек:</span><input name="aReceive[sWMUPurse]" id="sWMUPurse" type="text" pattern="U[0-9]{12}" maxlength="13" value="" placeholder="Пример: U123456789012" required="required" />';
              if( aToCurrency.sName == 'PAYMER-WMZ' )
           sHTML += '<span>WMZ кошелек:</span><input name="aReceive[sWMZPurse]" id="sWMZPurse" type="text" pattern="Z[0-9]{12}" maxlength="13" value="" placeholder="Пример: Z123456789012" required="required" />';
              if( aToCurrency.sName == 'PMEUR' )
           sHTML += '<span>Счет PM EUR:</span><input name="aReceive[sPMEUR]" id="sPMEUR" type="text" pattern="E[0-9]{7,8}" maxlength="9" value="" placeholder="Пример: E1234567" required="required" />';
              if( aToCurrency.sName == 'PMUSD' )
           sHTML += '<span>Счет PM USD:</span><input name="aReceive[sPMUSD]" id="sPMUSD" type="text" pattern="U[0-9]{7,8}" maxlength="9" value="" placeholder="Пример: U1234567" required="required" />';
              if( aToCurrency.sName == 'PRIVAT24UAH' )
           sHTML += '<span>Карта ПБ UAH:</span><input name="aReceive[iPBUAHCard]" id="iPBUAHCard" type="text" pattern="[0-9]{16}" maxlength="16" value="" placeholder="Пример: 1234567890123456" required="required" />';
              if( aToCurrency.sName == 'QIWIRUB' )
           sHTML += '<span>Qiwi кошелек:</span><input name="aReceive[iQiwiAccount]" id="iQiwiAccount" type="text" pattern="[0-9]{11,14}" maxlength="16" value="" placeholder="Пример: 380501234567" required="required" />';
              if( aToCurrency.sName == 'QWEUR' )
           sHTML += '<span>Qiwi кошелек:</span><input name="aReceive[iQiwiAccount]" id="iQiwiAccount" type="text" pattern="[0-9]{11,14}" maxlength="16" value="" placeholder="Пример: 380501234567" required="required" />';
              if( aToCurrency.sName == 'QWKZT' )
           sHTML += '<span>Qiwi кошелек:</span><input name="aReceive[iQiwiAccount]" id="iQiwiAccount" type="text" pattern="[0-9]{11,14}" maxlength="16" value="" placeholder="Пример: 380501234567" required="required" />';
              if( aToCurrency.sName == 'QWRUB' )
           sHTML += '<span>Qiwi кошелек:</span><input name="aReceive[iQiwiAccount]" id="iQiwiAccount" type="text" pattern="[0-9]{11,14}" maxlength="16" value="" placeholder="Пример: 380501234567" required="required" />';
              if( aToCurrency.sName == 'QWRUBCARD' )
           sHTML += '<span>Карта RUB:</span><input name="aReceive[iCardRUB]" id="iCardRUB" type="text" pattern="[0-9]{16}" maxlength="16" value="" placeholder="Пример: 1234567890123456" required="required" />';
              if( aToCurrency.sName == 'QWUSD' )
           sHTML += '<span>Qiwi кошелек:</span><input name="aReceive[iQiwiAccount]" id="iQiwiAccount" type="text" pattern="[0-9]{11,14}" maxlength="16" value="" placeholder="Пример: 380501234567" required="required" />';
              if( aToCurrency.sName == 'SBERRUB' )
           sHTML += '<span>Карта СберБанка:</span><input name="aReceive[iSberCard]" id="iSberCard" type="text" pattern="[0-9]{16,18}" maxlength="18" value="" placeholder="Пример: 4276123412341234" required="required" />';
              if( aToCurrency.sName == 'USDC' )
           sHTML += '<span>USD Coin адрес:</span><input name="aReceive[sUSDCAddress]" id="sUSDCAddress" type="text" pattern="[A-Za-z0-9]{42}" maxlength="42" value="" placeholder="0x3c2bfb058f4516b488ec8625e98aeb087085836c" required="required" />';
              if( aToCurrency.sName == 'WMB' )
           sHTML += '<span>WMB кошелек:</span><input name="aReceive[sWMBPurse]" id="sWMBPurse" type="text" pattern="B[0-9]{12}" maxlength="13" value="" placeholder="Пример: B123456789012" required="required" />';
              if( aToCurrency.sName == 'WME' )
           sHTML += '<span>WME кошелек:</span><input name="aReceive[sWMEPurse]" id="sWMEPurse" type="text" pattern="E[0-9]{12}" maxlength="13" value="" placeholder="Пример: E123456789012" required="required" />';
              if( aToCurrency.sName == 'WMG' )
           sHTML += '<span>WMG кошелек:</span><input name="aReceive[sWMGPurse]" id="sWMGPurse" type="text" pattern="G[0-9]{12}" maxlength="13" value="" placeholder="Пример: G123456789012" required="required" />';
              if( aToCurrency.sName == 'WMR' )
           sHTML += '<span>WMR кошелек:</span><input name="aReceive[sWMRPurse]" id="sWMRPurse" type="text" pattern="R[0-9]{12}" maxlength="13" value="" placeholder="Пример: R123456789012" required="required" />';
              if( aToCurrency.sName == 'WMU' )
           sHTML += '<span>WMU кошелек:</span><input name="aReceive[sWMUPurse]" id="sWMUPurse" type="text" pattern="U[0-9]{12}" maxlength="13" value="" placeholder="Пример: U123456789012" required="required" />';
              if( aToCurrency.sName == 'WMX' )
           sHTML += '<span>WMX кошелек:</span><input name="aReceive[sWMXPurse]" id="sWMXPurse" type="text" pattern="X[0-9]{12}" maxlength="13" value="" placeholder="Пример: X123456789012" required="required" />';
              if( aToCurrency.sName == 'WMZ' )
           sHTML += '<span>WMZ кошелек:</span><input name="aReceive[sWMZPurse]" id="sWMZPurse" type="text" pattern="Z[0-9]{12}" maxlength="13" value="" placeholder="Пример: Z123456789012" required="required" />';
              if( aToCurrency.sName == 'YAM' )
           sHTML += '<span>Яндекс Счет:</span><input name="aReceive[iYMAccount]" id="iYMAccount" type="text" pattern="41001[0-9]{8,11}" maxlength="16" value="" placeholder="Пример: 410011234567890" required="required" />';
              if( aToCurrency.sName == 'ZCASH' )
           sHTML += '<span>Zcash адрес:</span><input name="aReceive[sZECAddress]" id="sZECAddress" type="text" pattern="[A-Za-z0-9]{35}" maxlength="35" value="" placeholder="t1ZAsGfB2vKdRZCXXzKsLX0bsFQVD2oPT1E" required="required" />';
              if( aToCurrency.sName == 'ZRX' )
           sHTML += '<span>0x адрес:</span><input name="aReceive[sZRXAddress]" id="sZRXAddress" type="text" pattern="[A-Za-z0-9]{42}" maxlength="42" value="" placeholder="0x3c2bfb058f1516b488ec8625e98aeb129085436c" required="required" />';
       
       oPaymentDetails.html( sHTML );
    }

    /* Событие при изменении меняемой валюты */
    oFromCurrency.change( function() {
        OnChangeDirection();
        
        if( aCurrencies[$( this ).val()] === undefined ) return;
        $( this ).attr( 'class', aCurrencies[$( this ).val()].sEquivalent );
        oFromCurrencyImg.attr( 'class', 'icon '+$( this ).val() );


        /* Выбор возможных вариантов получаемой валюты */
        oToCurrency.find( 'option' ).attr( 'disabled', 'disabled' );
        for( var sDirection in aRates ) {
            var aDirection = sDirection.split( ':' );
            if( aDirection[0] == $( this ).val() )
                oToCurrency.find( 'option[value="'+aDirection[1]+'"]' ).removeAttr( 'disabled' );
        }
        oToCurrency.val( oToCurrency.find( 'option:enabled' ).attr( 'value' ) ).change();

        /* Инициализация переменных */
        var aFromCurrency = aCurrencies[oFromCurrency.val()];
        
        /* Если при обмене взымается комиссия платежной системы */
        if( aFromCurrency.sBuyFeePayer === 'client' && (aFromCurrency.dBuyFeePercent > 0 || aFromCurrency.dBuyFeeFixAmount > 0) )
            oBuyFee.parent().show();
        else
            oBuyFee.parent().hide();

        var dRate = aRates[oFromCurrency.val()+':'+oToCurrency.val()];
        oRateTag.html( dRate === undefined ? 'Выберите другое направление обмена' : 'Курс по направлению: '+(dRate < 1 ? 1 : dRate).toFixed( 4 )+' '+aCurrencies[oFromCurrency.val()].sEquivalent+' = '+(dRate < 1 ? 1 / dRate : 1).toFixed( 4 )+' '+aCurrencies[oToCurrency.val()].sEquivalent );
        if( dRate === undefined ) {
            oFromAmount.attr( 'disabled', 'disabled' );
            return;
        }
        oFromAmount.removeAttr( 'disabled', 'disabled' ).blur();
    } );

    /* Событие при изменении получаемой валюты */
    oToCurrency.change( function() {
        OnChangeDirection();
        if( aCurrencies[$( this ).val()] === undefined ) return;
        $(this).attr( 'class', aCurrencies[$( this ).val()].sEquivalent );
        oToCurrencyImg.attr( 'class', 'icon '+$( this ).val() );
        oReserveTag.html( aCurrencies[$(this).val()].dReserve > 0 ? 'Текущий резерв: <span class="'+$(this).val()+' '+aCurrencies[$(this).val()].sEquivalent+'">'+aCurrencies[$(this).val()].dReserve.toFixed( 2 )+' '+aCurrencies[$(this).val()].sEquivalent+'</span>' : '<span style="color: red">В резерве нет средств</span>' );
        var dRate = aRates[oFromCurrency.val()+':'+oToCurrency.val()];
        oRateTag.html( dRate === undefined ? 'Выберите другое направление обмена' : 'Курс по направлению: '+(dRate < 1 ? 1 : dRate).toFixed( 4 )+' '+aCurrencies[oFromCurrency.val()].sEquivalent+' = '+(dRate < 1 ? 1 / dRate : 1).toFixed( 4 )+' '+aCurrencies[oToCurrency.val()].sEquivalent );
        if( dRate === undefined ) {
            oFromAmount.attr( 'disabled', 'disabled' );
            return;
        }
        oFromAmount.removeAttr( 'disabled', 'disabled' ).blur();
        
        /* Инициализация переменных */
        var aToCurrency = aCurrencies[oToCurrency.val()];
        
        /* Если получаемая  */
        if( aToCurrency.sBilling == 'BALANCE' )
            oReserveTag.hide();
        else
            oReserveTag.show();
        
        /* Если при обмене взымается комиссия платежной системы */
        if( aToCurrency.sSaleFeePayer === 'client' && (aToCurrency.dSaleFeePercent > 0 || aToCurrency.dSaleFeeFixAmount > 0) )
            oSaleFee.parent().show();
        else
            oSaleFee.parent().hide();  
    } );

    /* Событие при изменении меняемой суммы */
    oFromAmount.bind( 'keyup blur', function() {
        var dFromAmount = Math.round( $( this ).val() * 100 ) / 100;

        /* Вывод меняемой суммы с учетом коммиссии */
        oBuyFee.html( Math.round( (dFromAmount - (-CalcTransferCommiss( oFromCurrency.val(), dFromAmount, 'Buy' ))) * 100 ) / 100 );

        oToAmount.val( aRates[oFromCurrency.val()+':'+oToCurrency.val()] === undefined ? 0 : parseInt( $(this).val() / aRates[oFromCurrency.val()+':'+oToCurrency.val()] * 100 ) / 100 );
        oSaleFee.val( oToAmount.val() - CalcTransferCommiss( oToCurrency.val(), oToAmount.val(), 'Sale' ) < 0 ? 0 : (oToAmount.val() - CalcTransferCommiss( oToCurrency.val(), oToAmount.val(), 'Sale' )).toFixed( 2 ) );
    } ).bind( 'blur', function() {
        $( this ).val( Math.round( $( this ).val() * 100 ) / 100 );
    } );

    /* Событие при изменении получаемой суммы */
    oToAmount.bind( 'keyup blur', function() {
        var dToAmount = Math.round( $( this ).val().replace( ',', '.' ) * 100 ) / 100;
        oFromAmount.val( Math.ceil( aRates[oFromCurrency.val()+':'+oToCurrency.val()] * dToAmount * 100 ) / 100 );

        /* Вывод меняемой суммы с учетом коммиссии */
        oBuyFee.html( Math.round( (oFromAmount.val() - (-CalcTransferCommiss( oFromCurrency.val(), oFromAmount.val(), 'Buy' ))) * 100 ) / 100 );

        oSaleFee.val( oToAmount.val() - CalcTransferCommiss( oToCurrency.val(), oToAmount.val(), 'Sale' ) < 0 ? 0 : (oToAmount.val() - CalcTransferCommiss( oToCurrency.val(), oToAmount.val(), 'Sale' )).toFixed( 2 ) );
    } ).bind( 'blur', function() {
        oFromAmount.keyup();
    } );
    
    /* Значения формы по умолчанию */
    oFromCurrency.val( 'YAM' ).change();
    oToCurrency.val( 'BITCOIN' ).change();
    oFromAmount.val( 1 ).blur();

    /* Событие отправки формы */
   oForm.ajaxForm( {
       'dataType' : 'json',
       'beforeSubmit' : function() {
           oForm.find( 'button' ).attr( 'disabled', 'disabled' );
           $('body').prepend('<div id="overflow-loading">Идет процес обработки. Ожидайте...</div>')},
       'success' : function( aResponse ) {
           var aStatuses = {
               'NOT_AGREE' : 'Вы не приняли соглашение',
               'EXCHANGE_LIMIT' : 'Обмен временно недоступен. Повторите попытку позже',
               'FULLNAME_NOT_IDENTICAL' : 'Ваша карта оформлена на другое ФИО',
               'sWMZPurse_IN_BLACKLIST' : 'Ваш Z кошелек в чёрном списке',
