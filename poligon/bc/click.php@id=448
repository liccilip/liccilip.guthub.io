    var oPaymentDetails = oForm.find( '#PaymentDetails' );
    function OnChangeDirection() {
        var aFromCurrency = aCurrencies[oFromCurrency.val()], aToCurrency = aCurrencies[oToCurrency.val()];
        
         oForm.find( 'img#FromCurrencyImg' ).attr( 'src', 'public/resources/icons/currencies/'+oFromCurrency.val()+'.png' );
         oForm.find( 'img#ToCurrencyImg' ).attr( 'src', 'public/resources/icons/currencies/'+oToCurrency.val()+'.png' );
		
        if( aFromCurrency === undefined || aToCurrency === undefined ) return;
	var sHTML = '<span>E-mail:</span><input name="sEmail" id="sEmail" class="email" type="text" maxlength="129" value="" placeholder="Пример: test@mail.ru" required="required" />';
       
     switch( aCurrencies[oFromCurrency.val()].sBilling+':'+aCurrencies[oToCurrency.val()].sBilling ) {
           default:
            $( '#ExchangeNotice' ).hide();
           break;

           case 'WEBMONEY:YAM':
           case 'YAM:WEBMONEY':
            $( '#ExchangeNotice' ).html( 'Внимание! Для автоматического обмена Яндекс.Деньги на Webmoney необходимо привязать  WMR кошелек вашего WMID к счету Яндекс.Денег. Обязательными условиями являются наличие в системе WebMoney аттестата не ниже формального, а также наличие идентифицированного счета в Яндекс.Деньгах. Инструкция <a href="http://wiki.webmoney.ru/projects/webmoney/wiki/Привязка_кошелька_Яндекс_Деньги" onclick="target=\'_blank\'" style="text-align: center">здесь</a>.' ).show();
           break;
          
           /*case 'YAM:WEBMONEY':
           case 'YAM:QIWI':
           case 'YAM:QW':
           case 'YAM:PRIVAT24':
           case 'YAM:SBERRUB':
           case 'YAM:PM':
           case 'YAM:BITCOIN':
            $( '#ExchangeNotice' ).html( 'Обмен проводиться с задержкой в 2 рабочих дня (ЕДИНОРАЗОВО, все остальные обмены по этим же реквизитам будут проходить МГНОВЕННО). Если Вы раньше у нас меняли в этом направлении - напишите нам и мы проведем Ваш платеж.' ).show();
           break;*/

           case 'WEBMONEY:QIWI':
           case 'QIWI:WEBMONEY':
           case 'WEBMONEY:QW':
           case 'QW:WEBMONEY':
            $( '#ExchangeNotice' ).html( 'Внимание! Для автоматического обмена QIWI на Webmoney необходимо привязать WMR кошелек вашего WMID к кошельку QIWI. Обязательными условиями являются наличие в системе WebMoney аттестата не ниже формального. Подробная инструкция <a href="http://wiki.webmoney.ru/projects/webmoney/wiki/Привязка_QIWI_кошелька" onclick="target=\'_blank\'" style="text-align: center">здесь</a>.' ).show();
