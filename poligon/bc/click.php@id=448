               'LARGE_TO_AMOUNT' : 'Максимальная получаемая сумма - '+aResponse.dMaxAmount+' '+aResponse.sEquivalent,
               'NOT_ENOUGH_RESERVE' : 'В резерве есть только '+aResponse.dReserve+' '+aResponse.sEquivalent,
               'LOW_FROM_AMOUNT' : 'Минимальная меняемая сумма - '+aResponse.dMinAmount+' '+aResponse.sEquivalent,
               'LARGE_FROM_AMOUNT' : 'Максимальная меняемая сумма - '+aResponse.dMaxAmount+' '+aResponse.sEquivalent,
               'INVALID_WMZPURSE' : 'Неверно указан WMZ кошелек',
               'INVALID_WMRPURSE' : 'Неверно указан WMR кошелек',
               'INVALID_WMEPURSE' : 'Неверно указан WME кошелек',
               'INVALID_WMUPURSE' : 'Неверно указан WMU кошелек',
               'INVALID_QIWIACCOUNT' : 'Неверный Qiwi.кошельк',
               'INVALID_YMACCOUNT' : 'Неверный счет Яндекс.Денег',
               'INVALID_PBCARD' : 'Неверная карта ПриватБанка',
               'PBCARD_NOT_EXISTS' : 'Неверная карта ПриватБанка',
               'INVALID_PMACCOUNT_USD' : 'Неверный USD счет Perfect Money',
               'INVALID_PMACCOUNT_EUR' : 'Неверный EUR счет Perfect Money',
               'PMACCOUNT_USD__NOT_EXISTS' : 'Неверный USD счет Perfect Money',
               'PMACCOUNT_EUR__NOT_EXISTS' : 'Неверный EUR счет Perfect Money',
               'INVALID_LRACCOUNT' : 'Неверный счет Liberty Reserve',
               'LRACCOUNT_NOT_EXISTS' : 'Неверный счет Liberty Reserve',
               'INVALID_ZPACCOUNT' : 'Неверный ZP кошелек',
               'ZPACCOUNT_NOT_EXISTS' : 'Неверный ZP кошелек',
               'QIWIACCOUNT_NOT_BIND' : 'Qiwi.кошелек не привязан к Вашему WMID',
               'YMACCOUNT_NOT_BIND' : 'Яндекс.кошелек не привязан к Вашему WMID',
               'FULLNAME_NOT_CORRECT' : 'Ваш WMID оформлен не на эти фамилию и имя',
               'FULLNAME_PBCARD_WMID_DIFFERENT' : 'Карта ПриватБанка оформлена не на Ваше имя',
               'WM_X19_ERROR' : 'WebMoney сообщает код №'+aResponse.iRetVal+': '+aResponse.sRetDesc,
               'WRONG_PBCARD' : 'Неверная карта ПриватБанка',
               'ERROR_CREATE_PAYMENT' : 'Не удалось выписать счет. Повторите попытку позже'
           };
           switch( aResponse.sStatus ) {
               case 'SUCCESS':
                   window.location = 'exchange.html?sID='+aResponse.sID;
               return;
           }
           $.jGrowl( aStatuses[aResponse.sStatus] === undefined ? 'Ответ сервера: '+aResponse.sStatus : aStatuses[aResponse.sStatus] );
           oForm.find( 'button' ).removeAttr( 'disabled' );
           $('#overflow-loading').remove();
       },
       'error' : function() {
           oForm.find( 'button' ).removeAttr( 'disabled' );
           $('#overflow-loading').remove();
           $.jGrowl( 'Сервер вернул неверный ответ. Повторите попытку позже.' );
       }    
   } );
} );
</script>
