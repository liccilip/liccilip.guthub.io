<input type="button" class="btn btn-primary btn-lg" onclick="get_trans();" value="Проверить"/>
<span id="nottrans" class="help-block f_red"></span>
<script>
function get_trans() {
if(Vu('extrans').value == '') {
alert('Введите ID транзакции');
Vu('extrans').focus();
return false;
}
if(Vu('extrans').value.length < 1) return;
$.ajax({
url: AJPath+'?moduleid=25&action=exchange&value='+Vu('extrans').value,
type: 'POST',
data: {'crypt':'b4d9521b8d09df16d2acb1b1c1d8ba4a78058acd34934db874d26c33231e5c9d'},
dataType: 'text'
})
.done(function(data) {
if(data != 1) $('#nottrans').html(data);
else $('#nottrans').html("Такой транзакции не существует");
});
}
</script>
  </div>
