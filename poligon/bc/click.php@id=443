<script type="text/javascript">
	function noNumbers(e)
	{
	var keynum;
	var keychar;
	var numcheck;
	if(window.event) // Блок кода для IE.
	{
	keynum = e.keyCode;
	}
	else if(e.which) // Блок кода для Netscape, Firefox и Opera.
	{
	keynum = e.which;
	}
	keychar = String.fromCharCode(keynum);
	numcheck = /\d/;
	return !numcheck.test(keychar);
	}
</script>
	
	
