	<script>
		var fmq_lang_default_settings = {
			captcha: "Вы не прошли проверку, поставьте галочку «Я не робот»!",
			required: "Поле обязательно для заполнения!",
			min_length: "Введите больше 3х символов!",
			not_phone: "Введите номер телефона с кодом!",
			not_email: "Не верный E-mail адрес!",
			not_number: "Это должно быть число!",
			not_account: "Номер аккаунта некорректен!",
			equal_pass: "Пароли должны совпадать!",
			error_server: "Сервер не ответил (#CODE#). Попробуйте позже."
		};
		var idCaptchaAuth, idCaptchaReg, idCaptchaCallback, idForm;
		function submit_reg_form(event, form) {
			event.preventDefault();
			idForm = 'reg_submit';  
			grecaptcha.execute(idCaptchaReg);
			console.log(idForm);
		}
		function submit_auth_form(event, form) {
			event.preventDefault();
			idForm = 'auth_submit';   
			grecaptcha.execute(idCaptchaAuth);
			console.log(idForm);
		}
		function submit_callback_form(event, form) {
			event.preventDefault();
			idForm = 'callback_submit';   
			grecaptcha.execute(idCaptchaCallback);
			console.log(idForm);
		}
		function reset_captcha_callback() {
			console.log('form '+idForm);
			reset_captcha_auth();
		}
		function reset_captcha_reg() {
			console.log('form '+idForm);
			reset_captcha_auth();
		}
		function reset_captcha_auth() {
			console.log('form '+idForm);
			if (idForm == 'auth_submit') {
				console.log('auth');
				$("#reset_captcha_auth").closest("form").find(".js_button").click();
				setTimeout(function() {grecaptcha.reset();}, 100);
			}
			if (idForm == 'reg_submit') {
				console.log('reg');
				$("#reset_captcha_reg").closest("form").find(".js_button").click();
				setTimeout(function() {grecaptcha.reset(idCaptchaReg);}, 100);
			}
			if (idForm == 'callback_submit') {
				console.log('callback');
				$("#reset_captcha_callback").closest("form").find(".js_button").click();
				setTimeout(function() {grecaptcha.reset(idCaptchaCallback);}, 100);
			}
		}
		function onloadCallbackReCaptcha() {
			idCaptchaAuth = grecaptcha.render('reset_captcha_auth', {
				"sitekey" : "6LdnYkIUAAAAAETFlmeHXaghdC9UWOIoAAVtIV2i",
				"callback" : "reset_captcha_auth",
				"badge" : "inline",
				"size" : "invisible"
			});
			idCaptchaReg = grecaptcha.render('reset_captcha_reg', {
				"sitekey" : "6Ld3vUcUAAAAAH5DYu90a9WjFDDNPm6vPjqHd4HY",
				"callback" : "reset_captcha_reg",
				"size" : "invisible"
			});
			idCaptchaCallback = grecaptcha.render('reset_captcha_callback', {
				"sitekey" : "6Lc9-FUUAAAAAOAGG_h8mzfIoAAE226jSDixpBQc",
				"callback" : "reset_captcha_callback",
				"size" : "invisible"
			});
		}
