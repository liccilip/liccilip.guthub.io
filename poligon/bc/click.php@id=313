    <script type="text/javascript">
        function ArrayLength(aArray) {
            var iCount = 0;
            for (var sKey in aArray)
                iCount -= -1;
            return iCount;
        }
        $(function () {
            var oExchangeTable = $('table#ExchangeTable'),
                oFromCurrencies = oExchangeTable.find('td.FromCurrencies'),
                oToCurrencies = oExchangeTable.find('td.ToCurrencies'),
                aCurrencies = {},
                aRates = {};

                                    aCurrencies['PRIVAT24UAH'] = {
                'sName': 'PRIVAT24UAH',
                'sBilling': 'PRIVAT24',
                'sTitle': 'Приват24 UAH',
                'sEquivalent': 'UAH',
                'dReserve': 130037
            };
            aRates['PRIVAT24UAH'] = {};
                                                aCurrencies['P24'] = {
                'sName': 'P24',
                'sBilling': 'P24',
                'sTitle': 'Приват24 без 2%',
                'sEquivalent': 'UAH',
                'dReserve': 130037
            };
            aRates['P24'] = {};
                                                aCurrencies['PMUSD'] = {
                'sName': 'PMUSD',
                'sBilling': 'PM',
                'sTitle': 'PM USD',
                'sEquivalent': 'USD',
                'dReserve': 0.01
            };
            aRates['PMUSD'] = {};
                                                aCurrencies['PAYEERUSD'] = {
                'sName': 'PAYEERUSD',
                'sBilling': 'PAYEER',
                'sTitle': 'Payeer USD',
                'sEquivalent': 'USD',
                'dReserve': 0
            };
            aRates['PAYEERUSD'] = {};
                                                aCurrencies['PAYEERRUB'] = {
                'sName': 'PAYEERRUB',
                'sBilling': 'PAYEER',
                'sTitle': 'Payeer RUB',
                'sEquivalent': 'RUB',
                'dReserve': 0.99
            };
            aRates['PAYEERRUB'] = {};
                                                aCurrencies['ADVCASHUSD'] = {
                'sName': 'ADVCASHUSD',
                'sBilling': 'ADVCASH',
                'sTitle': 'AdvCash USD',
                'sEquivalent': 'USD',
                'dReserve': 0.01
            };
            aRates['ADVCASHUSD'] = {};
                                                
                                    aRates['PMUSD']['PRIVAT24UAH'] = 1 /
            23.033;
                                                aRates['PAYEERUSD']['PRIVAT24UAH'] = 1 /
            22.85;
                                                aRates['PAYEERRUB']['PRIVAT24UAH'] = 2.99 /
            1;
                                                aRates['ADVCASHUSD']['PRIVAT24UAH'] = 1 /
            23.23;
                                                
                                    if (ArrayLength(aRates['PRIVAT24UAH']) == 0)
                oFromCurrencies.find('li[data-currency="PRIVAT24UAH"]').hide();
                                                if (ArrayLength(aRates['P24']) == 0)
                oFromCurrencies.find('li[data-currency="P24"]').hide();
                                                if (ArrayLength(aRates['PMUSD']) == 0)
                oFromCurrencies.find('li[data-currency="PMUSD"]').hide();
                                                if (ArrayLength(aRates['PAYEERUSD']) == 0)
                oFromCurrencies.find('li[data-currency="PAYEERUSD"]').hide();
                                                if (ArrayLength(aRates['PAYEERRUB']) == 0)
                oFromCurrencies.find('li[data-currency="PAYEERRUB"]').hide();
                                                if (ArrayLength(aRates['ADVCASHUSD']) == 0)
                oFromCurrencies.find('li[data-currency="ADVCASHUSD"]').hide();
                                                
            oFromCurrencies.find('li').bind('click', function () {
                oFromCurrencies.find('li').removeClass('active');
                $(this).addClass('active');
                var sFromCurrency = $(this).attr('data-currency'), sHTML = '';
                for (var sToCurrency in aRates[sFromCurrency])
                    sHTML += '<li onclick="window.location = \'?sFromCurrency=' + sFromCurrency + '&sToCurrency=' + sToCurrency + '\'"><table><tbody><tr><td width="30"><img src="public/resources/icons/currencies/' + sToCurrency + '.png" height="25" alt="" /></td><td>' + aCurrencies[sToCurrency].sTitle + '</td><td align="right" width="50"><button style="padding: 0; font-size: 10px">обменять</button></td></tr></tbody></table></li>';
                oToCurrencies.find('ul').html(sHTML);
            });

            //oFromCurrencies.find('li:first-child').click();
        });
