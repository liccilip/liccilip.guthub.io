            <script>
                $(".reserv_request").click(function(){
                    donotopen = 1;
                    $("#request_reserv_block").show();
                    $("#block_window").show();
                    $("#request_reserv input[name='xml_name_1']").val($(this).attr("xml_name_1"));
                    $("#request_reserv input[name='xml_name_2']").val($(this).attr("xml_name_2"));
                    $("#xml_reserv_span").text($(this).attr("xml_name_2"));
                });
                $(".one_table_row.left").click(function(){
                    $(".right_table_visible").hide();
                    $(".right_table_visible[value='"+$(this).attr("value")+"']").show();
                    $(".one_table_row.left.active").attr("class", "one_table_row one_nap_row left")
                    $(this).attr("class", "one_table_row one_nap_row left active")
                });
                $(".table_cur_one.left").click(function(){
                    $(".table_cur_one.left font").attr("class", "");
                    $(this).children("font").attr("class", "active");
                    $(".one_nap_row.left").hide();
                    var selector = "";
                    if($(this).attr("value") != "ALL"){
                        selector = "[valutcode='"+$(this).attr("value")+"']";
                    }
                    $(".one_nap_row.left"+selector).show();
                });
                $(".one_table_row.right").click(function(){
                    $(".one_table_row.right.active").attr("class", "one_table_row one_nap_row right")
                    $(this).attr("class", "one_table_row one_nap_row right active")
                });
                $(".table_cur_one.right").click(function(){
                    $(".table_cur_one.right font").attr("class", "");
                    $(this).children("font").attr("class", "active");
                    $(".one_nap_row.right").hide();
                    var selector = "";
                    if($(this).attr("value") != "ALL"){
                        selector = "[valutcode='"+$(this).attr("value")+"']";
                    }
                    $(".one_nap_row.right"+selector).show();
                });
                $("#request_reserv").submit(function(){
                    return false;
                });
                $("#close_request_reserv_button").click(function(){
                    $("#request_reserv_block").hide();
                    $("#block_window").hide();
                });
                $("#block_window").click(function(){
                    $("#request_reserv_block").hide();
                    $("#block_window").hide();
                });
                $(".one_nap_row.right").click(function(){
                    if(typeof donotopen != "undefined" && donotopen == 1){donotopen = 0;return false;}
                    $("input[name='nap_xml_value']").val($(this).attr("xmlcode"));
                    send_exchange_request("get_info", "get_user_data");
                    $("#exchange_field_block").show();
                });
            </script>
