        <script>
            var processing_send_exchange_request = 0;
            var try_processing_send_exchange_request = 0;
            function send_exchange_request(type, get_user_data){
                $("html, body").animate({ scrollTop: $("#scroll_top_block").offset().top - 50 }, 750);
                if(get_user_data == "get_user_data"){
                    uds = $.cookie("uds")
                    if(typeof uds == "undefined"){uds = {};}else{uds = JSON.parse(atob(uds));}
                    if(($("input[name='nap_xml_value']").attr("value")).split("_to_")[0]+"__from" in uds){
                        $("input[name='from_account_area']").val(eval("uds."+($("input[name='nap_xml_value']").attr("value")).split("_to_")[0]+"__from"));
                    } else {
                        $("input[name='from_account_area']").val("");
                    }
                    if(($("input[name='nap_xml_value']").attr("value")).split("_to_")[1]+"__to" in uds){
                        $("input[name='to_account_area']").val(eval("uds."+($("input[name='nap_xml_value']").attr("value")).split("_to_")[1]+"__to"));
                    } else {
                        $("input[name='to_account_area']").val("");
                    }
                    if("email" in uds){
                        $("input[name='email_area']").val(uds.email);
                    } else {
                        $("input[name='to_account_area']").val("");
                    }
                }
                processing_send_exchange_request++;
                var request_now = processing_send_exchange_request;
                $("#block_exchange_field").show();
                $("#exhange_field input").attr("class", "");
                $("#delete_double").hide();
                $("#agree_block").attr("class", "");
                $("#reserv_request_button").hide();
                $.ajax({
                    url:        "ajax_requests/send_request_exchange/",
                    type:       "post",
                    dataType:   "json",
                    data: $("#exhange_field").serialize()+"&type="+type+"&get_user_data="+get_user_data,
                    success: function(response){
                        if("to_account_big" in response){
                            $("#to_account input").attr("style", "width:350px;")
                        } else {
                            $("#to_account input").attr("style", "")
                        }
                        if(request_now != processing_send_exchange_request){
                            return false;
                        }
                        if(response.link != null){
                            window.location.href = response.link;
                            return false;
                        } else {
                            $("#exhange_field font").remove();
                            if(get_user_data == "get_user_data"){
                                if("from_account_area" in response){
                                    $("input[name='from_account_area']").val(response.from_account_area);
                                }
                                if("to_account_area" in response){
                                    $("input[name='to_account_area']").val(response.to_account_area);
                                }
                                if("email_area" in response){
                                    $("input[name='email_area']").val(response.email_area);
                                }
                            }
                            if("desc_from" in response){
                                if(response.desc_from == "no_data"){
                                    $("#error_field_from_account").hide();
                                    $("#from_account").hide();
                                } else {
                                    $("#error_field_from_account").show();
                                    $("#from_account").show();
                                    $("#from_account").prepend("<font>"+response.desc_from+" </font>");
                                    if("help_from" in response){
                                        $("#from_account input").attr("placeholder", response.help_from);
                                    }
                                }
                            }
                            if("add_field" in response){
                                if(response.add_field == "no_data"){
                                    $("#error_field_add_field").hide();
                                    $("#add_field").hide();
                                } else {
                                    $("#error_field_add_field").show();
                                    $("#add_field").show();
                                    $("#add_field input").attr("placeholder", response.add_field);
                                }
                            }
                            if("desc_to" in response){
                                $("#to_account").append("<font> "+response.desc_to+"</font>");
                            }
                            if("help_to" in response){
                                $("#to_account input").attr("placeholder", response.help_to);
                            }
                            if("min_sum" in response){
                                $("#min_sum").append("<font>"+response.min_sum+"</font>");
                            }
                            if("max_sum" in response){
                                $("#max_sum").append("<font>"+response.max_sum+"</font>");
                            }
                            if("nap_description" in response){
                                $("#exchange_field_title font").remove();
                                $("#exchange_field_title_in").append("<font>"+response.nap_description+"</font>");
                            }
                            if("from_name" in response){
                                $("#from_name").append("<font>Вы отдаёте: "+response.from_name+"</font>");
                            }
                            if("to_name" in response){
                                $("#to_name").append("<font>Вы получаете: "+response.to_name+"</font>");
                            }
                            if("summ_1" in response){
                                $("span.psys_comission").text(response.psys_comission);
                                if(response.sum_area != "summ_1" || response.refresh == 1){
                                    $("input[name='summ_1']").val(response.summ_1);
                                }
                                if(response.sum_area != "summ_2" || response.refresh == 1){
                                    $("input[name='summ_2']").val(response.summ_2);
                                }
                                if(response.sum_area != "summ_1_comission" || response.refresh == 1){
                                    $("input[name='summ_1_comission']").val(response.summ_1_comission);
                                }
                                if(response.sum_area != "summ_2_comission" || response.refresh == 1){
                                    $("input[name='summ_2_comission']").val(response.summ_2_comission);
                                }
                            }
                            if(typeof(response.error_message) != "undefined" && response.error_message != ""){
                                if("min_sum_1" in response.error_message){
                                    $("#error_field_sum_1").append("<font>"+response.error_message.min_sum_1.message+"</font>");
                                    $("input[name='summ_1']").attr("class", "border_red");
                                }
                                if("min_sum_2" in response.error_message){
                                    $("#error_field_sum_2").append("<font>"+response.error_message.min_sum_2.message+"</font>");
                                    $("input[name='summ_2']").attr("class", "border_red");
                                }
                                if("max_sum_1" in response.error_message){
                                    $("#error_field_sum_1").append("<font>"+response.error_message.max_sum_1.message+"</font>");
                                    $("input[name='summ_1']").attr("class", "border_red");
                                }
                                if("max_sum_2" in response.error_message){
                                    $("#error_field_sum_2").append("<font>"+response.error_message.max_sum_2.message+"</font>");
                                    $("input[name='summ_2']").attr("class", "border_red");
                                }
                                if("email_area" in response.error_message){
                                    $("#error_field_email").append("<font>"+response.error_message.email_area.message+"</font>");
                                    $("input[name='email_area']").attr("class", "border_red");
                                }
                                if("total_message_exchange_field" in response.error_message){
                                    $("#total_message_exchange_field").append("<font>"+response.error_message.total_message_exchange_field.message+"</font>");
                                }
                                if("double_find" in response.error_message){
                                    $("#delete_double").show();
                                }
                                if("not_agree" in response.error_message){
                                    $("#agree_block").attr("class", "bad");
                                    $("#agree_block").append("<font style='color:#ff1100'><br>"+response.error_message.not_agree.message+"</font>");
                                }
                                if("from_account_area" in response.error_message){
                                    $("#error_field_from_account").append("<font>"+response.error_message.from_account_area.message+"</font>");
                                    $("input[name='from_account_area']").attr("class", "border_red");
                                }
                                if("to_account_area" in response.error_message){
                                    $("#error_field_to_account").append("<font>"+response.error_message.to_account_area.message+"</font>");
                                    $("input[name='to_account_area']").attr("class", "border_red");
                                }
                                if("reserv_request_button" in response.error_message){
                                    $("#reserv_request_button").show();
                                }
                            }
                        }
                        try_processing_send_exchange_request = 0;
                        $("#block_exchange_field").hide();
                    },
                    error: function(){
                        if(request_now != processing_send_exchange_request){
                            return false;
                        }
                        if(try_processing_send_exchange_request < 5){
                            try_processing_send_exchange_request++;
                            setTimeout(function(){
                                send_exchange_request(type, get_user_data);
                            }, 500);
                        } else {
                            $("#block_exchange_field").hide();
                            try_processing_send_exchange_request = 0;
                            $("#total_message_exchange_field").append("<font>Ошибка соединения с сервером, пожалуйста попробуйте ещё раз.</font>");
                        }
                    },
                    timeout: 10000,
                });
            }
        </script>
        <script>
            $("#reserv_request_button").click(function(){
                $("#request_reserv_block").show();
                $("#block_window").show();
                $("#request_reserv input[name='xml_name_1']").val(($("#exhange_field input[name='nap_xml_value']").val()).split("_to_")[0]);
                $("#request_reserv input[name='xml_name_2']").val(($("#exhange_field input[name='nap_xml_value']").val()).split("_to_")[1]);
                $("#request_reserv input[name='sum']").val($("#exhange_field input[name='summ_2']").val());
                $("#request_reserv input[name='email']").val($("#exhange_field input[name='email_area']").val());
                $("#xml_reserv_span").text(($("#exhange_field input[name='nap_xml_value']").val()).split("_to_")[1]);
            });
        </script>
        <script id="update_by_key_up">
            $("[name='summ_1']").keyup(function(e) {
                update_by_key_up("summ_1", e);
            });
            $("[name='summ_2']").keyup(function(e) {
                update_by_key_up("summ_2", e);
            });
            $("[name='summ_1_comission']").keyup(function(e) {
                update_by_key_up("summ_1_comission", e);
            });
            $("[name='summ_2_comission']").keyup(function(e) {
                update_by_key_up("summ_2_comission", e);
            });
            function update_by_key_up(field, e){
                if ($("[name='"+field+"']").val().search(/^([0-9]+)$/)+1 > 0){
                    var check = 1;
                } else if ($("[name='"+field+"']").val().search(/^([0-9]+)(\.)$/)+1 > 0){
                    var check = 0;
                } else if ($("[name='"+field+"']").val().search(/^([0-9]+)(\,)$/)+1 > 0){
                    var check = 0;
                } else if ($("[name='"+field+"']").val().search(/^([0-9]+)(\.)([0-9]+)$/)+1 > 0){
                    var check = 1;
                } else if ($("[name='"+field+"']").val().search(/^([0-9]+)(\,)([0-9]+)$/)+1 > 0){
                    var check = 1;
                } else {
                    var wrong_data = $("[name='"+field+"']").val();
                    wrong_data = wrong_data.replace(/[^0-9\.\,]/, "");
                    $("[name='"+field+"']").val(wrong_data);
                    var check = 0;
                }
                if(check == 1){
                    var val_1 = $("[name='"+field+"']").val();
                    setTimeout(function(){
                        var val_2 = $("[name='"+field+"']").val()
                        if(val_1 == val_2){
                            if(e.which != 9 && e.which != 13 && e.which != 16 && e.which != 17 && e.which != 18 && e.which != 20 && e.which != 27 && e.which != 32 && e.which != 37 && e.which != 38 && e.which != 39 && e.which != 40){
                                $("input[name='sum_area']").val(""+field+"");
                                send_exchange_request("get_info", "");
                            }
                        }
                    }, 700);
                }
            }
        </script>
        <script>
            $("#submit_exchange").click(function(){
                if($("input[value='save']:checked").length > 0){
                    uds = $.cookie("uds")
                    if(typeof uds == "undefined"){uds = {};}else{uds = JSON.parse(atob(uds));}
                    if(($("input[name='from_account_area']").val()).length > 0){
                        uds[($("input[name='nap_xml_value']").val()).split("_to_")[0]+"__from"] = $("input[name='from_account_area']").val();
                    }
                    if(($("input[name='to_account_area']").val()).length > 0){
                        uds[($("input[name='nap_xml_value']").val()).split("_to_")[1]+"__to"] = $("input[name='to_account_area']").val();
                    }
                    if(($("input[name='email_area']").val()).length > 0){
                        uds["email"] = $("input[name='email_area']").val();
                    }
                    $.cookie("uds", btoa(JSON.stringify(uds)), {expires:365,path:"/",});
                }
            });
        </script>
    
