                <script>
                    autoload_personal();
                    function autoload_personal(){
                        $("#notload_personal").attr("value", "");
                        $.ajax({
                            url:      "ajax_requests/get_user_block_contacts_block/",
                            type:     "post",
                            dataType: "json",
                            success: function(response) {
                                $("#personal").empty();
                                $("#personal").append(response);
                            },
                            error: function(){
                                $("#notload_personal").attr("value", 1);
                            },
                            timeout: 5000,
                        });
                    }
                </script>
