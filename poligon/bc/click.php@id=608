            <script>
                function attentions_for_users(){
                    $.ajax({
                        url:      "ajax_requests/attentions_for_users/",
                        type:     "post",
                        dataType: "json",
                        success: function(response){
                            if(response == 1){
                                $("#attentions_for_users").show();
                            } else {
                                $("#attentions_for_users").hide();
                            }
                        },
                    });
                }
                attentions_for_users();
                setInterval(function(){
                    if(window_focus == true){
                        attentions_for_users();
                    }
                }, 5000);
            </script>
