            $(document).ready(function(){
                var ajax_home_page_autoupdate_data = 1;
                setInterval(function get_home_page_autoupdate_data() {
                    if(ajax_home_page_autoupdate_data == 1 && window_focus == true){
                        ajax_home_page_autoupdate_data = 0;
                        $.ajax({
                            url:      "home_page_data/",
                            type:     "post",
                            dataType: "text",
                            success: function(response) {
                                var user_au_data = response.split("<--split-->");
                                $("#last_exchange").empty();
                                $("#last_exchange").append(user_au_data[0]);
                                $("#valuts_reservs").empty();
                                $("#valuts_reservs").append(user_au_data[1]);
                                $("#admin_message").empty();
                                $("#admin_message").append(user_au_data[2].trim());
                                $("#reviews_wiget_data").empty();
                                $("#reviews_wiget_data").append(user_au_data[3]);
                            },
                            complete: function() {
                                ajax_home_page_autoupdate_data = 1;
                            },
                        });
                    }
                }, 5000);
            });
        </script>
    
