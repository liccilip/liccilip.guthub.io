                        <div id="request_reserv_status" style="display:none;"></div>
                        <script>
                            $("#request_reserv_form").submit(function(){
                                $("#block_loading_window_rr").show();
                                $.ajax({
                                    url:        "ajax_requests/send_request_reservs_requests/",
                                    type:       "post",
                                    dataType:   "json",
                                    data: $("#request_reserv_form").serialize(),
                                    success: function(response){
                                        $("#request_reserv_status").hide();
                                        if("status" in response){
                                            if(response.status == "success"){
                                                if("message" in response){
                                                    $("#request_reserv_status").text(response.message);
                                                    $("#request_reserv_status").show();
                                                    $("#request_reserv_status").attr("class", "status_green");
                                                }
                                            } else if(response.status == "error"){
                                                if("message" in response){
                                                    $("#request_reserv_status").text(response.message);
                                                    $("#request_reserv_status").show();
                                                    $("#request_reserv_status").attr("class", "status_red");
                                                }
                                            } else {
                                                $("#request_reserv_status").text("Ошибка соединения с сервером, пожалуйста попробуйте ещё раз.");
                                                $("#request_reserv_status").show();
                                                $("#request_reserv_status").attr("class", "status_red");
                                            }
                                        } else {
                                            $("#request_reserv_status").text("Ошибка соединения с сервером, пожалуйста попробуйте ещё раз.");
                                            $("#request_reserv_status").show();
                                            $("#request_reserv_status").attr("class", "status_red");
                                        }
                                    },
                                    complete: function(response){
                                        $("#block_loading_window_rr").hide();
                                    },
                                });
                            });
                        </script>
                    </div>
