<script type="text/javascript">
function changeBlock(element, block_id) {
	
	if ( $(element).hasClass('exchange_point_active') === false ) {
		$(element).parent().children('.exchange_point_active').removeClass().addClass('exchange_point');
		$(element).removeClass().addClass('exchange_point_active');
		$('.exchange_out_block > div:visible').hide();
		$('#'+ block_id).show();
	}
	
}
function setTime() {
	
	var TimezoneOffset = 3; // указать нужное смещение по Гринвичу 
	var localTime = new Date(); 
	var ms = localTime.getTime() + (localTime.getTimezoneOffset() * 60000) + TimezoneOffset * 3600000; 
	var time = new Date(ms);
	var hour = time.getHours();  
  	var minute = time.getMinutes();
  	
  	var hours = hour == 0 ? '00' : hour < 10 ? '0' + hour : hour;
  	var minutes = minute == 0 ? '00' : minute < 10 ? '0' + minute : minute;
  	
  	$('#cur_time').text(hours + ':' + minutes);
  	
	setTimeout(setTime, 1000);
	
}
/*$(document).ready(function() {
	$('.tooltip').tooltip({showURL: false});
});*/
</script>
