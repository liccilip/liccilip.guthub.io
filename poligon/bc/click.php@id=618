		}
		setTimeout(checkNewInput,100);
	});
}

function check_nap() {
	if($('#hexch_html') && $('#hexch_html').width() < 1) {
		setTimeout(check_nap, 1000);
	} else {
		$('.home_text_wrap').fadeOut(500);
	}
}
