<script>
var curr_class = $('.current_page_item').attr('class');
if(curr_class) {
var vkakg = $.grep(curr_class.split(" "), function(v, i){
    return v.indexOf('ion-md') === 0;
}).join();
if(vkakg)
	$('.part_logo').addClass(vkakg.substring(4));
else
	$('.part_logo').addClass('ion-md-wallet');
} else
	$('.part_logo').addClass('ion-md-wallet');
	//$('.part_logo').addClass('ion-md-zaim');

$(".spincrement").spincrement({ duration: 2000 });
if ($(window).width() >= 768) {
	$(document).on('click', '.js_exchange_link', function(){
		if(!$('#hexch_html').width() || $('#hexch_html').width() < 1) {
			var hv = $('.home_text_wrap').width();
			$('.ajax_post_bids').css('width', hv);
			$('.text').fadeOut();
			$('.home_text_wrap').fadeOut(5, function() {
