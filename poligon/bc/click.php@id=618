		setTimeout(checkNewInput,100);
		setInterval(checkInputForm,2000);
		$('.xtt4_right_col_table').removeClass('open-selector');
		change_to_mobile($(this).attr('data-direction-id'));
	});
}


// MOBILE CHANGES
function change_to_mobile(changeFromCoin = false) {
	if(changeFromCoin) {
		console.log(changeFromCoin);
		$('.selected-mobile-to').removeClass('not-selected-mobile-to');
		var placement = $(".js_exchange_link[data-direction-id='" + changeFromCoin + "'] .xtt4_one_line_name").html();
		var iconBgs = $(".js_exchange_link[data-direction-id='" + changeFromCoin + "'] .xtt4_change_ico").attr('style');
		$( ".homepage_top_pl" ).hide();
	} else {
		$('.selected-mobile-to').addClass('not-selected-mobile-to');
		var placement = 'Выберите валюту "Получаете"'; // $('.xtt4_right_col_table .active .xtt4_one_line_right .xtt4_one_line_name').html();
		var iconBgs = 'background: url(/wp-content/themes/exchanger/images/icons8-sort-down-48-2.png) no-repeat center center;';
	}
	if ( $( ".selected-mobile-to" ).length ) {
		$( ".selected-mobile-to" ).html(placement);
		$( ".selected-mobile-to" ).attr('style', iconBgs);
	} else {
		$('.xtt4_right_col_title .xtt4_table_title2').append('<div class="selected-mobile-to" style="' + iconBgs + '">' + placement + '</div>');
	}
}
function change_from_mobile(changeToCoin = false) {
	if(changeToCoin) {
		$('.selected-mobile-from').removeClass('not-selected-mobile-from');
		var placement = $(".js_item_left[data-id='" + changeToCoin + "'] .xtt4_one_line_left .xtt4_one_line_name").html();
		var iconBgs = $(".js_item_left[data-id='" + changeToCoin + "'] .xtt4_one_line_ico_left .xtt4_change_ico").attr('style');
	} else {
		if($('.xtt4_left_col_table .active .xtt4_one_line_left .xtt4_one_line_name').length) {
			$('.selected-mobile-from').removeClass('not-selected-mobile-from');
			var placement = $('.xtt4_left_col_table .active .xtt4_one_line_left .xtt4_one_line_name').html();
			var iconBgs = $('.xtt4_left_col_table .active .xtt4_one_line_left .xtt4_one_line_ico_left .xtt4_change_ico').attr('style');
		} else {
			$('.selected-mobile-from').addClass('not-selected-mobile-from');
			var placement = 'Выберите валюту "Отдаете"';
			var iconBgs = 'background: url(/wp-content/themes/exchanger/images/icons8-sort-down-48-2.png) no-repeat center center;';
		}
	}

	if ( $( ".selected-mobile-from" ).length ) {
		$( ".selected-mobile-from" ).html(placement);
		$( ".selected-mobile-from" ).attr('style', iconBgs);
	} else {
	   $('.xtt4_left_col_title .xtt4_table_title1').append('<div class="selected-mobile-from" style="' + iconBgs + '">' + placement + '</div>');
	 	 $('.selected-mobile-from').addClass('not-selected-mobile-from');
	}
	change_to_mobile();
}



// FOCUS FORM LINES
$(document).on('focusin', '.hexch_curs_line input, .hexch_pers_line input', function() {
	$(this).parents(".hexch_curs_line, .hexch_pers_line").addClass('floatinput-is-focused');
});
$(document).on('focusout', '.hexch_curs_line input, .hexch_pers_line input', function() {
	$(this).parents(".hexch_curs_line, .hexch_pers_line").removeClass('floatinput-is-focused');
});
// SPOILER AND ACTIVE FORM LINES
$(document).ready(function(){
  $('.spoiler-body').hide()
  $('.spoiler-head').click(function(){
  	$(this).toggleClass("folded").toggleClass("unfolded").next().toggle()
  })

  function updateText(event){
    var input=$(this);
    setTimeout(function(){
      var val=input.val();
      if(val!="")
        input.parents(".hexch_curs_line, .hexch_pers_line").addClass("floatinput-is-active");
      else
        input.parents(".hexch_curs_line, .hexch_pers_line").removeClass("floatinput-is-active");
    },1)
  }
  $(document).on("keydown", ".hexch_curs_line input, .hexch_pers_line input", updateText);
  $(document).on("change", ".hexch_curs_line input, .hexch_pers_line input", updateText);




	if ($(window).width() < 768) {
		var homeTextWrapH = $('.home_text_wrap').outerHeight( true );
		var xchangeTableWrapH = $('.xchange_table_wrap').outerHeight( true );
		var topheadWrapH = $('.tophead_wrap').outerHeight( true );
		// var homepageTopPlH = 100; //$('.homepage_top_pl').outerHeight( true );
		var newContactsH = $('.new-contacts').outerHeight( true );
		var newContactsGrayH = $('.new-contacts-gray').outerHeight( true );
		var topingH = window.innerHeight - (topheadWrapH+xchangeTableWrapH) - 20;
		$(".new-contacts").height(topingH);
	}




});

// CHECK INPUT LABELS
function checkNewInput(counter = 1) {
	$('.ajax_post_bids .hexch_curs_line input, .ajax_post_bids .hexch_pers_line input').each(function () {
		if($(this).val() == '') {
			var attr = $(this).attr('placeholder');
			if(typeof attr !== typeof undefined && attr !== false && attr != '') {
				$(this).parents(".hexch_curs_line, .hexch_pers_line").addClass('floatinput-is-active');
			} else {
				$(this).parents(".hexch_curs_line, .hexch_pers_line").removeClass('floatinput-is-active');
			}
		} else {
			$(this).parents(".hexch_curs_line, .hexch_pers_line").addClass('floatinput-is-active');
		}
	});
	counter++;
	var timing = 10000;
	if(counter < 40) timing = 3000;
	if(counter < 20) timing = 1000;
	if(counter < 10) timing = 500;
	if(counter < 5) timing = 200;
	setTimeout(checkNewInput,timing, counter);
}

var filterXFloat = function (value) {
    if(/^(\-|\+)?([0-9]+(\.[0-9]+)?|Infinity)$/
      .test(value))
      return Number(value).toString();
  return NaN;
}
function isNormalInteger(str) {
    var n = Math.floor(Number(str));
    return n !== Infinity && String(n) === str && n >= 0;
}

// CHECK INPUT FORM ICONS
function checkInputForm() {
	var tx = $('.js_direction_id').attr('value');
	var exchange_link_to = $(".js_exchange_link[data-direction-id='" + tx + "'] .xtt4_change_ico").attr('style');
	var exchange_type_to = $(".js_exchange_link[data-direction-id='" + tx + "']").data('type');
	var exchange_link_pid = $(".js_exchange_link[data-direction-id='" + tx + "']").parent().attr('id');
	var exchenge_id_from = exchange_link_pid.replace('js_tabnaps_', '');
	var exchange_link_from = $(".js_item_left[data-id='" + exchenge_id_from + "'] .xtt4_change_ico").attr('style');
	var sm2 = $('.js_sum2').val();
	console.log(sm2);
	console.log(isNormalInteger(filterXFloat(sm2)));
	if(exchange_type_to == 'NEO' && !isNormalInteger(filterXFloat(sm2))) {
		console.log('add');
		if(!$(".neo_errorr").length) {
			$('.hexch_div_ins').before( "<div class='neo_errorr'><h4>Внимание!</h4>Валюта NEO неделима и может передаваться только в целых числах (например, 1, 2 или 50).<br>Проверьте и введите целое число.</div>" );
		}
	} else {
		$(".neo_errorr").remove();
	}
	// console.log(exchange_link_from + ' -> ' + exchange_link_to);
	$('.js_sum1').attr('style', exchange_link_from + 'background-size: 20px; background-position: top 7px right 7px; transition: background 0s;');
	$('.js_sum2').attr('style', exchange_link_to + 'background-size: 20px; background-position: top 7px right 7px; transition: background 0s;');
}

</script>

