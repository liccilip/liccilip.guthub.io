function checkMobileHeight(cityName) {
	if ($( document ).height() > $( window ).height()) {
		$(".copyr-mobile").hide();
	} else {
		setTimeout(checkMobileHeight,1000);
	}
}

function openAdvantages(cityName) {
    var i;
    var x = document.getElementsByClassName("advantages-area");
    for (i = 0; i < x.length; i++) {
       x[i].style.display = "none";
    }
	$( ".advantages-button" ).each(function() {
	  $( this ).removeClass( "advantages-active" );
	});
	$('#punct_'+cityName).addClass('advantages-active');
    document.getElementById(cityName).style.display = "block";
}

