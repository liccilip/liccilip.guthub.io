	if (sm == 0) {
		$('.w-currency .w-give li').removeClass('w-give-active');
		$(this).addClass('w-give-active');
		incursel($(this).attr('id').substr(2));
	}
}, function () {});
$('.w-currency .w-give li').click(function() {
	sm = 1;
	$('.w-currency .w-give li').removeClass('w-give-active');
	$(this).addClass('w-give-active');
	incursel($(this).attr('id').substr(2));
});
