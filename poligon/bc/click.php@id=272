		<script>
			function getCookie(name) {
				var matches = document.cookie.match(new RegExp(
					"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
				));
				return matches ? decodeURIComponent(matches[1]) : undefined;
			}

			var notifies = [
								{
					'id' : '19',
					'title' : 'Обязательная верификация.',
					'description' : "<p><strong>Уважаемые пользователи!</strong></p>    <p><strong>В связи с участившимися попытками мошенничества наш сервис вынужден ввести верификацию клиентов при обмене с казахстанских тенге.</strong></p>    <p><strong>Для этого необходимо загрузить в личном кабинете фото паспорта (удостоверения)&nbsp; на фоне нашего сайта в соответсвующем разделе.</strong></p>    <p><strong>С заботой о Вашей безопасности.</strong></p>    <p><strong>Ваш Wmcentre.</strong></p>  ",
				},
							];

			function checkNotify(){
				while( notifies.length > 0 ) {
					var notify = notifies.shift();
					if( !getCookie('warningModal-' + notify.id)) {
						$('#warningModal h4').html(notify.title);
						$('#warningModal .modal-body').html(notify.description);
						$('#warningModal').modal();
						$('#warningModal').on('hidden.bs.modal', function (e) {
							document.cookie = "warningModal-" + notify.id + "=closed";
							checkNotify();
						});
						return
					}
				}
			}

			$(document).ready(function(){
				checkNotify();
			});
			setTimeout(function(){
				document.location.reload();;
			},600000)
		</script>
	</body>
</html>
