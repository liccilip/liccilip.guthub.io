<script> // Убираем со страниц циклические ссылки на самих себя
$(document).ready(function(){
  var links = $('a');
  var url = window.location.href;
  [].forEach.call(links, function(elem){
    if(elem.href == url) {
      elem.href = '#';
    }
  });
});
</script>
