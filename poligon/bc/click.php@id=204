<script type="text/javascript">
jQuery(function($){

var auto_load = 1;
function globalajax_timer(){

	if(auto_load == 1){
		auto_load = 0;
		
		var param = 'link=%2F%3Frid%3D10';
		$('.globalajax_ind').addClass('active');
		$.ajax({
			type: "POST",
			url: "https://www.change.am/ajax-globalajax_wp_check.html?meth=post&yid=006ef0d92a07&lang=ru",
			dataType: 'json',
			data: param,
			error: function(res, res2, res3){
				
			var res_content = res2;
		
	$(document).JsWindow('show', {
		id: 'js_error_window',
		div_class: 'error_window',
		title: 'Произошла ошибка',
		content: res_content,
		shadow: 1
	});	
	
			},			
			success: function(res)
			{		
				if(res['status'] == 'success'){
					auto_load = 1;						
									}	
				$('.globalajax_ind').removeClass('active');
			}
		});
	}

}	
	setInterval(globalajax_timer, 120000);
	globalajax_timer();
});	
</script>

