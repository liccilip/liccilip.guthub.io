jQuery(function($){ 
    $('.js_wmess_close').on('click',function(){
		
		var thet = $(this);
		var id = $(this).parents('.js_wmess').attr('id').replace('wmess_','');
		thet.addClass('active');
		
		Cookies.set("wmes"+id, 1, { expires: 7, path: '/' });
		
		$('#wmess_' + id).hide();
		thet.removeClass('active');
 
        return false;
    });
});	
