    var currencies = {"btc":"BTC (Bitcoin)","eth":"ETH (Ethereum)","xrp":"XRP (Ripple)","ltc":"LTC (Litecoin)","zec":"ZEC (Zcash)","bcn":" BCN (Bytecoin)","xlm":"XLM(Stellar)","waves":"Waves","pm":"Perfect Money USD","advcash":"Advanced Cash USD","advcash_rub":"Advanced Cash RUB","payeer":"Payeer USD","payeer_rub":"Payeer RUB","nixmoney":"NixMoney USD","yad":"\u042f\u043d\u0434\u0435\u043a\u0441.\u0414\u0435\u043d\u044c\u0433\u0438","sbr":"\u0421\u0431\u0435\u0440\u0431\u0430\u043d\u043a","vtb":"\u0412\u0422\u041124"};

    var user_wallets = [];

    function updateCurIn() {
        curin = $('select[name="OrderForm[cur_in]"]').val();
        curout = $('select[name="OrderForm[cur_out]"]').val();

        $("#orderform-cur_out").empty();

        $.each(rates, function (key, value) {
            if (value.cur_in == curin) {
                $('#orderform-cur_out').append($(new Option(currencies[value.cur_out], value.cur_out)));
            }

        });

        if (curout && curin != curout) {
            $("#orderform-cur_out").val(curout).change();
        }
        updateForm();
    }

    function updateForm() {
        curin = $('select[name="OrderForm[cur_in]"]').val();
        curout = $('select[name="OrderForm[cur_out]"]').val();

        $('#orderform-wallet_in').hide();
        $('#orderform-wallet_in-label').hide();

        $('#orderform-info-label').hide();
        $('#orderform-info').hide();

        $('#orderform-fio-label').hide();
        $('#orderform-fio').hide();

        $('#orderform-wallet_out').show();
        $('#orderform-wallet_out-label').show();

        $('#orderform-amount_in-label').html('Отдадите сумму (Минимум: ' + rates[curin + '_' + curout]['min_amount'] + ')');
        $('#orderform-amount_out-label').html('Максимум : ' + rates[curin + '_' + curout]['reserve'] + '');

        if (rates[curin + '_' + curout]['comission'] > 0 ) {
            $('#orderform-amount_out-label').html('Максимум : ' + rates[curin + '_' + curout]['reserve'] + ', Доп. комиссия: '+rates[curin + '_' + curout]['comission']);
        }

        if (rates[curin + '_' + curout]['comission_percent'] > 0 ) {
            $('#orderform-amount_out-label').html('Максимум : ' + rates[curin + '_' + curout]['reserve'] + ', Доп. комиссия: '+rates[curin + '_' + curout]['comission_percent']+' %');
        }

        if (rates[curin + '_' + curout]['comission_in'] > 0 ) {
            $('#orderform-amount_in-label').html('Минимум : ' + rates[curin + '_' + curout]['min_amount'] + ', Доп. комиссия: '+rates[curin + '_' + curout]['comission_in']);
        }

        if (rates[curin + '_' + curout]['comission_in_percent'] > 0 ) {
            $('#orderform-amount_in-label').html('Минимум : ' + rates[curin + '_' + curout]['min_amount'] + ', Доп. комиссия: '+rates[curin + '_' + curout]['comission_in_percent']+' %');
        }

        $('#orderform-wallet_out-label').html(rates[curin + '_' + curout]['wallet_out_field_name']);


        $('#rate_comments').html('');
        if (rates[curin + '_' + curout]['type'] == 'manual') {
            $('#rate_comments').html('<div class="alert alert-dismissible alert-info"><strong>Важно! Тип обмена ручной, вы подтверждаете согласие на проведение ручной операции</strong></div>');
        }

        if (curin == 'yad') {
            $('#rate_comments').append('<div class="alert alert-dismissible alert-info"><strong>Важно! Обмен проводится с задержкой от 48-72 часов. В соответствии с частью 11, 12, 13, 14 и 15 статьи 9 Федерального закона РФ о «Национальной платежной системе», вступившими в силу 1 января 2014 года (часть 4 статьи 39 данного документа), и п. 10.1.9. Соглашения об использовании сервиса «Яндекс.Деньги» прием платежей через систему Яндекс.Деньги будет проводиться с задержкой от 48-72 часов.</strong></div>');

            $('#orderform-wallet_in').show();
            $('#orderform-wallet_in-label').show();
            $('#orderform-wallet_in-label').html('Телефон, привязанный к Яндекс.Деньгам');
        }

        if (curout == 'payeer' || curout == 'payeer_rub') {
            $('#rate_comments').append('<div class="alert alert-dismissible alert-info"><strong>Учитывайте, что комиссию PAYEER берет с получателя, которая составит – 0.95%</strong></div>');
        }

        if (curin == 'wex-usd' || curin == 'wex-rub') {
            $('#orderform-wallet_in').show();
            $('#orderform-wallet_in-label').show();
            $('#orderform-wallet_in-label').html('Ваш WEX-CODE');
        }

        if (curout == 'wex-usd' || curout == 'wex-rub') {
            $('#orderform-wallet_out').show();
            $('#orderform-wallet_out-label').show();
            $('#orderform-wallet_out-label').html('Логин получателя');

        }

        if (curin == 'card' ||
            curin == 'sbr' ||
            curin == 'bank' ||
            curin == 'vtb' ||
            curin == 'alf' ||
            curin == 'kukuruza' ||
            curin == 'rs'
        ) {
            $('#orderform-fio').show();
            $('#orderform-fio-label').html('ФИО владельца счета');
            $('#orderform-fio-label').show();

            $('#orderform-wallet_in').show();
            $('#orderform-wallet_in-label').show();
            $('#orderform-wallet_in-label').html('Телефон, привязанный к карте вашего банка');
        }

        if (curout == 'card' ||
            curout == 'sbr' ||
            curout == 'bank' ||
            curout == 'vtb' ||
            curout == 'alf' ||
            curout == 'kukuruza' ||
            curout == 'rs'
        ) {
            $('#orderform-fio').show();
            $('#orderform-fio-label').html('ФИО владельца счета');
            $('#orderform-fio-label').show();
        }

        if (curin=='sbr' || curout=='sbr' || curin=='card'){
            $('#orderform-fio-label').html('ФИО + номер карты, пример: Иванов Иван Иванович 51234567890987');
        }

        if (curout == 'xmr' || curout == 'bcn' || curout == 'xmc') {
            $('#orderform-info').show();
            $('#orderform-info-label').html('Payment ID');
            $('#orderform-info-label').show();
        }

        if (curout=='xrp'){
            $('#orderform-info').show();
            $('#orderform-info-label').html('Destination tag');
            $('#orderform-info-label').show();
        }

        if (curout=='xlm'){
            $('#orderform-info').show();
            $('#orderform-info-label').html('Memo text');
            $('#orderform-info-label').show();
        }

        $('#orderform-wallet_out').val(user_wallets[curout]);

        $('#orderform-amount_in').val(rates[curin + '_' + curout]['min_amount']);
        calc();
    }

    
            
             amount = 0;
             
             function roundM(x, n) {
                var mult = Math.pow(10, n);
                return Math.round(x*mult)/mult;
             }

             function calc() {
                var curin = $('select[name="OrderForm[cur_in]"]').val();
                var curout = $('select[name="OrderForm[cur_out]"]').val();
                digit = 2;
                
                if (curout=='btc' || curout=='ltc' || curout=='xmr' || curout=='bcn' || curout=='zec' || curout=='xrp'){
                    digit = 5;
                }
                
                if (curout=='eth'){
                    digit = 4;
                }
                
                rate_value = rates[curin+'_'+curout]['value'];
                rate_value_manual = rates[curin+'_'+curout]['man_value'];
                if (rate_value_manual > 0){
                    rate_value = rate_value_manual;
                }
                
                
                // Calculation begin
                amount_in = $('#orderform-amount_in').val();
                
                comission_in = rates[curin+'_'+curout]['comission_in'];               
                comission_in_percent = rates[curin+'_'+curout]['comission_in_percent'];               
                
                if (comission_in > 0 && comission_in_percent == 0) {
                    console.log('comission_in triggered');
                    amount_in = (amount_in-comission_in).toFixed(digit);                                      
                }                
                if (comission_in_percent > 0) {
                    console.log('comission_in_percent triggered');
                    //console.log(amount_in+'*'+comission_in_percent+'/100');
                    percent_value = (amount_in * comission_in_percent)/100;
                    amount_in = (amount_in-percent_value).toFixed(digit);
                }
                
                //console.log(amount_in+'*'+rate_value);
                amount = (amount_in * rate_value).toFixed(digit);                
                
                comission = rates[curin+'_'+curout]['comission'];                
                comission_percent = rates[curin+'_'+curout]['comission_percent'];                
                
                if (comission > 0 && comission_percent == 0) {
                    console.log('comission triggered');
                    amount = (amount-comission).toFixed(digit);
                }               
                if (comission_percent > 0) {
                    console.log('comission_percent triggered');
                    //console.log(amount+'*'+comission_percent+'/100');
                    percent_value = (amount * comission_percent)/100;
                    amount = (amount-percent_value).toFixed(digit);                    
                }
                
                amount = roundM(amount,digit);                         
                $('#orderform-amount_out').val(amount);
             }
             
             function calcReverse() {
                var curin = $('select[name="OrderForm[cur_in]"]').val();
                var curout = $('select[name="OrderForm[cur_out]"]').val();
                digit = 2;
                
                if (curin=='btc' || curin=='ltc' || curin=='xmr' || curin=='bcn' || curin=='zec' || curin=='xrp'){
                    digit = 5;
                }
                
                if (curin=='eth'){
                    digit = 2;
                }
                
                rate_value = rates[curin+'_'+curout]['value'];
                rate_value_manual = rates[curin+'_'+curout]['man_value'];
                if (rate_value_manual > 0){
                    rate_value = rate_value_manual;
                }
                
                
                // Calculation begin
                amount = $('#orderform-amount_out').val();
                
                comission = rates[curin+'_'+curout]['comission'];    
                comission_percent = rates[curin+'_'+curout]['comission_percent'];            
                
                if (comission > 0 && comission_percent == 0) {
                    console.log('1) ' + amount+' + '+comission);
                    amount = (+amount + +comission).toFixed(digit);
                }
                if (comission_percent > 0) {
                    console.log('comission_percent triggered');                    
                    percent_value = (amount * comission_percent)/100;
                    amount = (+amount + +percent_value).toFixed(digit);
                }
                
                amount_in = (amount / rate_value).toFixed(digit);
                console.log('2) '+amount+' / '+rate_value);
                
                comission_in = rates[curin+'_'+curout]['comission_in'];    
                comission_in_percent = rates[curin+'_'+curout]['comission_in_percent'];
                           
                if (comission_in > 0 && comission_in_percent == 0) {
                    console.log('3) ' + amount_in+' / '+comission_in);
                    amount_in = (+amount_in + +comission_in).toFixed(digit);
                }
                
                if (comission_in_percent > 0) {
                    console.log('comission_in_percent triggered');                
                    percent_value = (amount_in * comission_in_percent)/100;
                    amount_in = (+amount_in + +percent_value).toFixed(digit);
                }
                
                $('#orderform-amount_in').val(amount_in);
                
             }           
            calc();
</script>
