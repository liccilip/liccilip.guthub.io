<script type="text/javascript">
    var cddone = 0;
    $(document).ready(function () {

        if (cddone == 0) {
            $("#Source2Id").change(sourceCurrency_OnChange);
            sourceCurrency_OnChange();
            $('#SourceAmount').blur(function () {
                $('#LastChanged').val('s');
                exch_OnChange();
            });
            $('#TargetAmount').blur(function () {
                $('#LastChanged').val('t');
                exch_OnChange();
            });
            $('#SourceAmount').keyup(function () {
                if (_exch_data) {
                    if ($('#SourceAmount').val() * 1) {
                        if (_exch_data.RateValue * 1) {
                            $('#TargetAmount').val(Math.floor(100 * $('#SourceAmount').val() / _exch_data.RateValue) / 100);
                        }
                        else {
                            $('#TargetAmount').val('');
                        }
                    }
                    else {
                        $('#TargetAmount').val('');
                    }
                }
            });

            $("input[data-val-number]").keydown(function (e) { prventNN(e); });

            $('#TargetAmount').keyup(function () {
                if (_exch_data) {
                    $('#SourceAmount').val(Math.floor(100 * $('#TargetAmount').val() * _exch_data.RateValue) / 100);
                }
            });
            $('#btn-next1').click(function () {
                exch_OnChange();
                var validator = $("form").validate();
                validator.showErrors({ field: "Error Message" })

                return _exch_valid;
            });
            $('#exchange-types-row').on("mouseup", function () {
                exch_OnChange();
            });

            $("#xform").submit(function (e) {
                var self = this;
                e.preventDefault();

                if ($('.attention').length > 0) {
                    return false;
                }
                self.submit();
            });

            Custom.init();
        }
        cddone++;
        hideTwitterBoxElements();


    });
    // Customize twitter feed
    var hideTwitterAttempts = 0;
    function hideTwitterBoxElements() {
        setTimeout(function () {
            if ($('[id*=twitter]').length) {
                $('[id*=twitter]').each(function () {
                    var ibody = $(this).contents().find('body');

                    ibody.find('.header').each(function () {
                        $(this).hide();
                    });
                });
            }
            hideTwitterAttempts++;
            if (hideTwitterAttempts < 3) {
                hideTwitterBoxElements();
            }
        }, 1500);
    }
</script>




