    //Проверка сохранена ли кука о временной зоне юзера
    $user_timezone = (-new Date().getTimezoneOffset() / 60) - +2;
    if ($.cookie('user_timezone')!=$user_timezone) $.cookie('user_timezone', $user_timezone, { expires: 365, path: '/' });
</script>

