            <div class="list_all_responces_wrap">
                <div class="modal_window_link" modal_content="make_responce_modal" dop_class="lk_modal_window make_responce_modal_window">оставить отзыв</div>
                <div id="make_responce_modal" class="to_modal_window">
    <h2 class="title_modal">Оставить отзыв</h2>
    <div class="add_obmen_responce_form_wrap">
        <div class="make_responce_modal_note">Мы ежедевно работаем над улучшением сервиса. Пожалуйста, помогите нам и оставьте отзыв</div>
        <form method="post" action="/send_order_responce" name="add_obmen_responce_form">
            Ваше имя: <input class="site_input" type="text" value="" name="name" />
            <div class="add_obmen_responce_behovator"></div>
            Ваш отзыв:
            <textarea class="site_input" name="message"></textarea><br />
            <div class="form_errors"></div>
            <div class="google_captcha_container"></div>
            <div class="add_responce_form_submit_wrap">
                <input class="fchange_button" type="button" name="send_order_responce" value="Отправить" />
            </div>
        </form>
    </div>
                                
    <div class="add_obmen_responce_sucess">
        Благодарим за Ваш отзыв - Вы нам очень помогли. От всей души желаем Вам хорошего дня!    </div>
</div>

<script src='https://www.google.com/recaptcha/api.js'></script>

<script type="text/javascript">
        /*Начало: если юзер не залогинен - выводим капчу от гугл*/
    $("[modal_content='make_responce_modal']").live("click", function(){
        setTimeout(function(){
            $(".make_responce_modal_window .google_captcha_container").html('<div id="responce_add_google_recaptha" class="g-recaptcha" data-sitekey="6LepUxoUAAAAAJ5CgpAVLBDlR5DWOyKxaLrEDlAD"></div>');
            grecaptcha.render( "responce_add_google_recaptha", {"sitekey": "6LepUxoUAAAAAJ5CgpAVLBDlR5DWOyKxaLrEDlAD"});
            
            modal_heigt_normalize();
            }, 10);
        });
    /*Конец: если юзер не залогинен - выводим капчу от гугл*/
    
    /*начало: отправляем форму отзыва*/
    var $send_order_responce_in_progress = false;
    var $form_vals;
    
    $("input[name='send_order_responce']").live("click", function(){
        if ($send_order_responce_in_progress) return;
        $send_order_responce_in_progress = true;
        
        //Считываем поля с формы
        $form_to_send = $(this).parents("form[name='add_obmen_responce_form']");
        $form_vals = $form_to_send.find("input[type='text'], input[type='hidden']").serialize();
        $form_to_send.find("textarea, select, input[type='button'], input[type='submit']").each(function(){ $form_vals = $form_vals+"&"+$(this).attr("name")+"="+$(this).attr("value");});
        
        $.ajax({ type: $form_to_send.attr('method'), url: $form_to_send.attr('action'), data: $form_vals, success: function($ajax_data){
            $ajax_data = $.parseJSON( $ajax_data );
                    
            if ($ajax_data.sucess_responce){
                $(".make_responce_modal_window .add_obmen_responce_form_wrap").slideUp(200);
                $(".make_responce_modal_window .add_obmen_responce_sucess").slideDown(200);
            } else $(".make_responce_modal_window .form_errors").html($ajax_data.validation_errors).slideDown(200);
            
            setTimeout(function(){modal_heigt_normalize()}, 50);
            setTimeout(function(){modal_heigt_normalize()}, 130);
            setTimeout(function(){modal_heigt_normalize()}, 210);
            
            $send_order_responce_in_progress = false;
            }});
        })
    /*конец: отправляем форму отзыва*/
</script>                
                <a href="/reviews" target="_blank">посмотреть все</a>
