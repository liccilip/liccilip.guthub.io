    oFromCurrencies.find( 'li' ).bind( 'click', function() {
        oFromCurrencies.find( 'li' ).removeClass( 'active' );
        $( this ).addClass( 'active' );
        var sFromCurrency = $( this ).attr( 'data-currency' ), sHTML = '';
        for( var sToCurrency in aRates[sFromCurrency] )
            sHTML += '<li onclick="window.location = \'?sFromCurrency='+sFromCurrency+'&sToCurrency='+sToCurrency+'\'"><table><tbody><tr><td width="80"><b>'+(parseInt( 1 / aRates[sFromCurrency][sToCurrency] * 10000 ) / 10000)+'</b> '+aCurrencies[sToCurrency].sEquivalent.replace( 'USD', '$' ).replace( 'RUB', '₽' ).replace( 'EUR', '&euro;' ).replace( 'UAH', 'грн.' )+'</td><td width="30"><img src="public/resources/icons/currencies/'+sToCurrency+'.png" height="25" alt="" /></td><td>'+aCurrencies[sToCurrency].sBillingTitle+' '+aCurrencies[sToCurrency].sEquivalent+'</td></tr></tbody></table></li>';
        oToCurrencies.find( 'ul' ).html( sHTML );
    } );
    
    oFromCurrencies.find( 'li:first-child' ).click();
