                  if( ArrayLength( aRates['LITECOIN'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="LITECOIN"]' ).hide();
                  if( ArrayLength( aRates['EXMORUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="EXMORUB"]' ).hide();
                  if( ArrayLength( aRates['EXMOUSD'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="EXMOUSD"]' ).hide();
                  if( ArrayLength( aRates['DASH'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="DASH"]' ).hide();
                  if( ArrayLength( aRates['ZEC'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="ZEC"]' ).hide();
                  if( ArrayLength( aRates['ADVCUSD'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="ADVCUSD"]' ).hide();
                  if( ArrayLength( aRates['QWRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="QWRUB"]' ).hide();
                  if( ArrayLength( aRates['CARDRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="CARDRUB"]' ).hide();
                  if( ArrayLength( aRates['SBERRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="SBERRUB"]' ).hide();
                  if( ArrayLength( aRates['TINKOFF'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="TINKOFF"]' ).hide();
                  if( ArrayLength( aRates['ALFARUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="ALFARUB"]' ).hide();
                  if( ArrayLength( aRates['PSBANK'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="PSBANK"]' ).hide();
                  if( ArrayLength( aRates['TBRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="TBRUB"]' ).hide();
                  if( ArrayLength( aRates['BALANCE'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="BALANCE"]' ).hide();
         
    oFromCurrencies.find( 'li' ).bind( 'click', function() {
        oFromCurrencies.find( 'li' ).removeClass( 'active' );
        $( this ).addClass( 'active' );
        var sFromCurrency = $( this ).attr( 'data-currency' ), sHTML = '';
        for( var sToCurrency in aRates[sFromCurrency] )
            if( sFromCurrency == "QWRUB" && sToCurrency == "ADVCUSD" && 0 ) {
              sHTML += '<li onclick="window.location = \'?sFromCurrency='+sFromCurrency+'&sToCurrency='+sToCurrency+'\'"><span><img class="hidden-xs" src="public/resources/icons/currencies/'+sToCurrency+'.png" alt="" /></span><span>'+aCurrencies[sToCurrency].sTitle+'</span><div class="subinfo"><small>Доступно:</small>'+57000+'</div></li>';
            }
            else {
              sHTML += '<li onclick="window.location = \'?sFromCurrency='+sFromCurrency+'&sToCurrency='+sToCurrency+'\'"><span><img class="hidden-xs" src="public/resources/icons/currencies/'+sToCurrency+'.png" alt="" /></span><span>'+aCurrencies[sToCurrency].sTitle+'</span><div class="subinfo"><small>Доступно:</small>'+aCurrencies[sToCurrency].dReserve+'</div></li>';
            }
            
//        oToCurrencies.find( 'ul' ).html( sHTML );
        oToCurrencies.html( sHTML );
    } );

    oFromCurrencies.find( 'li:first-child' ).click();
} );
</script>
