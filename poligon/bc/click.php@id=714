    <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
        /*Начало: Ajax отправка Post данных и проверка пароля*/
        $("form.top_head_login_form").submit(function(e){
            //отменяем стандартное действие при отправке формы
            e.preventDefault();
            //берем из формы метод передачи данных
            var $form_method=$(this).attr('method');
            //получаем данные, введенные пользователем в формате input1=value1&input2=value2...,
            //то есть в стандартном формате передачи данных формы
            var $form_vals=$(this).find("input").serialize();
            if ( $(this).find("input[type=submit]").length ) $form_vals = $form_vals+"&"+$(this).find("input[type=submit]").attr("name")+"=1";
            
            $.ajax({ type: $form_method, url: "/lk/check_login", data: $form_vals, success: function($ajax_data){
                $ajax_data = $.parseJSON( $ajax_data );
                
                if ($ajax_data.error!=0){
                    $(".top_head_login_form .form_errors").html($ajax_data.text).slideDown(200);
                    
                    setTimeout(function(){
                    $(".top_head_login_form .form_errors").slideUp(200);
                    }, 2000);
                } else window.location.replace($ajax_data.text);
                }});
            })
        /*Конец: Ajax отправка Post данных и проверка пароля*/
        
        $("a.modal_window_link[modal_content='remember_pass_modal_window']").click(function(e){
            setTimeout(function(){
            $.ajax({ url: "/users/remember_pass", success: function($ajax_data){
                $(".remember_pass_modal .remember_pass_ajax").html($ajax_data);
                setTimeout(modal_heigt_normalize(), 1);
                }})
            }, 1);
        });
    </script>
