<!-- /Yandex.Metrika counter  --><script type='text/javascript' src='https://obmen.money/wp-includes/js/wp-embed.min.js?ver=4.9.5'></script>
			<div id="the_shadow" class="js_techwindow"></div>
			<script>
    $(document).ready(function () {

        
        var upMode = 0; // 1 - сайт выключен, 0 - включен;

        // Проверка телефона:
        var id = "#exc_user_phone"; //"#exc_user_phone";
        var alertMessage = "Ошибка!\nВведите действующий номер телефона в формате +71234567890\nБез точек, скобок, тире и пробелов.";
        $(id).val("");
        $(id).change(function(){
            checkTel(id, upMode);
        });
        function checkTel(id, upMode){
            var telMatch = "";
            var res = false;
            var tel = $(id).val();
            if(tel && upMode === 0) {
                telMatch = tel.match(/[\+]\d+/g);
                if(telMatch) {
                    res = telMatch[0].length >= 10 && telMatch[0].length < 17 && telMatch[0].length === tel.length;
                }
            }
            if(upMode === 1){
                alertMessage = "Приём заявок временно приостановлен,\nоператор отсутствует на месте.";
                res = false;
            }
            if(!res){
                $(id).val("");
                alert(alertMessage);
                return false;
            }else{
                //console.log("Телефон введён верно: " + tel);
                return true;
            }
        }

        // Проверка ФИО:
        $("#exc_last_name").change(function(){
            checkFIO("#exc_last_name", "Ошибка!\nСлишком короткая фамилия.");
        });
        $("#exc_first_name").change(function(){
            checkFIO("#exc_first_name", "Ошибка!\nСлишком короткое имя.");
        });
        $("#exc_second_name").change(function(){
            checkFIO("#exc_second_name", "Ошибка!\nСлишком короткое отчество.");
        });
        function checkFIO(id, error){
            var res = false;
            var fio = $(id).val();
            if(fio) {
                res = fio.length >= 3;
            }
            if(!res){
                $(id).val("");
                alert(error);
                return false;
            }else{
                return true;
            }
        }

        function addBookmark() {
            if (document.all) window.external.addFavorite('https://obmen.money', 'Сайт обмена электронных валют obmen.money');
        }
        if($(".step2submit").val() === "") {
            var t = 120;
            var warn = "Внимание! Заявка устарела. Создайте новую";
            var d_begin = new Date();
            d_begin = d_begin.getTime();
            var action = $("form.ajax_post_form").attr("action");
            var hash = $("form.ajax_post_form input[name=hash]").val();
            if (!getCookie("hash") || getCookie("hash") == "undefined") {
                setCookie("hash", hash, {});
                setCookie("d_begin", d_begin, {});
            } else {
                d_begin = parseInt(getCookie("d_begin"));
            }

            $("form.ajax_post_form").attr("action", "javascript:return false;");
            $("div.stepwarning").append("<div class='bid_status'></div>");

            var d_end = new Date(); d_end = d_end.getTime();
            var ost = t - Math.round((d_end - d_begin) / 1000);
            if(ost < 0){
                $(".bid_status").removeClass("bid_new").addClass("bid_old").html(warn);
            }else {
                $(".bid_status").removeClass("bid_old").addClass("bid_new");
                start_time(ost, ".bid_status");
            }

            $(".step2submit #createzaja").click(function () {

                d_end = new Date(); d_end = d_end.getTime();

                if (Math.round((d_end - d_begin) / 1000) <= t) {
                    $("form.ajax_post_form").attr("action", action);
                    $("form.ajax_post_form").submit();
                    deleteCookie("hash");
                    deleteCookie("d_begin");
                } else {
                    $(".bid_status").removeClass("bid_new").addClass("bid_old").html(warn);
                }
            });
        }

        // https://learn.javascript.ru/cookie
        function setCookie(name, value, options) {
            options = options || {};
            var expires = options.expires;

            if (typeof expires === "number" && expires) {
                var d = new Date();
                d.setTime(d.getTime() + expires * 1000);
                expires = options.expires = d;
            }
            if (expires && expires.toUTCString) {
                options.expires = expires.toUTCString();
            }

            value = encodeURIComponent(value);
            var updatedCookie = name + "=" + value;

            for (var propName in options) {
                updatedCookie += "; " + propName;
                var propValue = options[propName];
                if (propValue !== true) {
                    updatedCookie += "=" + propValue;
                }
            }
            document.cookie = updatedCookie;
        }
        function deleteCookie(name) {
            setCookie(name, "", {
                expires: -1
            })
        }
        function getCookie(name) {
            var matches = document.cookie.match(new RegExp(
                "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
            ));
            return matches ? decodeURIComponent(matches[1]) : false;
        }
        function start_time(s, div) {
            $(div).text('Осталось 00:00 сек.');
            var this_date = new Date();

            start_time_interval = setInterval(function () {
                var new_date = new Date() - this_date;
                //var sec = s - Math.abs(Math.floor(new_date / 1000) % 60); //sek
                var sec = 60 - Math.abs(Math.floor(new_date / 1000) % 60); //sek
                var min = 1 - Math.abs(Math.floor(new_date / 1000 / 60) % 60); //min
                var hours = Math.abs(Math.floor(new_date / 1000 / 60 / 60) % 24); //hours
                if (sec.toString().length === 1) sec = '0' + sec;
                if (min.toString().length === 1) min = '0' + min;
                if (hours.toString().length === 1) hours = '0' + hours;
                //var res = 'Осталось ' + min + ':' + sec + ' сек.';
                var res = 'Осталось ' + min + " мин. " + sec + ' сек.';
                // выводим только положительные значения
                if (parseInt(min) >=0 && parseInt(min) < 2) {
                    $(div).text(res);
                } else {
                    $(div).removeClass("bid_new").addClass("bid_old").html(warn);
                }
            }, s);
        }

        // Сообщение вместо "техобслуживание":
        //$(".resultfalse").html("<img src = 'https://obmen.money/wp-content/uploads/icon-warning.png' class='h20px'> Оператор отсутствует на рабочем месте. <div>Мы заботимся о качестве предоставления наших услуг, поэтому отключаем сервис, когда не можем оперативно обработать вашу заявку. В любом случае, вы можете всегда с нами связаться. Для этого используйте телеграм <a href='https://t.me/helpobmen' target='_blank'>@helpobmen</a> и <a class='cp' onclick='jivo_api.open()'>чат</a> на сайте.</div>")
        // Добавляем div для вывода ошибок, если он еще не был добавлен:
        if($(".alertDiv").length === 0){$(".contentzone").prepend("<div class='alertDiv hidden'></div>")}
        // Меняем статус оператора в виджете:
        //if($(".resultfalse").html()){ // если статус off;
        if(upMode === 1){ // если статус off;
            $(".alertDiv").html("<img src = 'https://obmen.money/wp-content/uploads/icon-warning.png'> Оператор отсутствует на рабочем месте. <div>Мы заботимся о качестве предоставления наших услуг, поэтому отключаем сервис, когда не можем оперативно обработать вашу заявку. В любом случае, вы можете всегда с нами связаться. Для этого используйте телеграм <a href='https://t.me/helpobmen' target='_blank'>@helpobmen</a> или <a class='cp' onclick='jivo_api.open()'>чат</a> на сайте.</div>")
            $(".alertDiv").removeClass("hidden");
            $(".sidebar .statuswork_widget").removeClass("online").addClass("offline");
            $(".sidebar .statuswork_widget .opstatus").html("offline");
        } else {
            $(".alertDiv").html("").addClass("hidden");
            $(".sidebar .statuswork_widget").removeClass("offline").addClass("online");
            $(".sidebar .statuswork_widget .opstatus").html("online");
        }
    });
</script>

