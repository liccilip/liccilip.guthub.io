function check (place) {
	
	var v1 = $( "#amount_from" ).val();
	v1 = v1.replace(",", ".");
	var v1_orig = v1;
	v1 = parseFloat(v1);
	var v2 = $( "#amount_to" ).val();
	v2 = v2.replace(",", ".");
	var v2_orig = v2;
	v2 = parseFloat(v2);
	var from = $( "#cur_left").val();
	var to = $( "#cur_right").val();
	var cou = courses[from+to];
	var amount = amounts[from+to];
	var comiss = comissions[from];
	var minimum_left = minimums[from];
	var minimum_right = minimums[to];
	var name_left = names[from];
	var name_right = names[to];

	//alert(from.to);
	
	$( "#icon_name_reserve" ).html(amount);
	
	if (from == to) { show_error("Обмен одной и той же валюты самой на себя невозможен", 0, 1); return; };
	
	if (typeof cou == "undefined") { show_error("Это направление обмена временно отключено", 0, 1); $( "#icon_name_reserve" ).html(0); return; };
	
	if (place == "left") {
		v2 = Math.floor(Number(v1).toFixed(2) * cou * 100) / 100;
		if ((isNaN(v1)) || (v1 == '') || (v1 == 0)) { show_error("Введена недопустимая сумма обмена", 0, 1); return; }
	} else {
		//v1 = Math.ceil(Number(v2).toFixed(2) * (1/cou) * 100) / 100;
		v1 = Math.ceil((Number(v2).toFixed(2) / cou) * 100) / 100;
		if ((isNaN(v2)) || (v2 == '') || (v2 == 0)) { show_error("Введена недопустимая сумма обмена", 0, 1); return; }
	}
	
	var result_comiss = Math.ceil(Number(v1).toFixed(2) * comiss * 100) / 100;
	var v1_comiss = v1 + result_comiss;
	v1_comiss = Math.ceil(Number(v1_comiss).toFixed(2) * 100) / 100;
	
	
	// set
	$( "#amount_from" ).val((place == "left") ? v1_orig : v1);
	$( "#amount_to" ).val((place == "right") ? v2_orig : v2);
	$( "#icon_name_comm" ).html(v1_comiss);
	$( "#button_subsign_amount_left" ).html(v1);
	$( "#button_subsign_amount_right" ).html(v2);
	$( "#button_subsign_cur_left" ).html(name_left);
	$( "#button_subsign_cur_right" ).html(name_right);
	
	// check reserves
	if (v2 > amount) { 
		show_error("Сумма к получению превышает резерв сервиса, пожалуйста, уменьшите сумму обмена", 0, 1);
		$( "#icon_name_right" ).animate({
          backgroundColor: "#FF0000",
          color: "#FFFFFF"
        }, 1000 );
		setTimeout(function(){
		$( "#icon_name_right" ).animate({
          backgroundColor: "#F6DEAC",
          color: "#6B3D21"
        }, 1000 );
		}, 1000)
		return;
	}
	
	// check minimums
	if (v1 < minimum_left) { show_error("Сумма обмена слишком мала; пожалуйста, увеличьте её", 0, 1); return; }
	if (v2 < minimum_right) { show_error("Сумма обмена слишком мала; пожалуйста, увеличьте её", 0, 1); return; }
	
	if ( $( "#message_refresh" ).val() != "1") {
		message_hide();
	}
	button_enable();
	
}

function check_purse() {
	
	var to = $( "#cur_right").val();
	var mask = masks[to];
	var purse = $( "#req_to" ).val().trim();
	
	var temp = mask.test(purse);
	if (temp != true) {
		show_error("Неверно указан кошелёк получателя", 0, 1);
		button_disable();
		$( "#req_to" ).css("backgroundColor", "#FF0000");
		$( "#req_to" ).animate({
          backgroundColor: "#FFFFFF",
        }, 1000 );
		return;
	} else {
		var n = "purse_right_" + $( "#cur_right" ).val();
		var v = $( "#req_to" ).val().trim();
		//set_cookie (n, v);
	}
	
	check('left');
	
}

function check_inpurse() {
	
	var from = $( "#cur_left").val();
	
	if ( $( "#main_content_form_inreq_container" ).is(":visible")) {
		
		var mask = masks[from];
		var purse = $( "#req_from" ).val().trim();
		
		var temp = mask.test(purse);
		if (temp != true) {
			show_error("Неверно указан кошелёк отправителя", 0, 1);
			button_disable();
			$( "#req_from" ).css("backgroundColor", "#FF0000");
			$( "#req_from" ).animate({
			  backgroundColor: "#FFFFFF",
			}, 1000 );
			return;
		} else {
			var n = "purse_left_" + $( "#cur_left" ).val();
			var v = $( "#req_from" ).val().trim();
			//set_cookie (n, v);
		}
		
		check('left');
		
	}
	
}

function show_error(text, l1, l2) {
	
	var ll1 = (l1 == "1") ? 1 : 0;
	var ll2 = (l2 == "1") ? 1 : 0;
	$( "#message_lock" ).val(1);
	message_throw ("yellow", "Ошибка", text, ll1, ll2);
	button_disable();
	
}

function buttonclick() {
	
	check('left');
	check_purse();
	if ( $( "#main_content_form_inreq_container" ).is(":visible")) {
		check_inpurse();
	}

	
	if ($('#main_content_form_button').hasClass('pointer') == true) $( "#main_form" ).submit();
	button_disable();
	
}

/* ]]> */
</script>



<script type="text/javascript">
/* <![CDATA[ */

function toggle_action_hidden () {
	
	if ($( "#action_hidden_status" ).val() != "0") {
		$( "#action_hidden" ).slideUp();
		$( "#action_hidden_status" ).val("0");
	} else {
		$( "#action_hidden" ).slideDown();
		$( "#action_hidden_status" ).val("1");
	}

}

/* ]]> */
</script>





