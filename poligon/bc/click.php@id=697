function message_hide () {
	
	if ($( "#message_green_active" ).val() != "0") {
		$( "#message_green" ).slideUp();
		$( "#message_green_active" ).val("0");
	}
	if ($( "#message_yellow_active" ).val() != "0") {
		$( "#message_yellow" ).slideUp();
		$( "#message_yellow_active" ).val("0");
	}
	if ($( "#message_yellow_active" ).val() != "0") {
		$( "#message_yellow" ).slideUp();
		$( "#message_yellow_active" ).val("0");
	}
	
	
}

function message_throw (color, header, text, lock, block) {
	
	var llock = (lock == "1") ? "1" : "0";
	var lblock = (block == "0") ? "0" : "1";
	
	var id = "#message_" + color;
	var h_id = "#message_" + color + "_header";
	var t_id = "#message_" + color + "_text";
	var a_id = "#message_" + color + "_active";
	
	
	$( h_id ).html( header );
	$( t_id ).html( text );
	
	if ($( a_id ).val() == "0") {
		$( id ).hide();
		$( id ).slideDown ( "slow" );
	}
	
	$( a_id ).val("1");
	$( "#message_block").val(lblock);
	$( "#message_lock").val(llock);
	
}

function ajax_loader_form (exturl, target, sdata) {

	if ( $( "#message_refresh" ).val() != "1") message_hide();
	
	var target_id = "#" + target;

	if (target == "main") {
		var temp = exturl.match(/i_(.*).php/);
		var link = "/" + temp[1] + ".html";
		if (history.pushState) {
			history.pushState (null, null, link); // второй параметр - TITLE страницы
		}
		$( "#cpage" ).val(temp[1]);
		if (temp[1] == "form") {
			left_animate('left_exch', 0);
		} else {
			left_animate('left_exch', 1);
		}
	}
	
	$.ajax(
		{
			type: "POST",
			url: exturl + "?nocache=" + new Date().getTime(),
			data: sdata,
			mimeType: "multipart/form-data",
			contentType: false,
			cache: false,
			processData: false,
			beforeSend: function( data ) {
				//$( target_id ).hide();
				
			}
		}
	)
	.done(function( data ) {
		$( target_id ).fadeOut( 0 );
		$( target_id ).html( data );
		$( target_id ).fadeIn( 500 );
	}
	)
	.fail(function( xhr, ajaxOptions, thrownError ) {
		message_throw ("red", "Ошибка!", "Произошла ошибка загрузки информации, повторите попытку позднее (" + xhr.status + " / " + thrownError + ")");
	}
	)
	;

}

function ajax_loader_plain (exturl, target, sdata) {

	if ( $( "#message_refresh" ).val() != "1") message_hide();
	
	var target_id = "#" + target;

	if (target == "main") {
		var temp = exturl.match(/i_(.*).php/);
		var link = "/" + temp[1] + ".html";
		if (sdata) link = link + "?" + sdata;
		if (history.pushState) {
			history.pushState (null, null, link); // второй параметр - TITLE страницы
		}
		$( "#cpage" ).val(temp[1]);
		if (temp[1] == "form") {
			left_animate('left_exch', 0);
		} else {
			left_animate('left_exch', 1);
		}
		/*
		if (temp[1][0] == "w") {
			var t = temp.match(/w_(.*)/);
			exturl = "/wiki/" + t[1];
		}
		*/
	}
	
	$.ajax(
		{
			type: "POST",
			url: exturl + "?nocache=" + new Date().getTime(),
			data: {
				plaindata : sdata
			},
			cache: false,
			beforeSend: function( data ) {
				//$( target_id ).hide();
				
			}
		}
	)
	.done(function( data ) {
		$( target_id ).fadeOut( 0 );
		$( target_id ).html( data );
		$( target_id ).fadeIn( 500 );
	}
	)
	.fail(function( xhr, ajaxOptions, thrownError ) {
		message_throw ("red", "Ошибка!", "Произошла ошибка загрузки информации, повторите попытку позднее (" + xhr.status + " / " + thrownError + ")");
	}
	)
	;

}

function set_cookie(cname, cvalue) {
	
    var d = new Date();
    d.setTime(d.getTime() + (365*24*60*60*1000));
    var expires = "expires=" + d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires;
	
}

function left_animate(div, type) {
	
	var n = "#" + div;
	
	if (type == 0) { // hide
		$( n ).animate({width: "0px"}, 500);
		if (div == "left_help") {
			$( "#help_append" ).css("display", "block");
		}
	} else { // show
		if (div == "left_help") {
			set_cookie ("donothelp", "0");
			$( "#help_append" ).css("display", "none");
		}
		if (div != "left_exch") $( '#help_text' ).html("ПОМОЩЬ");
		$( n ).animate({width: "110px"}, 500);
	}
	
}

