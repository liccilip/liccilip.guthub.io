<script type="text/javascript">
/* <![CDATA[ */


// listeners
$( "#body" ).on( "mousedown", function( event ) {
	if ($( "#message_lock" ).val() == "0") {
		if ($( "#message_block" ).val() == "1") {
			$( "#message_block" ).val("0") ;
		} else {
			if (
				(document.getElementById('message_green').style.display != "none") ||
				(document.getElementById('message_yellow').style.display != "none") ||
				(document.getElementById('message_red').style.display != "none")
			) message_hide();
		}
	}
});
$( "#body" ).on( "keyup", function( event ) {
	if ($( "#message_lock" ).val() == "0") {
		if ($( "#message_block" ).val() == "1") {
			$( "#message_block" ).val("0") ;
		} else {
			if (
				(document.getElementById('message_green').style.display != "none") ||
				(document.getElementById('message_yellow').style.display != "none") ||
				(document.getElementById('message_red').style.display != "none")
			) message_hide();
		}
	}
});

function controller () {
	
	ajax_loader_plain("i_controller.php", "controller", "");
	
}

controller();
setInterval("controller()", 30000);

left_animate('left_help', 1);
/* ]]> */
</script>

