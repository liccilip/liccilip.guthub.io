var WMR_up = 'WebMoney';var WMR_down = 'WMR';var WMR_name = 'WebMoney WMR';var WMU_up = 'WebMoney';var WMU_down = 'WMU';var WMU_name = 'WebMoney WMU';var WMZ_up = 'WebMoney';var WMZ_down = 'WMZ';var WMZ_name = 'WebMoney WMZ';var WME_up = 'WebMoney';var WME_down = 'WME';var WME_name = 'WebMoney WME';var WMX_up = 'WebMoney';var WMX_down = 'WMX';var WMX_name = 'WebMoney WMX';var WMH_up = 'WebMoney';var WMH_down = 'WMH';var WMH_name = 'WebMoney WMH';var WMG_up = 'WebMoney';var WMG_down = 'WMG';var WMG_name = 'WebMoney WMG';var WMK_up = 'WebMoney';var WMK_down = 'WMK';var WMK_name = 'WebMoney WMK';var YAD_up = 'Яндекс';var YAD_down = 'Деньги';var YAD_name = 'Яндекс.Деньги';var cookies = {};
function button_disable () {
	
	$( "#main_content_form_button" ).fadeTo(300, 0.5);
	$( "#main_content_form_button_subsign" ).fadeOut(200);
	$( "#main_content_form_button" ).removeClass( "pointer" );
	
}

function button_enable () {
	
	message_hide();
	$( "#main_content_form_button" ).fadeTo(300, 1);
	$( "#main_content_form_button_subsign" ).fadeIn(200);
	$( "#main_content_form_button" ).addClass( "pointer" );
	
}

function show_c_selector ( place ) {
	
	set_selectable (place, 0);
	
	var name = "#" + "form_selector_container_" + place;

	var options = {};
	//options = { direction: place, duration: 500 };
	options = { direction: "left", duration: 500 };
	
	$( name ).show ("drop", options);
	
	setTimeout(function(){set_selectable( place, 1 );}, 400)

	
	name = "#main_content_form_input_" + place;
	
	$( name ).fadeOut( 300 );

	$( "#main_content_form_req_container" ).fadeOut( 300 );
	
	button_disable();
	
}

function set_selectable (place, val) {
	
	$( "#selectable_" + place ).val( val );
	
}

function c_select (place, code, donotcheck) {
	
	donotcheck = donotcheck || 0;
	
	$( "#icon_up_" + place ).html ( this[code + "_up"] );
	$( "#icon_down_" + place ).html ( this[code + "_down"] );
	//$( "#icon_name_" + place ).html ( this[code + "_name"] );
	$( "#cur_" + place ).val ( code );
	
	document.getElementById('icon_' + place + '_pic').style.background = "url(/images/cur_" + code + ".png) center no-repeat";
	
	
	// hide form
	var name = "#" + "form_selector_container_" + place;
	//var dir = (place == "left") ? "right" : "left"; // direction for hide
	var dir = "right";
	var options = {};
	options = { direction: dir };
	$( name ).hide ("drop", options, 500);
	
	
	// show sum form
	name = "#main_content_form_input_" + place;
	$( name ).fadeIn( 300 );
	
	
	// show destination purse
	$( "#main_content_form_req_container" ).fadeIn( 300 );
	
	set_purse(code, place);
	
	if (donotcheck == 0) {
		check('left');
	}

	if (place == "left") {
		if (code == "YAD") {
			$( "#main_content_form_inreq_container" ).fadeIn( 600 );
		} else {
			$( "#main_content_form_inreq_container" ).hide();
		}
	}
	
	//set_cookie (place, code);
	
	//var n = (place == "left") ? "from" : "to";
	//$( "#amount_" + n )[0].focus();
	
}

function set_purse (code, place) {
		
		var target = (place == "left") ? "#req_from" : "#req_to";
		var n ='purse_' + code;
		//var matches = document.cookie.match(new RegExp("(?:^|; )" + n.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"));
		//if (matches) {
			if (n in cookies) {
				//var p = decodeURIComponent(matches[1]);
				var p = cookies[n];
			} else {
				var p = '';
			}
		$( target ).val(p);
		$( target ).css('color', '#00FF00');
		$( target ).animate({
		  color: "#000000"
		}, 1000 );
	
}

function hover_toggle ( o, x, place ) {
	
	if ($( "#selectable_" + place ).val() == 1) {
		o.style.background = "url(/images/sprites_h.png) -" + x + "px -1077px no-repeat";
	}
	
}

