        aRates['TCSBRUB']['EXMUSD'] = 64.8634 / 1;
        aRates['TCSBRUB']['EXMRUB'] = 1.04 / 1;
        aRates['TCSBRUB']['SBERRUB'] = 1.04 / 1;
         
        if( ArrayLength( aRates['QWRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="QWRUB"]' ).hide();
        if( ArrayLength( aRates['YAMRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="YAMRUB"]' ).hide();
        if( ArrayLength( aRates['PMUSD'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="PMUSD"]' ).hide();
        if( ArrayLength( aRates['ADVCUSD'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="ADVCUSD"]' ).hide();
        if( ArrayLength( aRates['ADVCRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="ADVCRUB"]' ).hide();
        if( ArrayLength( aRates['ADVCEUR'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="ADVCEUR"]' ).hide();
        if( ArrayLength( aRates['EXMUSD'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="EXMUSD"]' ).hide();
        if( ArrayLength( aRates['EXMRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="EXMRUB"]' ).hide();
        if( ArrayLength( aRates['SBERRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="SBERRUB"]' ).hide();
        if( ArrayLength( aRates['TBRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="TBRUB"]' ).hide();
        if( ArrayLength( aRates['TCSBRUB'] ) == 0 )
        oFromCurrencies.find( 'li[data-currency="TCSBRUB"]' ).hide();
    
    oFromCurrencies.find( 'li' ).bind( 'click', function() {
        oFromCurrencies.find( 'li' ).removeClass( 'active' );
        $( this ).addClass( 'active' );
        var sFromCurrency = $( this ).attr( 'data-currency' ), sHTML = '';
        for( var sToCurrency in aRates[sFromCurrency] )
            sHTML += '<li onclick="window.location = \'?sFromCurrency='+sFromCurrency+'&sToCurrency='+sToCurrency+'\'"><table><tbody><tr><td width="80"><b>'+(parseInt( 1 / aRates[sFromCurrency][sToCurrency] * 10000 ) / 10000)+'</b> '+aCurrencies[sToCurrency].sEquivalent.replace( 'USD', '$' ).replace( 'RUB', '₽' ).replace( 'EUR', '&euro;' ).replace( 'UAH', 'грн.' )+'</td><td width="30"><img src="public/resources/icons/currencies/'+sToCurrency+'.png" height="25" alt="" /></td><td>'+aCurrencies[sToCurrency].sBillingTitle+' '+aCurrencies[sToCurrency].sEquivalent+'</td></tr></tbody></table></li>';
        oToCurrencies.find( 'ul' ).html( sHTML );
    } );
    
    oFromCurrencies.find( 'li:first-child' ).click();
} );
</script>
