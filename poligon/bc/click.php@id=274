<script>
document.addEventListener("DOMContentLoaded", function () {
  $(".restore-btn").on("click", function(){
    var o = null;
    $(".lpems,.lpeme").hide();
    o = VR.readForm($(this).data('form'));
    if($("#lostpass-recaptcha").length){
      o["g-recaptcha-response"] = $("#lostpass-recaptcha .g-recaptcha-response").val();
    }

    return Query(o, function(data){
      VR.recaptcha_reset("lostpass-recaptcha");
      if(typeof data == 'object'){
        switch(data.status){
          case "error":
            $(".lpeme").show().text(data.msg);
            break;
          default:
            $(".lpems").show().text(data.msg);
            if(o.action == 'restore-s3')
              setTimeout(function(){
                window.location.href = '/';
              }, 1000);
            break;
        }
      }

    }, {url : '/lostpass/'});
  });
})
</script>
