				var fq_recaptcha_list 	= {};
				
				var fq_recaptcha_cb 	= function(response) {};
				var fq_recaptcha_key 	= "6Lfawl0UAAAAAI843T67k7GBBfQYyjABzErvCxC7";
				var fq_recaptcha_inv_key= "6Ldfxl0UAAAAAA0Nz7zqBT8fMYiUyn1PuS8jRAml";
				var onloadCallback 		= function() {
					$(".js_fq_recaptcha").each(function() {
						var $this = this;
						
						if(!$($this).hasClass("js_sleep"))
							recaptchaInit(this);
					});
				};
				
				function recaptchaInit($element) {
					var id 			= $($element).attr("id");
					var theme 		= $($element).data("theme");
					var size 		= $($element).data("size");
					var custom_cb 	= $($element).data("callback");
					
					if(id) {
						if(custom_cb)
							custom_cb = window[custom_cb];
						
						fq_recaptcha_list[id] = grecaptcha.render(id, {
							"sitekey": (size === 'invisible' ? fq_recaptcha_inv_key : fq_recaptcha_key),
							"callback": (custom_cb ? custom_cb : fq_recaptcha_cb),
							"theme": theme,
							"size": size
						});
					}
				}
			</script>
		
