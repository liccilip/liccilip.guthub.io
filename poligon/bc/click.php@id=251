    var oPaymentDetails = oForm.find( '#PaymentDetails' );
    function OnChangeDirection() {
        var aFromCurrency = aCurrencies[oFromCurrency.val()], aToCurrency = aCurrencies[oToCurrency.val()];
        
         oForm.find( 'img#FromCurrencyImg' ).attr( 'src', 'public/resources/icons/currencies/'+oFromCurrency.val()+'.png' );
         oForm.find( 'img#ToCurrencyImg' ).attr( 'src', 'public/resources/icons/currencies/'+oToCurrency.val()+'.png' );
		
        if( aFromCurrency === undefined || aToCurrency === undefined ) return;
	var sHTML = '<div class="m-input clr"><label for="sEmail">E-mail:</label><input name="sEmail" id="sEmail" class="email" type="text" maxlength="129" value="" placeholder="Пример: test@mail.ru" required="required" /></div>';
       
     switch( aCurrencies[oFromCurrency.val()].sBilling+':'+aCurrencies[oToCurrency.val()].sBilling ) {
           default:
            $( '#ExchangeNotice' ).hide();
           break;


           case 'PRIVAT24:YAM':
           case 'PRIVAT24:PM':
           case 'PRIVAT24:OKPAY':
         /*  case 'PRIVAT24:BITCOIN':*/
           case 'PRIVAT24:BTCE':
           case 'PRIVAT24:HELIX':
           case 'PRIVAT24:ADVCASH':
           case 'PRIVAT24:PAYEER':
           case 'PRIVAT24:COINWALLET':
           case 'PRIVAT24:BLOCKIO':
           case 'PRIVAT24:QW':
           case 'PRIVAT24:QIWI':
           case 'PRIVAT24:SBERRUB':

            $( '#ExchangeNotice' ).html( 'Внимание! Возможен обмен в ручном режиме без 2% комиссии Приват24.<br>Для осуществления обмена без комиссии, просьба заполнить реквизиты заявки и Перейти к платежу' ).show();
           break;

           case 'WEBMONEY:YAM':
           case 'YAM:WEBMONEY':
            $( '#ExchangeNotice' ).html( 'Внимание! Для автоматического обмена Яндекс.Деньги на Webmoney необходимо привязать  WMR кошелек вашего WMID к счету Яндекс.Денег. Обязательными условиями являются наличие в системе WebMoney аттестата не ниже формального, а также наличие идентифицированного счета в Яндекс.Деньгах. Инструкция <a href="http://wiki.webmoney.ru/projects/webmoney/wiki/Привязка_кошелька_Яндекс_Деньги" onclick="target=\'_blank\'" style="text-align: center">здесь</a>.' ).show();
           break;
          


           case 'WEBMONEY:QIWI':
           case 'QIWI:WEBMONEY':
           case 'WEBMONEY:QW':
           case 'QW:WEBMONEY':
            $( '#ExchangeNotice' ).html( 'Внимание! Для автоматического обмена QIWI на Webmoney необходимо привязать WMR кошелек вашего WMID к кошельку QIWI. Обязательными условиями являются наличие в системе WebMoney аттестата не ниже формального. Подробная инструкция <a href="http://wiki.webmoney.ru/projects/webmoney/wiki/Привязка_QIWI_кошелька" onclick="target=\'_blank\'" style="text-align: center">здесь</a>.' ).show();
           break;

           case 'WEBMONEY:PRIVAT24':
           case 'PRIVAT24:WEBMONEY':
            $( '#ExchangeNotice' ).html( 'Внимание! Для автоматического обмена Приват24 на Webmoney или в обратном направлении необходимо чтоб ФИО в аттестате Вашего WMID было равно ФИО в Приват24. Обязательными условиями являются наличие в системе WebMoney аттестата не ниже формального с загруженной сканкопией паспорта. Инструкция <a href="http://wiki.webmoney.ru/projects/webmoney/wiki/Загрузка_документов_в_Центр_аттестации" onclick="target=\'_blank\'" style="text-align: center">здесь</a>.' ).show();
           break;
           
            /*case 'PRIVAT24:ETHEREUM':
           case 'YAM:ETHEREUM':
            $( '#ExchangeNotice' ).html( 'Внимание! Сейчас наблюдаются зависание транзакций. Если вы не готовы ждать от 30 мин до 1 часа, а в некоторых случаях и несколько часов - не оформяйте заявку' ).show();
           break; */
           
           /* case 'PRIVAT24:BITCOIN':
