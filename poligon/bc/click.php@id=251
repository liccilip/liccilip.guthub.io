    function CalcTransferCommiss( sCurrency, dAmount, sBuyOrSale ) {
        if( aCurrencies[sCurrency] === undefined ) return;
        var dFeePercent = sBuyOrSale === 'Sale' ? aCurrencies[sCurrency].dSaleFeePercent : aCurrencies[sCurrency].dBuyFeePercent,
            sFeeRound = sBuyOrSale === 'Sale' ? aCurrencies[sCurrency].sSaleFeeRound : aCurrencies[sCurrency].sBuyFeeRound,
            dFeeMinAmount = sBuyOrSale === 'Sale' ? aCurrencies[sCurrency].dSaleFeeMinAmount : aCurrencies[sCurrency].dBuyFeeMinAmount,
            dFeeMaxAmount = sBuyOrSale === 'Sale' ? aCurrencies[sCurrency].dSaleFeeMaxAmount : aCurrencies[sCurrency].dBuyFeeMaxAmount,
            dFeeFixAmount = sBuyOrSale === 'Sale' ? aCurrencies[sCurrency].dSaleFeeFixAmount : aCurrencies[sCurrency].dBuyFeeFixAmount;

        var dCommiss = dAmount * dFeePercent / 100;
        if( sFeeRound !== '' )
            dCommiss = Math[sFeeRound]( dCommiss * 100 ) / 100;
        if( dCommiss < dFeeMinAmount )
            dCommiss = dFeeMinAmount;
        else if( dFeeMaxAmount > 0 && dCommiss > dFeeMaxAmount )
            dCommiss = dFeeMaxAmount;
        dCommiss -= dFeeFixAmount * (-1);

        return dCommiss;
    }

