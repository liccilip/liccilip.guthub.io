           break; */
           
           case 'WEBMONEY:SBERRUB':
           case 'SBERRUB:WEBMONEY':
            $( '#ExchangeNotice' ).html( 'Внимание! Для автоматического обмена СберБанк на Webmoney или в обратном направлении необходимо чтоб ФИО в аттестате Вашего WMID было равно ФИО в СберБанке. Обязательными условиями являются наличие в системе WebMoney аттестата не ниже формального с загруженной сканкопией паспорта. Подробная инструкция <a href="http://wiki.webmoney.ru/projects/webmoney/wiki/Загрузка_документов_в_Центр_аттестации" onclick="target=\'_blank\'" style="text-align: center">здесь</a>.' ).show();
           break;
       }
       
              
       /*                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     if( aFromCurrency.sName == 'BALANCE' && aToCurrency.sName == 'PMUSD' )
            $( '#ExchangeNotice' ).html( 'Обмен проводится в полуавтоматическом режиме.' ).show();
                               if( aFromCurrency.sName == 'BALANCE' && aToCurrency.sName == 'PAYEERUSD' )
            $( '#ExchangeNotice' ).html( 'Обмен проводится в полуавтоматическом режиме.' ).show();
                               if( aFromCurrency.sName == 'BALANCE' && aToCurrency.sName == 'ADVCASHUSD' )
            $( '#ExchangeNotice' ).html( 'Обмен проводится в полуавтоматическом режиме.' ).show();
               */
       
       /* В зависимости от направления обмена */
              if( aFromCurrency.sName+':'+aToCurrency.sName == 'WMZ:QWRUB' )
           sHTML += '<div class="m-input clr"><label for="sWMZPurse">WMZ кошелек:</label></td><td><input name="aSource[sWMZPurse]" id="sWMZPurse" type="text" pattern="Z[0-9]{12}" maxlength="13" value="" placeholder="Пример: Z123456789012" required="required" /></div>';
              if( aFromCurrency.sName+':'+aToCurrency.sName == 'WMR:QWRUB' )
           sHTML += '<div class="m-input clr"><label for="sWMRPurse">WMR кошелек:</label></td><td><input name="aSource[sWMRPurse]" id="sWMRPurse" type="text" pattern="R[0-9]{12}" maxlength="13" value="" placeholder="Пример: R123456789012" required="required" /></div>';
