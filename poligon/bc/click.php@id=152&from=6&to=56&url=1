Number.prototype.round=function(base) {
	var string=this.toString();
	var point_index=string.indexOf('.');
	if ((string.length-point_index)>7) {
		return (Math.round((this+0.0000001)*100)/100).toFixed(2);
	} else
		return (Math.round(this*100)/100).toFixed(2);
}
function r2(value) {
	return value.round(2).replace(/\.00/g, '');
}
function r6(value) {
	return value.toFixed(6);
}
function p100(p) {
	return (100-p)/100;
}
function set_text(str) {
	return document.getElementById('cs2').innerHTML=str;
}
