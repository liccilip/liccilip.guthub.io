				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in));
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%='+fm.a2.value+' грн';
			set_text(wmu_uah);
		}
		if (gs('usd2')) { // wmu - $
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_wmu_in)/usd_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in)*usd_out);
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%/'+usd_out+'='+fm.a2.value+' $';
			set_text(wmu_usd);
		}
		if (gs('eur2')) { // wmu - €
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_wmu_in)/wme_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in)*wme_out);
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%/'+wme_out+'='+fm.a2.value+' €';
			set_text(wmu_eur);
		}
		if (gs('rur2')) { // wmu - руб
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_wmu_in)/wmr_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in)*wmr_out);
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%/'+wmr_out+'='+fm.a2.value+' руб';
			set_text(wmu_rur);
		}
		if (gs('wmz2')) { // wmu - wmz
			fm.calcstr.value=swm;
		}
		if (gs('wme2')) { // wmu - wme
			fm.calcstr.value=swm;
		}
		if (gs('wmr2')) { // wmu - wmr
			fm.calcstr.value=swm;
		}
		if (gs('wmu2')) { // wmu - wmu
			fm.calcstr.value=swm;
		}
		if (gs('yd2')) { // wmu - yd
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_wmu_in)/yd_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in)*yd_out);
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%/'+yd_out+'='+fm.a2.value+' ЯД';
			set_text(wmu_yd);
		}
		if (gs('pm2')) { // wmu - pm
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_wmu_in+com_pm_out)/usd_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in+com_pm_out)*usd_out);
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%-'+com_pm_out+'%/'+usd_out+'='+fm.a2.value+' PM';
			set_text(wmu_pm);
		}
		if (gs('eth2')) { // wmu - eth
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_wmu_in+com_btc_out)/usd_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in+com_btc_out)*usd_out);
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%-'+com_btc_out+'%/'+usd_out+'='+fm.a2.value+' ETH';
			set_text(wm_btc);
		}
		if (gs('btc2')) { // wmu - btc
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_wmu_in+com_btc_out)/usd_out*btc_rate); else
				fm.a1.value=r2(fm.a2.value/p100(com_wmu_in+com_btc_out)*usd_out/btc_rate);
			fm.calcstr.value=fm.a1.value+' WMU-'+com_wmu_in+'%-'+com_btc_out+'%/'+usd_out+'*'+btc_rate+'='+fm.a2.value+' BTC';
			set_text(wmu_btc);
		}
	}
	if (gs('yd1')) { // Расчеты по ЯД ЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖ
		if (gs('uah2')) { // yd - грн
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in); else
				fm.a1.value=r2(fm.a2.value/yd_in);
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'='+fm.a2.value+' грн';
			set_text(yd_uah);
		}
		if (gs('usd2')) { // yd - $
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in/usd_out); else
				fm.a1.value=r2(fm.a2.value/yd_in*usd_out);
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'/'+usd_out+'='+fm.a2.value+' $';
			set_text(yd_usd);
		}
		if (gs('eur2')) { // yd - €
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in/wme_out); else
				fm.a1.value=r2(fm.a2.value*wme_out/yd_in);
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'/'+wme_out+'='+fm.a2.value+' €';
			set_text(yd_eur);
		}
		if (gs('rur2')) { // yd - руб
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_yd_in)); else
				fm.a1.value=r2(fm.a2.value/p100(com_yd_in));
			fm.calcstr.value=fm.a1.value+' ЯД-'+com_yd_in+'%='+fm.a2.value+' руб';
			set_text(yd_rur);
		}
		if (gs('wmz2')) { // yd - wmz
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in/usd_out*p100(com_wmz_out)); else
				fm.a1.value=r2(fm.a2.value/yd_in*usd_out/p100(com_wmz_out));
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'/'+usd_out+'-'+com_wmz_out+'%='+fm.a2.value+' WMZ';
			set_text(yd_wmz);
		}
		if (gs('wme2')) { // yd - wme 
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in/wme_out*p100(com_wme_out)); else
				fm.a1.value=r2(fm.a2.value/yd_in*wme_out/p100(com_wme_out));
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'/'+wme_out+'-'+com_wme_out+'%='+fm.a2.value+' WME';
			set_text(yd_wme);
		}
		if (gs('wmr2')) { // yd - wmr
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_yd_in)); else
				fm.a1.value=r2(fm.a2.value/p100(com_yd_in));
			fm.calcstr.value=fm.a1.value+' ЯД-'+com_yd_in+'%='+fm.a2.value+' WMR';
			set_text(yd_wmr);
		}
		if (gs('wmu2')) { // yd - wmu
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in*p100(com_wmu_out)); else
				fm.a1.value=r2(fm.a2.value/yd_in/p100(com_wmu_out));
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'-'+com_wmu_out+'='+fm.a2.value+' грн';
			set_text(yd_wmu);
		}
		if (gs('yd2')) { // yd - yd
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(1.5)); else
				fm.a1.value=r2(fm.a2.value/p100(1.5));
			fm.calcstr.value=fm.a1.value+' ЯД-1.5%='+fm.a2.value+' ЯД';
			set_text(yd_yd);
		}
		if (gs('pm2')) { // yd - pm
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in/usd_out*p100(com_pm_out)); else
				fm.a1.value=r2(fm.a2.value/yd_in*usd_out/p100(com_pm_out));
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'/'+usd_out+'-'+com_pm_out+'%='+fm.a2.value+' PM';
			set_text(yd_pm);
		}
		if (gs('eth2')) { // yd - eth
			if (b)
				fm.a2.value=r2(fm.a1.value*yd_in/usd_out*p100(com_btc_out)); else
				fm.a1.value=r2(fm.a2.value/yd_in*usd_out/p100(com_btc_out));
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'/'+usd_out+'-'+com_btc_out+'%='+fm.a2.value+' eth';
		}
		if (gs('btc2')) { // yd - btc
			if (b)
				fm.a2.value=r6(fm.a1.value*yd_in/usd_out/btc_rate*p100(com_btc_out)); else
				fm.a1.value=r2(fm.a2.value/yd_in*usd_out*btc_rate/p100(com_btc_out));
			fm.calcstr.value=fm.a1.value+' ЯД*'+yd_in+'/'+usd_out+'/'+btc_rate+'-'+com_btc_out+'%='+fm.a2.value+' BTC';
			set_text(yd_btc);
		}
	}
	if (gs('pm1')) { // Расчеты по PM ЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖ
		if (gs('uah2')) { // pm - грн 
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*usd_in); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/usd_in);
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%*'+usd_in+'='+fm.a2.value+' грн';
			set_text(pm_uah);
		}
		if (gs('usd2')) { // pm - $
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in));
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%='+fm.a2.value+' $';
			set_text(pm_usd);
		}
		if (gs('eur2')) { // pm - €
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*usd_in/wme_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/usd_in*wme_out);
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%*'+usd_in+'/'+wme_out+'='+fm.a2.value+' €';
			set_text(pm_eur);
		}
		if (gs('rur2')) { // pm - руб
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*usd_in/wmr_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/usd_in*wmr_out);
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%*'+usd_in+'/'+wmr_out+'='+fm.a2.value+' руб';
			set_text(pm_rur);
		}
		if (gs('wmz2')) { // pm - wmz
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*p100(com_wmz_out)); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/p100(com_wmz_out));
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%-'+com_wmz_out+'%='+fm.a2.value+' WMZ';
			set_text(pm_wmz);
		}
		if (gs('wme2')) { // pm - wme
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*usd_out/wme_out/p100(com_wme_out)); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/usd_out*wme_out/p100(com_wme_out));
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%*'+usd_out+'/'+wme_out+'-'+com_wme_out+'%='+fm.a2.value+' WME';
			set_text(pm_wme);
		}
		if (gs('wmr2')) { // pm - wmr
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*usd_in/wmr_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/usd_in*wmr_out);
			fm.calcstr.value=fm.a1.value+' PM*'+usd_in+'/'+wmr_out+'-'+com_pm_in+'%='+fm.a2.value+' WMR';
			set_text(pm_wmr);
		}
		if (gs('wmu2')) { // pm - wmu
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*usd_in*p100(com_wmu_out)); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/usd_in/p100(com_wmu_out));
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%*'+usd_in+'-'+com_wmu_out+'='+fm.a2.value+' WMU';
			set_text(pm_wmu);
		}
		if (gs('yd2')) { // pm - yd
			if (b)
				fm.a2.value=r2(fm.a1.value*usd_in/yd_out*p100(com_pm_in)); else
				fm.a1.value=r2(fm.a2.value/usd_in*yd_out/p100(com_pm_in));
			fm.calcstr.value=fm.a1.value+' PM*'+usd_in+'/'+yd_out+'-'+com_pm_in+'%='+fm.a2.value+' ЯД';
			set_text(pm_yd);
		}
		if (gs('pm2')) { // pm - pm
			if (b)
				fm.a1.value=r2(fm.a2.value*0.99); else
				fm.a2.value=r2(fm.a1.value/0.99);
			fm.calcstr.value=fm.a1.value+' PM-1%='+fm.a2.value+' PM';
			set_text(pm_pm);
		}
		if (gs('eth2')) { // pm - eth
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_pm_in)*p100(com_btc_out)); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/p100(com_btc_out));
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%-'+com_btc_out+'%='+fm.a2.value+' ETH';
		}
		if (gs('btc2')) { // pm - btc
			if (b)
				fm.a2.value=r6(fm.a1.value*p100(com_pm_in)*p100(com_btc_out)/btc_rate); else
				fm.a1.value=r2(fm.a2.value/p100(com_pm_in)/p100(com_btc_out)*btc_rate);
			fm.calcstr.value=fm.a1.value+' PM-'+com_pm_in+'%-'+com_btc_out+'%/'+btc_rate+'='+fm.a2.value+' BTC';
			set_text(pm_btc);
		}
	}
	if (gs('eth1')) { // Расчеты по ETH ЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖ
		if (gs('uah2')) { // eth - грн
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*usd_in); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)/usd_in);
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%*'+usd_in+'='+fm.a2.value+' грн';
			set_text(eth_bank);
		}
		if (gs('usd2')) { // eth - $
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in));
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%='+fm.a2.value+' $';
			set_text(eth_cash);
		}
		if (gs('eur2')) { // eth - €
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*usd_in/wme_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)/usd_in*wme_out);
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%*'+usd_in+'/'+wme_out+'='+fm.a2.value+' €';
			set_text(eth_cash);
		}
		if (gs('rur2')) { // ETH - руб
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*usd_in/wmr_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)/usd_in*wmr_out);
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%*'+usd_in+'/'+wmr_out+'='+fm.a2.value+' руб';
			set_text(eth_bank);
		}
		if (gs('wmz2')) { // eth - wmz
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_wmz)); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_wmz));
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_wmz+'%='+fm.a2.value+' WMZ';
			set_text(eth_wm);
		}
		if (gs('wme2')) { // eth - wme
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*p100(com_wmz_out)*usd_in/wme_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)*p100(com_wmz_out)*usd_in/wme_out);
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%-'+com_wmz_out+'%*'+usd_in+'/'+wme_out+'='+fm.a2.value+' WME';
			set_text(eth_wm);
		}
		if (gs('wmr2')) { // eth - wmr
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*usd_in/wmr_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)/usd_in*wmr_out);
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%*'+usd_in+'/'+wmr_out+'='+fm.a2.value+' WMR';
			set_text(eth_wm);
		}
		if (gs('wmu2')) { // eth - wmu
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*usd_in*p100(com_wmu_out)); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)/usd_in/p100(com_wmu_out));
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%*'+usd_in+'-'+com_wmu_out+'='+fm.a2.value+' WMU';
			set_text(eth_wm);
		}
		if (gs('yd2')) { // eth - yd
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*usd_in/yd_out); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)/usd_in*yd_out);
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%*'+usd_in+'/'+yd_out+'='+fm.a2.value+' ЯД';
		}
		if (gs('pm2')) { // eth - pm
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*p100(com_pm_out)); else
				fm.a1.value=r2(fm.a2.value/p100(com_btc_in)/p100(com_pm_out));
			fm.calcstr.value=fm.a1.value+' ETH-'+com_btc_in+'%-'+com_pm_out+'%='+fm.a1.value+' PM';
			set_text(eth_pm);
		}
		if (gs('eth2')) { // eth - eth
			if (b)
				fm.a2.value=r2(fm.a1.value*0.99); else
				fm.a1.value=r2(fm.a2.value/0.99);
			fm.calcstr.value=fm.a1.value+' ETH-1%='+fm.a2.value+' ETH';
		}
		if (gs('btc2')) { // eth - btc
			if (b)
				fm.a2.value=r6(fm.a1.value*0.99/btc_rate); else
				fm.a1.value=r2(fm.a2.value/0.99*btc_rate);
			fm.calcstr.value=fm.a1.value+' ETH-1%*'+btc_rate+'='+fm.a2.value+' BTC';
		}
	}
	if (gs('btc1')) { // Расчеты по BTC ЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖ
		if (gs('uah2')) { // btc - грн
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate*usd_in); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate/usd_in);
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'*'+usd_in+'='+fm.a2.value+' грн';
			set_text(btc_bank);
		}
		if (gs('usd2')) { // btc - $
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate);
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'='+fm.a2.value+' $';
			set_text(btc_cash);
		}
		if (gs('eur2')) { // btc - €
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate*usd_in/wme_out); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate/usd_in*wme_out);
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'*'+usd_in+'/'+wme_out+'='+fm.a2.value+' €';
			set_text(btc_cash);
		}
		if (gs('rur2')) { // btc - руб
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate*usd_in/wmr_out); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate/usd_in*wmr_out);
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'*'+usd_in+'/'+wmr_out+'='+fm.a2.value+' руб';
			set_text(btc_rur);
		}
		if (gs('wmz2')) { // btc - wmz
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_wmz)*btc_rate); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_wmz)/btc_rate);
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_wmz+'%*'+btc_rate+'='+fm.a2.value+' WMZ';
			set_text(btc_wmz);
		}
		if (gs('wme2')) { // btc - wme
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate*usd_out/wme_out*p100(com_wme_out)); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate/usd_out*wme_out/p100(com_wme_out));
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'*'+usd_out+'/'+wme_out+'-'+com_wme_out+'%='+fm.a2.value+' WME';
			set_text(btc_wme);
		}
		if (gs('wmr2')) { // btc - wmr
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate*usd_in/wmr_out); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate/usd_in*wmr_out);
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'*'+usd_in+'/'+wmr_out+'='+fm.a2.value+' WMR';
			set_text(btc_wmr);
		}
		if (gs('wmu2')) { // btc - wmu
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate*usd_out*p100(com_wmu_out)); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate/usd_out/p100(com_wmu_out));
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'*'+usd_out+'-'+com_wmu_out+'%='+fm.a2.value+' WMU';
			set_text(btc_wmu);
		}
		if (gs('yd2')) { // btc - yd
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate*usd_out/yd_out); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate/usd_out*yd_out);
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'*'+usd_out+'/'+yd_out+'='+fm.a2.value+' ЯД';
			set_text(btc_yd);
		}
		if (gs('pm2')) { // btc - pm
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(com_btc_in)*btc_rate/p100(com_pm_out)); else
				fm.a1.value=r6(fm.a2.value/p100(com_btc_in)/btc_rate*p100(com_pm_out));
			fm.calcstr.value=fm.a1.value+' BTC-'+com_btc_in+'%*'+btc_rate+'-'+com_pm_out+'%='+fm.a2.value+' PM';
			set_text(btc_pm);
		}
		if (gs('eth2')) { // btc - eth
			if (b)
				fm.a2.value=r2(fm.a1.value*p100(1)*btc_rate); else
				fm.a1.value=r6(fm.a2.value/p100(1)/btc_rate);
			fm.calcstr.value=fm.a1.value+' BTC-1%*'+btc_rate+'='+fm.a2.value+' ETH';
		}
		if (gs('btc2')) { // btc - btc
			if (b)
				fm.a2.value=r6(fm.a1.value*p100(1)); else
				fm.a1.value=r6(fm.a2.value/p100(1));
			fm.calcstr.value=fm.a1.value+' BTC-1%='+fm.a2.value+' BTC';
			set_text(btc_btc);
		}
	}
	
	if (gs('wmz1') || gs('wme1') || gs('wmu1') || gs('wmr1') || gs('yd1') || gs('pm1')) {
		// if (b<2) {
			fm.a3.style.visibility='visible';
			fm.a3.value=r2((gs('yd1') || gs('pm1')) ? fm.a1.value*1.005 : fm.a1.value*1.008);
		} else 
			fm.a3.style.visibility='hidden';
		
	if (gs('btc1') && gs('uah2') || gs('btc1') && gs('rur2') || gs('yd1') || gs('btc1') && gs('yd2') || gs('btc1') && gs('pm2')) { // Форму для перевода отобразить только ЯД и BTC на Приват 
	 	document.getElementById('i9').style.display='block';
	 	fm.bt.disabled=false;
	} else {
		document.getElementById('i9').style.display='none'; // Спрятать блок
	 	fm.bt.disabled=true;
	}
	
	if (gs('yd1') && gs('yd2')) { // Скрыть формочку ЯД-ЯД
		document.getElementById('i9').style.display='none'
		} else {
			fm.a3.style.visibility='hidden';
	}
	
}  // calc

function calc2(){
	fm.a1.value=r2(gs('yd1') ? fm.a3.value/1.005 : fm.a3.value/1.008-0.01);
	calc(2);
}
</script>
