			fm.calcstr.value=fm.a1.value+' €*'+wme_in+'/'+usd_out+'-'+com_btc_out+'%='+fm.a2.value+' ETH';
			set_text(cash_btc);
		}
		if (gs('btc2')) { // eur - btc
			if (b)
				fm.a2.value=r6(fm.a1.value*wme_in/usd_out/btc_rate*p100(com_btc_out)); else
