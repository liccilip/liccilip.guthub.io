			set_text(cash_btc);
		}
	}
	if (gs('rur1')) { // Руб Сбер/РФ банк ЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖЖ
		set_text(rub_all);
