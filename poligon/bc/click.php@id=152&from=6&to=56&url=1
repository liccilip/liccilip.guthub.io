			set_text(eur_yd);
		}
		if (gs('pm2')) { // eur - pm
			if (b)
				fm.a2.value=r2(fm.a1.value*wme_in/usd_out*p100(com_pm_out)); else
				fm.a1.value=r2(fm.a2.value/wme_in*usd_out/p100(com_pm_out));
			fm.calcstr.value=fm.a1.value+' €*'+wme_in+'/'+usd_out+'-'+com_pm_out+'%='+fm.a2.value+' PM';
			set_text(eur_pm);
		}
		if (gs('eth2')) { // eur - eth
			if (b)
				fm.a2.value=r2(fm.a1.value*wme_in/usd_out*p100(com_btc_out)); else
