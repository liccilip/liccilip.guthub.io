                            } else bal = "&nbsp;";
                            document.getElementById(("table_ball"+i)).innerHTML = bal;
                        }
                    }
                    Prev = PosY;
                }

                function ShowVal(PosY) {
                    var FromDiv = document.getElementById("dir_gray");
                    FromDiv.style.display="block";
                    FromDiv.style.top = ((PosY*50)+39);
                }

                function ShowNeed(elemId)
                {
                    var need = document.getElementById("need_div");
                    if(need.style.display == "none") {
                        var elem = elemId;
                        var t = 0;
                        t += elem.offsetTop; elem = elem.offsetParent;
                        t += elem.offsetTop; elem = elem.offsetParent;
                        t += elem.offsetTop; elem = elem.offsetParent;

                        need.style.top=(t+60);
                        need.style.left=(400);
                        need.style.display="block";
                        need.style.zIndex=50;
                        var needv = document.getElementById("need_valute");
                        needv.selectedIndex = (t-72)/50;
                    }
                    else need.style.display="none";

                    return false;
                }
