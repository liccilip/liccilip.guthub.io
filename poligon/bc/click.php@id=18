                function ShowDirection(PosY) {
                    var bal="";
                    var cour = "";
                    document.getElementById("dir_white").style.top = ((PosY*50)+35);
                    document.getElementById("dir_gray").style.display="none";

                    if(Prev != PosY) {
                        for(i=0;i<7;i++) {
                            if(courses[PosY][i] > 0) cour = "<A href=\"#\" OnClick=\"return Exchange("+PosY+","+i+");\" title=\"Обмен "+valute[PosY]+" на "+valute[i]+"\">"+courses[PosY][i]+"<span class=\"valblock\" style=\"background-position:0 "+(-40*i)+"px;\">&nbsp;<\/span><span>&nbsp;"+valute[i]+"<\/span><\/a>";
                            else cour = "&nbsp;";
