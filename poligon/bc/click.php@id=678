    oFromCurrencies.find( 'li' ).bind( 'hover click', function() {
        oFromCurrencies.find( 'li' ).removeClass( 'active' );
        $( this ).addClass( 'active' );
        var sFromCurrency = $( this ).attr( 'data-currency' ), sHTML = '';
        for( var sToCurrency in aRates[sFromCurrency] )
        //alert(sToCurrency);
            if(sToCurrency.substr(sToCurrency.length - 3) == 'BTC' || sToCurrency.substr(sToCurrency.length - 3) == 'ETH' || sToCurrency.substr(sToCurrency.length - 3) == 'LTC') {
                sHTML += '<li onclick="window.location = \'?sFromCurrency='+sFromCurrency+'&sToCurrency='+sToCurrency+'\'"><table><tbody><tr><td width="80">'+(parseInt( 1 / aRates[sFromCurrency][sToCurrency] * 10000000 ) / 10000000)+' '+aCurrencies[sToCurrency].sEquivalent.replace( 'USD', '$' ).replace( 'RUB', 'р' ).replace( 'EUR', '&euro;' ).replace( 'UAH', 'грн.' )+'</td><td width="30"><img src="public/resources/icons/currencies/'+sToCurrency+'.png" height="25" alt="" /></td><td>'+aCurrencies[sToCurrency].sBillingTitle+' '+aCurrencies[sToCurrency].sEquivalent+'</td><td class="reserve" style="text-align: right">'+aCurrencies[sToCurrency].dReserve.toFixed(6)+'</td><td align="right" width="50"><button style="padding: 0; font-size: 10px">обменять</button></td></tr></tbody></table></li>';
            } else {
                sHTML += '<li onclick="window.location = \'?sFromCurrency='+sFromCurrency+'&sToCurrency='+sToCurrency+'\'"><table><tbody><tr><td width="80">'+(parseInt( 1 / aRates[sFromCurrency][sToCurrency] * 100 ) / 100)+' '+aCurrencies[sToCurrency].sEquivalent.replace( 'USD', '$' ).replace( 'RUB', 'р' ).replace( 'EUR', '&euro;' ).replace( 'UAH', 'грн.' )+'</td><td width="30"><img src="public/resources/icons/currencies/'+sToCurrency+'.png" height="25" alt="" /></td><td>'+aCurrencies[sToCurrency].sBillingTitle+' '+aCurrencies[sToCurrency].sEquivalent+'</td><td class="reserve" style="text-align: right">'+aCurrencies[sToCurrency].dReserve+'</td><td align="right" width="50"><button style="padding: 0; font-size: 10px">обменять</button></td></tr></tbody></table></li>';
            }
        oToCurrencies.find( 'ul' ).html( sHTML );
    } );
    
    oFromCurrencies.find( 'li:first-child' ).click();
} );
</script>

<marquee height="300" onmouseout="this.start()" onmouseover="this.stop()" scrolldelay="10" scrollamount="1" direction="up">
    <h3>Последние операции</h3>
    <p style="text-align: center; margin-top: 15px;">Последний <b class="sLastExchangeDate StopWatch">00:00:00</b> назад<br /><b class="sLastExchange"></b></p>

            <h3>Новости</h3>
  <p>
  <span style="float: left; font-weight: bolder"> EXMO вводит комиссию на создание EX-CODE в фиатных валютах.</span>
  <span style="float: right; font-size: 11px; color: darkgreen;">14.02.2019</span><br/>
  <div style="clear: both">C 1 марта 2019 года криптовалютная платформа EXMO вводит комиссию на создание EX-CODE в фиатных валютах: USD, EUR, PLN, TRY, UAH, RUB. <br/> Комиссия на генерацию EX-CODE в фиатных валютах составит:<br/>
- 0,2% - до 2 операций в сутки;<br/>
- 0,6% - для 3-х и более операций в сутки.<br/>Остальные операции с EX-CODE остаются без комиссий.<br>
<a href="https://exmo.me/ru/news_view?id=2580&ref=401971">Ссылка на новость</a><br/> К сожалению, для компенсации потерь наш обменник вынужден будет заложить аналогичную комиссию на выплату в фиатных EX-code.
Для избежания комиссионных потерь нашими клиентами, мы расширяем перечень криптовалют, в которых возможны обмены с использованием EX-code. Уже доступен BTC, с сегодняшнего дня будет достпен эфир ETH, в ближайшее время планируем сделать доступным LTC.<br/>
Благодарим за понимание, и спасибо, что Вы с нами!
</div>
 </p>
 <br />
  <p>
  <span style="float: left; font-weight: bolder">Начинаем работать с Ethereum (эфирами)</span>
  <span style="float: right; font-size: 11px; color: darkgreen;">17.12.2017</span><br/>
  <div style="clear: both">Подключена популярная криптовалюта <b>Ethereum</b> (эфиры), занимающая 2-ое место по капитализации после Bitcoin.</div>
 </p>
 <br />
  <p>
  <span style="float: left; font-weight: bolder">Подключена платёжная система EPAY</span>
  <span style="float: right; font-size: 11px; color: darkgreen;">26.09.2017</span><br/>
  <div style="clear: both">Подключена платёжная система <b>EPAY</b>.<br/>
Обменный пункт YM-CHANGE включён в список авторизованных обменных пунктов <b>EPAY</b>.<br/>
Операции обмена проходят в автоматическом режиме.<br/> Комиссия за переводы средств внутри платёжной системы - 0%.</div>
 </p>
