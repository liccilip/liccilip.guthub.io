<script type="text/javascript">jQuery(document).ready(function () {
    $('.not-enough a').click(function (event) {
        $.get(
            event.target.href,
            function (data) {
                var notEnoughForm = $('#not-enough-form');
                notEnoughForm.html(data);
                notEnoughForm.dialog({
                    modal: true,
                    dialogClass: 'not-enough-modal',
                    width: 'auto',
                    draggable: false,
                    resizable: false,
                    autoOpen: false
                });
                notEnoughForm.dialog('open');
                notEnoughForm.first('input').blur();
            }
        );
        return false;
    });
