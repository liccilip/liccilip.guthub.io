     <script>

         $(document).ready(function () {

             show_hidden('exchange');
             show_pay("exch_100","exch1_27", this);
			 show_hiddenorder('zayorder');
			 $('#exch_100 >ul >li:first-child > a').addClass('active');
			 
         });

         function ButtonL() {

             if ($('#uemail').val().length == 0 || $('#upass').val().length == 0) {
                 $("#errLoginPass").show();
                 return;
             }

             $.ajax({
                 type: "POST",
                 url: "/ulogin/slogin/RU",
                 data: {
                     uemail: $('#uemail').val(), upass: $('#upass').val(),
                     tk: $('#gl-csrf-token').attr('name'), tv: $('#gl-csrf-token').val()
                 },
                 dataType: "json",
                 success: function (data, textStatus) {

                     if (data.retdat == null) {

                         $("#errLoginPass").show();

                         $('#gl-csrf-token')
                                 .attr('name', data.csrfname)
                                 .val(data.csrfval);
                     }
                     else {

                         $("#insucf").html(data.retdat);

                         $('#gl-csrf-token')
                                 .attr('name', data.csrfname)
                                 .val(data.csrfval);
                     }
                 },
                 error: function () {
                 }
             });
         }

         function ButtonUL() {

             $.ajax({
                 type: "POST",
                 url: "/ulogin/unlogin/RU",
                 data: {},
                 dataType: "html",
                 success: function (results) {
                     window.location.href = '/RU';
                 },
                 error: function () {
                 }
             });

         }

         function ButtonPUL() {

             $.ajax({
                 type: "POST",
                 url: "/partners/punlogin/RU",
                 data: {},
                 dataType: "html",
                 success: function (results) {
                     window.location.href = '/partners/RU';
                 },
                 error: function () {
                 }
             });

         }

     </script>

