</div>
<div class="block-inner form-inline">
<input type="text" class="form-control" style="width:60%;" name="extrans" id="extrans" placeholder="Введите ID транзакции" onKeyUp="value=value.replace(/\D+/g,'')"/>
<input type="button" class="btn btn-primary" onclick="get_trans();" value="Проверить"/>
<span id="nottrans" class="help-block margin-top-sm f_red"></span>
<script type="text/javascript">
function get_trans() {
if(Vu('extrans').value == '') {
alert('Введите ID транзакции');
Vu('extrans').focus();
return false;
}
if(Vu('extrans').value.length < 1) return;
$.ajax({
url: AJPath+'?moduleid=25&action=exchange&value='+Vu('extrans').value,
type: 'POST',
data: {'crypt':'faf51084af0ac718e93e71c093b56a7c3b4f570551a806f40ce34712fff3ebab'},
dataType: 'text'
})
.done(function(data) {
if(data != 1) $('#nottrans').html(data);
else $('#nottrans').html("Такой транзакции не существует");
});
}
</script>
