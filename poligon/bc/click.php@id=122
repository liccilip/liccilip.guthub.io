            1.0979;
                                                aRates['WMU']['PRIVAT24UAH'] = 1.89 /
            1;
                                                aRates['P24']['WMZ'] = 29.986 /
            1;
                                                aRates['P24']['WMR'] = 1 /
            2.25;
                                                aRates['P24']['YAM'] = 1 /
            2.27;
                                                aRates['P24']['BITCOIN'] = 121981.0956 /
            1;
                                                aRates['P24']['LITECOIN'] = 1 /
            1;
                                                aRates['P24']['ETHEREUM'] = 10091.7739 /
            1;
                                                aRates['BITCOIN']['PRIVAT24UAH'] = 1 /
            65375.4595;
                                                aRates['LITECOIN']['PRIVAT24UAH'] = 1 /
            900.9949;
                                                aRates['ETHEREUM']['PRIVAT24UAH'] = 1 /
            2001.351;
                                                                        
                                    if (ArrayLength(aRates['WMZ']) == 0)
                oFromCurrencies.find('li[data-currency="WMZ"]').hide();
                                                if (ArrayLength(aRates['WMR']) == 0)
                oFromCurrencies.find('li[data-currency="WMR"]').hide();
                                                if (ArrayLength(aRates['WMU']) == 0)
                oFromCurrencies.find('li[data-currency="WMU"]').hide();
                                                if (ArrayLength(aRates['YAM']) == 0)
                oFromCurrencies.find('li[data-currency="YAM"]').hide();
                                                if (ArrayLength(aRates['PRIVAT24UAH']) == 0)
                oFromCurrencies.find('li[data-currency="PRIVAT24UAH"]').hide();
                                                if (ArrayLength(aRates['P24']) == 0)
                oFromCurrencies.find('li[data-currency="P24"]').hide();
                                                if (ArrayLength(aRates['BITCOIN']) == 0)
                oFromCurrencies.find('li[data-currency="BITCOIN"]').hide();
                                                if (ArrayLength(aRates['ETHEREUM']) == 0)
                oFromCurrencies.find('li[data-currency="ETHEREUM"]').hide();
                                                if (ArrayLength(aRates['LITECOIN']) == 0)
                oFromCurrencies.find('li[data-currency="LITECOIN"]').hide();
                                                
            oFromCurrencies.find('li').bind('click', function () {
                oFromCurrencies.find('li').removeClass('active');
                $(this).addClass('active');
                var sFromCurrency = $(this).attr('data-currency'), sHTML = '';
                for (var sToCurrency in aRates[sFromCurrency])
                    sHTML += '<li onclick="window.location = \'?sFromCurrency=' + sFromCurrency + '&sToCurrency=' + sToCurrency + '\'"><table><tbody><tr><td width="30"><img src="public/resources/icons/currencies/' + sToCurrency + '.png" height="25" alt="" /></td><td>' + aCurrencies[sToCurrency].sTitle + '</td><td align="right" width="50"><button style="padding: 0; font-size: 10px">обменять</button></td></tr></tbody></table></li>';
                oToCurrencies.find('ul').html(sHTML);
            });

            //oFromCurrencies.find('li:first-child').click();
        });
