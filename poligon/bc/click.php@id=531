		<script>
			function getCookie(name) {
				var matches = document.cookie.match(new RegExp(
					"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
				));
				return matches ? decodeURIComponent(matches[1]) : undefined;
			}

			var notifies = [
							];

			function checkNotify(){
				while( notifies.length > 0 ) {
					var notify = notifies.shift();
					if( !getCookie('warningModal-' + notify.id)) {
						$('#warningModal h4').html(notify.title);
						$('#warningModal .modal-body').html(notify.description);
						$('#warningModal').modal();
						$('#warningModal').on('hidden.bs.modal', function (e) {
							document.cookie = "warningModal-" + notify.id + "=closed";
							checkNotify();
						});
						return
					}
				}
			}

			$(document).ready(function(){
				checkNotify();
			});
			setTimeout(function(){
				document.location.reload();;
			},600000)
		</script>
