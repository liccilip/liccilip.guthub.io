<script>
	$(function () {
		$.fn.xmlLoading = function(action) {
			if (action === 'show') {
				var $loading = $('<div></div>').addClass('btn-loading');
				$loading.width(this.outerWidth()).height(this.outerHeight()).html('<div></div><div></div><div></div>');
				this.hide();
				$loading.appendTo(this.parent().get(0));
			} else if (action === 'hide') {
				$('.btn-loading').remove();
				$(this).show();
			} else {
				console.error('Incorrect action, use: show or hide');
			}
			return this;
		};
	});
</script>
