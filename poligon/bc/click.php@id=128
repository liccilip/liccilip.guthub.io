		$.get('/ru/site/loadReserves/', function (data) {
			$('#reserve-body').html(data);
			var $items = $('.reserve-item');
			$items.click(function () {
				var code = $(this).data('code');
				RateInfo.toggle(code);
			});
			if (!RateInfo.isTouchDevice()) {
				$items.mouseover(function () {
					var code = $(this).data('code');
					RateInfo.show(code);
				});
				$items.mouseout(function () {
					RateInfo.hide();
				});
			}

		});

		$('.idx-cc-item, .idx-cc-top-item').mouseenter(function () {
			var $t = $(this);
			var style = $t.attr('style');
			if (style.indexOf('gradient') < 0) {
				style = style.replace(';', '');
				style += ', linear-gradient(to bottom, #fff, #f1f3f2) repeat-x top left;';
				$t.attr('style', style);
			}
		}).mouseleave(function () {
			var $t = $(this);
			var style = $t.attr('style');
			var arr = style.split(',', 2);
			$t.attr('style', arr[0]);
		});
		xmlExRun.loadData();
	});
		// GTM from google
	var gtmTimer;
	function gtmPush(name) {
		if (gtmTimer) {
			window.clearTimeout(gtmTimer);
		}
		gtmTimer = window.setTimeout(function () {
			dataLayer.push({
				'name': name,
				'event': 'checkcolumn'
			});
		}, 2000);
	}
		// Reserve / Rates table actions
	var RateInfo = {
		rdShow: false,
		$rd: null,
		init: function () {
			RateInfo.$rd = $('.idx-reserve-data');
			RateInfo.$rd.mouseover(function () {
				RateInfo.rdShow = true;
			}).mouseout(function () {
				RateInfo.rdShow = false;
				RateInfo.idleHide();
			});
		},
		show: function (k) {
			$('.reserve-tbl-hover').removeClass('reserve-tbl-hover');
			RateInfo.rdShow = true;
			var $k_k = $('#k_' + k);
			var $tr_k = $('#tr' + k);
			RateInfo.$rd.html($k_k.html()).removeClass('hide');
						gtmPush($tr_k.data('name'));
						$tr_k.addClass('reserve-tbl-hover');
		},
		idleHide: function () {
			window.setTimeout(function () {
				if (!RateInfo.rdShow) {
					RateInfo.$rd.addClass('hide');
					$('.reserve-tbl-hover').removeClass('reserve-tbl-hover');
				}
			}, 300);
		},
		hide: function () {
			RateInfo.rdShow = false;
						if (gtmTimer) {
				window.clearTimeout(gtmTimer);
			}
						RateInfo.idleHide();
		},
		toggle: function (k) {
			var $tr_k = $('#tr' + k);
			if (!$tr_k.hasClass('reserve-tbl-hover')) {
				RateInfo.show(k);
			} else {
				RateInfo.hide();
			}
		},
		isTouchDevice: function () {
			var prefixes = ' -webkit- -moz- -o- -ms- '.split(' ');
			var mq = function(query) {
				return window.matchMedia(query).matches;
			};
			if (('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) {
				return true;
			}
			var query = ['(', prefixes.join('touch-enabled),('), 'heartz', ')'].join('');
			return mq(query);
		}
	};
	RateInfo.init();
</script>		</div>
