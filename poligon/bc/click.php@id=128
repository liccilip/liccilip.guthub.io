	<script src="/assets/97750c4f/js/js.cookie.js"></script>
	<script>
		function cancelSubscr() {
			$.cookie.set('subscr', '1',  { expires: 30 });
			$('.subscr-email-offer').remove();
		}
		function submitSubscr() {
			$('#subscrBtn').attr('disabled', true).css('background', '#ccc');
			$.cookie.set('subscr', '1',  { expires: 30 });
			var $form = $('#subscrForm');
			var $error = $('.subscrError');
			$.post($form.attr('action'), $form.serialize(), function (data) {
				$error.html('');
				if (data['success'] !== undefined) {
					$('.subscr-email-content').html('<div class="subscr-success">' + data['success'] + '</div>');
					$('.subscr-email-offer').fadeOut(3000, function(){ $(this).remove();});
				} else if (data['error'] !== undefined) {
					$error.html(data['error']);
				}
			});
			return false;
		}
		$(function () {
			var loaded_t = new Date().getTime();
			var leave_done = false;
			$('body').mouseleave(function () {
				if (!leave_done) {
					var now = new Date().getTime();
					var diff = (now - loaded_t) / 1000;
					var cookie_is = $.cookie.get('subscr');
					if (cookie_is !== undefined) {
						leave_done = true;
					} else if (diff >= 10) {
						var $w = $('<div></div>').addClass('subscr-email-offer');
						var $block = $('<div></div>').addClass('subscr-email-content');
						var msgs = {
							t1 :  'Вы хотите пользоваться привилегиями наших подписчиков: получать скидки и бонусы за совершение операций, а также получать новости о последних трендах на финансовых рынках?',
