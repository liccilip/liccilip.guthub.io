				html += '<div class="just-event-row bold">' +
					'<span>'+data[i]['from']+' </span>' +
					'<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 21.92 24.63"><defs><style>.just-e{fill:#efa933;}</style></defs><g><g><path class="just-e" d="M0,12.63c.13-.5.24-1,.38-1.5,1.29-4.57,4.56-6.85,9-7.73.94-.18,1.9-.25,2.92-.38V0l6.58,5.41L12.35,10.8v-3A14.76,14.76,0,0,0,4.94,9.54,10,10,0,0,0,.45,15.62L0,14.51Q0,13.57,0,12.63Z"/><path class="just-e" d="M21.92,12c-.13.5-.25,1-.39,1.5-1.18,4.23-4.13,6.56-8.25,7.57-1.18.29-2.41.38-3.68.58v3L3,19.22l6.55-5.39v3A14.84,14.84,0,0,0,17,15.08,10,10,0,0,0,21.47,9l.45,1.11Z"/></g></g></svg>' +
					' <span>'+data[i]['to']+'</span>' +
					'</div>';
			}
			$obj.html(html);
			return $obj;
		},
		loadData: function () {
			self = this;
			$.get('/ru/site/getJustEvent/?t=' + this.t, function (data) {
				self.t = data.t;
				if (data.d.length) {
					var $obj = self.getPopup(data.d);
					$obj.appendTo('body');
					$obj.attr('style', 'margin-bottom: -'+$obj.height()+'px; display: block;');
					$obj.animate({marginBottom: 0}, 400, function () {
						var ani_this = this;
						window.setTimeout(function () {
							$(ani_this).fadeOut(1000, function () {
								$(this).remove();
							})
						}, 3000);
					});
				}
			});
		}
	};
	var justEv = Object.create(JustEvent);
	justEv.init();
</script><!--Start of Tawk.to Script-->
