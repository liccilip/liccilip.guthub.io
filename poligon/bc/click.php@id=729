$(document).ready(function () {
                $('#btnAuthTab').click(function () {
                    $('.tabMainAuth').removeClass('activeAuth');
                    $(this).addClass('activeAuth');
                    $('#blockAuth').show();
                    $('#blockReg').hide();
                });
                $('#btnRegTab').click(function () {
                    $('.tabMainAuth').removeClass('activeAuth');
                    $(this).addClass('activeAuth');
                    $('#blockAuth').hide();
                    $('#blockReg').show();
                });
            });


