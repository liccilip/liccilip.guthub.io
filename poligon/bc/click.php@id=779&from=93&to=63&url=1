             (function ($) {
                $.fn.restrict = function () {
                    return this.each(function(){
                        $(this).on('change', function(){
                            var _self = this,
                                v = parseFloat(_self.value),
                                min = parseFloat(_self.min),
                                max = parseFloat(_self.max);
                            if (v >= min && v <= max){
                                _self.value = v;
                            }
                            else {
                                _self.value = v < min ? min : max;
                            }
                        });
