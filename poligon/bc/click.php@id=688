<script>
    $(document).ready(function () {
        
        var upMode = 0; // 1 - сайт выключен, 0 - включен;

        // Проверка телефона:
        var id = "#obmen_tel";
        var alertMessage = "Ошибка!\nВведите действующий номер телефона в формате +71234567890\nБез точек, скобок, тире и пробелов.";
        $(id).val("");
        $(id).change(function(){
            checkTel(id, upMode);
        });
        function checkTel(id, upMode){
            var telMatch = "";
            var res = false;
            var tel = $(id).val();
            if(tel && upMode === 0) {
                telMatch = tel.match(/[\+]\d+/g);
                if(telMatch) {
                    res = telMatch[0].length >= 10 && telMatch[0].length < 17 && telMatch[0].length === tel.length;
                }
            }
            if(upMode === 1){
                alertMessage = "Приём заявок временно приостановлен,\nоператор отсутствует на месте.";
                res = false;
            }
            if(!res){
                $(id).val("");
                alert(alertMessage);
                return false;
            }else{
                //console.log("Телефон введён верно: " + tel);
                return true;
            }
        }

        // Проверка ФИО:
        $("#obmen_f").change(function(){
            checkFIO("#obmen_f", "Ошибка!\nСлишком короткая фамилия.");
        });
        $("#obmen_i").change(function(){
            checkFIO("#obmen_i", "Ошибка!\nСлишком короткое имя.");
        });
        $("#obmen_o").change(function(){
            checkFIO("#obmen_o", "Ошибка!\nСлишком короткое отчество.");
        });
        function checkFIO(id, error){
            var res = false;
            var fio = $(id).val();
            if(fio) {
                res = fio.length >= 3;
            }
            if(!res){
                $(id).val("");
                alert(error);
                return false;
            }else{
                return true;
            }
        }

        // Сообщение вместо "техобслуживание":
        //$(".resultfalse").html("<img src = 'https://obmen.money/wp-content/uploads/icon-warning.png' class='h20px'> Оператор отсутствует на рабочем месте. <div>Мы заботимся о качестве предоставления наших услуг, поэтому отключаем сервис, когда не можем оперативно обработать вашу заявку. В любом случае, вы можете всегда с нами связаться. Для этого используйте телеграм <a href='https://t.me/helpobmen' target='_blank'>@helpobmen</a> и <a class='cp' onclick='jivo_api.open()'>чат</a> на сайте.</div>")
        // Добавляем div для вывода ошибок, если он еще не был добавлен:
        if($(".alertDiv").length === 0){$(".contentzone").prepend("<div class='alertDiv hidden'></div>")}
        // Меняем статус оператора в виджете:
        //if($(".resultfalse").html()){ // если статус off;
        if(upMode === 1){ // если статус off;
            $(".alertDiv").html("<img src = 'https://obmen.money/wp-content/uploads/icon-warning.png'> Оператор отсутствует на рабочем месте. <div>Мы заботимся о качестве предоставления наших услуг, поэтому отключаем сервис, когда не можем оперативно обработать вашу заявку. В любом случае, вы можете всегда с нами связаться. Для этого используйте телеграм <a href='https://t.me/helpobmen' target='_blank'>@helpobmen</a> или <a class='cp' onclick='jivo_api.open()'>чат</a> на сайте.</div>")
            $(".alertDiv").removeClass("hidden");
            $(".sidebar .statuswork_widget").removeClass("online").addClass("offline");
            $(".sidebar .statuswork_widget .opstatus").html("offline");
        } else {
            $(".alertDiv").html("").addClass("hidden");
            $(".sidebar .statuswork_widget").removeClass("offline").addClass("online");
            $(".sidebar .statuswork_widget .opstatus").html("online");
        }
    });

    $(".xchangestep1divvn h1").append("<img src='https://exchange.express/wp-content/uploads/obmen-money-banner-wmr.png' class='mt20'>");
</script>


