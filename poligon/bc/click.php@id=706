<script>
    console.log('--------fileupload start-----------');

    var idPsIn = $('.wrap-form-card-verif .from-js-id-ps-in');
    var form_cardverif = $('.wrap-form-card-verif');
    // var form_cardverif_submit = $('#form-card-verif [type="submit"]');
    var card_added_no_check = $('.card-added-no-check');
    var csrfToken = $('meta[name="csrf-token"]').attr("content");

    /*
    var fieldBlock = {
        nameIn: $('[data-block-exchange="name-in"]'),
        numberIn: $('[data-block-exchange="number-in"]'),
        email: $('[data-form-exchange="email"]'),

    };
    var fieldInput = {
        nameIn: fieldBlock.nameIn.find('input'),
        numberIn: fieldBlock.numberIn.find('input'),
        email: fieldBlock.email.find('input'),

    };
    console.log(idPsIn);
    console.log(fieldInput.numberIn.val());
    console.log(fieldInput.nameIn.val());
    console.log(fieldInput.email.val());
    */
    console.log(csrfToken);

    $('#form-card-verif #filename').text('');

    $('#fileupload').fileupload({
        // formData: {
        //     // number_in: '1111999922228888'
        //     // ,fio_in: "Иванов Иван Иванович"
        //     // ,email: "d@mail.ru"
        //     // ,id_ps_in: 1,
        //     id_ps_in: idPsIn,
        //     number_in: fieldInput.numberIn.val(),
        //     fio_in: fieldInput.nameIn.val(),
        //     email: fieldInput.email.val(),
        //     _csrf: csrfToken
        // },
        // maxFileSize: 999000,
        dataType: 'json',
        add: function (e, data) {
            $('#form-card-verif .global-btn-md-yellow').remove();
            $('#form-card-verif .help-block').hide().text('');
            $.each(data.files, function (index, file) {
                // $('<p/>').text(file.name);
                // console.log(file);
                $('#form-card-verif #filename').text(file.name);
                // console.log(file.name);
            });
            console.log('---add---');
            data.context = $('<button/>').addClass('global-btn-md-yellow').text('Отправить фото')
            // data.context = $('<p/>').text('Загрузить фото')
            // data.context = $('<button/>').text('Загрузить фото')
            // data.context = $('#form-card-verif button')
            //     .appendTo(document.body)
            // .appendTo($("#8456form"))
                .appendTo($("#form-card-verif .tmp888"))
                .click(function () {
                    data.context = $('<p/>').text('Загрузка...').replaceAll($(this)); // error if del this stuff

                    // data.context = $('<p/>').text('Загрузка...'); // error if del this stuff
                    // data.context = $(this); // error if del this stuff
                    // data.context = $('<p/>').text('Загрузка...');
                    // data.context = $(this).parent().append('Загрузка...');
                    data.submit();
                });
        },
        done: function (e, data) {
            $('#form-card-verif #filename').text('');
            $('#form-card-verif .tmp888 p').remove();
            // form_cardverif.hide();
            // card_added_no_check.show();
            console.log('----------get response from server--------');
            // data.context.text('Фото успешно загружено!');
            // console.log(data);
            console.log(data.result);
            console.log("---end------\r\n");
            var res = data.result;

            // if (res.card_added) {
            //     console.log('Ваша карта находится на проверке!');
            //     form_cardverif.hide();
            //     card_added_no_check.show();
            // }

            if (res.card_upload) {
                form_cardverif.hide();
                card_added_no_check.show();
            }
            form_cardverif.find('form .help-block').hide().text('');
            if (res.error) {
                $('.global-btn-md-yellow').remove();
                form_cardverif.find('form .help-block').show().text(res.error);
            }

            /* debug
            var objarr = data.result.files;
            var parse_obj = '';

            if (typeof objarr === 'object' && objarr !== null) {

                for (var key in objarr) {
                    if (objarr.hasOwnProperty(key)) {
                        var parse_obj = parse_obj + "<br>" + key + " : " + objarr[key];
                        console.log(key + " : " + objarr[key]);

                    }
                }
            }

            $('.data').html(parse_obj);
            */
            // return false;
        },
        fail: function (e, data) {
            // console.log(data.errorThrown);
            // console.log(data.textStatus);
            // console.log(data.jqXHR);
            alert('Ваш браузер не поддерживает загрузку файлов либо размер вашего фото больше 4.8 Mb, пожалуйста' +
                'попробуйте использовать другой браузер!');
        }
    });
    console.log('--------fileupload end-----------');



</script>

